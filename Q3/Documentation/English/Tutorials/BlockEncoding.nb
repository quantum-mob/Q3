Cell[CellGroupData[{

Cell["Geometric View: Relation to the Grover Rotation", "Subsection",
 CellID->1339493437,ExpressionUUID->"2c996f7e-877b-4bd9-949d-98da0139f1fe"],

Cell["\<\
The GSLW form of block encoding also allows a similar geometric \
interpretation as the above. To see this, let us introduce the rotated basis \
vector\
\>", "TechNoteText",
 CellID->795124158,ExpressionUUID->"fb1e2dc8-9180-479b-a5ed-c95e9466c68c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      RowBox[{"x", ":", 
        RowBox[{"-", "\[Theta]"}]}]},
     "Ket"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", "\[Theta]", ")"}], 
      TemplateBox[{"0"},
       "Ket"]}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"cos", "(", 
          RowBox[{"\[Theta]", "/", "2"}], ")"}]},
        {
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"sin", "(", 
           RowBox[{"\[Theta]", "/", "2"}], ")"}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{Right}}, "Rows" -> {{Baseline}}}], 
      ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "ba5fc611-417b-4fe3-a05d-993bfa97a8bf"],
 " "
}], "TechNoteText",
 CellID->2008471872,ExpressionUUID->"33e2169c-db34-48b7-b587-b3e6fa8cee88"],

Cell[TextData[{
 "and the ",
 StyleBox["reflection",
  FontSlant->"Italic"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"x", ":", 
       RowBox[{"-", "\[Theta]"}]}]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "9ae1d90b-e4ef-453a-b9de-b821a7280e5a"],
 " (see also ",
 ButtonBox["Figure 2",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/tutorial/BlochEncoding#529413056"],
 " for a geometric interpretation)"
}], "TechNoteText",
 CellID->91305511,ExpressionUUID->"8723784c-ee61-47bb-bed5-bb7a03c4a053"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Ket"], 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Bra"]}], "-", "1"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}], "Z", "\[VeryThinSpace]", 
       RowBox[{
        SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}]}], "\[DotEqual]", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "sin", 
           "\[VeryThinSpace]", "\[Theta]"}]},
         {
          RowBox[{
          "\[ImaginaryI]", "\[VeryThinSpace]", "sin", "\[VeryThinSpace]", 
           "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "cos"}], "\[VeryThinSpace]", "\[Theta]"}]}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a9ec9c8a-364e-4edb-8c93-81594122caee"],
 "."
}], "TechNoteText",
 CellID->1268399419,ExpressionUUID->"b1099763-8f5e-4e9b-be68-4322ef83f2e4"],

Cell["Now, note that", "TechNoteText",
 CellID->2066355011,ExpressionUUID->"1fbd4ac5-e36e-4182-b60b-51af230b491e"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["G", "x"], "(", "\[Lambda]", ")"}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {"\[Lambda]", 
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}]},
        {
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}], "\[Lambda]"}
       }], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "x"], "(", "\[Lambda]", ")"}], "Z"}], 
     "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], 
       "Z"}], "=", 
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
       RowBox[{"2", "\[Theta]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"c0916c3f-1f27-4015-9bb7-d2dca6acf2cd"],
 "."
}], "TechNoteText",
 CellID->123130593,ExpressionUUID->"05b9d734-ad4e-4354-8f85-1d213db7ba90"],

Cell[TextData[{
 "Combined with the Pauli ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
