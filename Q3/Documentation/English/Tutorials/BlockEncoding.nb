Cell[TextData[{
 "Block encoding is a method to get a quantum computer accessible to the \
information in a matrix ",
 Cell[BoxData[
  FormBox["M", TraditionalForm]],ExpressionUUID->
  "a6c9432c-7837-4cb2-a9c9-698999abde60"],
 ". Typically, the resulting bigger embedding matrix is denoted by ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["U", "M"], TraditionalForm]],ExpressionUUID->
  "218e438e-fd52-4063-befa-f238cb93c813"],
 " and has the block form"
}], "TechNoteText",
 CellID->495219038,ExpressionUUID->"45b9db24-d2de-4edc-834e-a71bc74d96e8"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "M"], ":=", 
    RowBox[{"(", GridBox[{
       {"M", "*"},
       {"*", "*"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "2db66cf1-c9d6-404d-af45-97777a84dd36"],
 " ,"
}], "TechNoteText",
 CellID->914482391,ExpressionUUID->"4a7d2cf5-e3f4-4047-b618-9de4ef001b9b"],

