Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "M"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", "V"}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       UnderscriptBox["\[CapitalSigma]", "k"], 
       RowBox[{
        RowBox[{
         SubscriptBox["\[CapitalOmega]", "y"], "(", 
         SubscriptBox["\[Lambda]", "k"], ")"}], "\[CircleTimes]", 
        TemplateBox[{"k"},
         "Ket"]}], 
       TemplateBox[{"k"},
        "Bra"]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", 
       SuperscriptBox["W", "\[Dagger]"]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"4c3c184a-8502-48df-83fb-5ca6d4c9922e"],
 ","
}], "TechNoteText",
 CellID->1134072701,ExpressionUUID->"e37fdbc6-2042-459c-894f-5b87af8ca25e"],

