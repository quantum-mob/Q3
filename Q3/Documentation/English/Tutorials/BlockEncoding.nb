Cell[TextData[{
 "The reflection ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}], 
   TraditionalForm]],ExpressionUUID->"5a31e0b5-8287-45bb-b1fd-db1b66b13fa1"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[Theta]"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "c23fe63e-5bf1-4393-9acf-4fb449939a57"],
 " can be converted to the rotation around the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "f9f8af3f-c1f9-4522-8d57-455fa6624665"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Theta]"}], TraditionalForm]],ExpressionUUID->
  "38a7042d-2ae3-421f-8433-d432c09a4e85"],
 " by multiplying the Pauli ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "e2326214-e64e-4931-916f-ec837245e91d"],
 " operator. In other words, it is useful to define a Grover-like rotation \
around the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "e2f71c6e-e58f-4d30-ad4a-072ffd10cf8d"],
 "-axis by"
}], "TechNoteText",
 CellID->465522068,ExpressionUUID->"c3bb6394-4a88-4bc2-9815-497cdaee9d76"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", "y"], "(", "\[Lambda]", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}], "Z"}],
      "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], "Z", "\[VeryThinSpace]", 
       RowBox[{
        SubsuperscriptBox["R", "y", "\[Dagger]"], "(", "\[Theta]", ")"}], 
       "Z"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], 
        RowBox[{
         SubscriptBox["R", "y"], "(", "\[Theta]", ")"}]}], "=", 
       RowBox[{
        RowBox[{
         SubscriptBox["R", "y"], "(", 
         RowBox[{"2", "\[Theta]"}], ")"}], "\[DotEqual]", 
        RowBox[{"(", GridBox[{
           {
            RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
            RowBox[{
             RowBox[{"-", "sin"}], "\[VeryThinSpace]", "\[Theta]"}]},
           {
            RowBox[{"sin", "\[VeryThinSpace]", "\[Theta]"}], 
            RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}]}
          },
          GridBoxAlignment->{"Columns" -> {{Right}}, "Rows" -> {{Baseline}}}],
          ")"}]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "55e13c9d-a91f-43d1-96f1-5bacb8e1e613"],
 " ."
}], "TechNoteText",
 CellID->746470855,ExpressionUUID->"66119d51-cdcb-450d-b99f-ce8e76706cca"],

