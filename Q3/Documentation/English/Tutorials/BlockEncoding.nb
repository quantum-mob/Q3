 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"V", "0"},
      {"0", "I"}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "260194cc-3af7-4857-a376-c3b698750e77"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
