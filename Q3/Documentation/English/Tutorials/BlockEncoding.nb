Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"M", "\[RightTeeArrow]", 
     SubscriptBox["U", "M"]}], ":=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"V", "0"},
        {"0", "V"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {"\[CapitalLambda]", 
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[CapitalLambda]", "2"]}]]}]},
        {
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[CapitalLambda]", "2"]}]]}], "\[CapitalLambda]"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["W", "\[Dagger]"], "0"},
        {"0", 
         SuperscriptBox["W", "\[Dagger]"]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "c947fc1a-b70b-4ef0-9c25-9777bdd3bb62"],
 " ."
}], "TechNoteText",
 CellID->365608999,ExpressionUUID->"0a8eb2de-e60d-4dc0-90ec-2f9bf330b826"],

