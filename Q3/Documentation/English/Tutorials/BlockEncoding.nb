 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "a3d4634f-cc7c-4bf6-8bdd-f1d521b3052b"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Theta]"}], TraditionalForm]],ExpressionUUID->
  "8fb5d36c-3c75-4b43-8da0-6fc80b760c79"]
}], "TechNoteText",
 CellID->1429124490,ExpressionUUID->"3087a033-a7f3-4704-bd21-ffcc4ca555a9"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}], "Z"}], 
    "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], "Z", 
      "\[VeryThinSpace]", 
      RowBox[{
       SubsuperscriptBox["R", "y", "\[Dagger]"], "(", "\[Theta]", ")"}], 
      "Z"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], 
       RowBox[{
        SubscriptBox["R", "y"], "(", "\[Theta]", ")"}]}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["R", "y"], "(", 
        RowBox[{"2", "\[Theta]"}], ")"}], "\[DotEqual]", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{
            RowBox[{"-", "sin"}], "\[VeryThinSpace]", "\[Theta]"}]},
          {
           RowBox[{"sin", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}]}
         }], ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "5247d716-0fc0-400b-a596-8e9f436ba2f1"],
 " ."
}], "TechNoteText",
 CellID->1838195382,ExpressionUUID->"ccf5e4c3-4876-4844-99bb-f29a7c18e1aa"],

