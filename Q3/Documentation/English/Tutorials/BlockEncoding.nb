Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"M", "\[RightTeeArrow]", 
     SubscriptBox["U", "M"]}], ":=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"V", "0"},
        {"0", "V"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {"\[CapitalLambda]", 
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[CapitalLambda]", "2"]}]]}]},
        {
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[CapitalLambda]", "2"]}]]}], "\[CapitalLambda]"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["W", "\[Dagger]"], "0"},
        {"0", 
         SuperscriptBox["W", "\[Dagger]"]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "648f5077-a891-42b8-a620-f6fce8249a12"],
 " ."
}], "TechNoteText",
 CellID->365608999,ExpressionUUID->"eed3ea23-28a9-4ea4-99cd-dce9434b96e6"],

