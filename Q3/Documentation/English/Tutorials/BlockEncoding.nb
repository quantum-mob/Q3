Cell[CellGroupData[{

Cell["Geometric View: Relation to the Grover Rotation", "Subsection",
 CellID->1339493437,ExpressionUUID->"2ce4bc03-4e4d-48d3-9301-f71302e6c864"],

Cell["\<\
The GSLW form of block encoding also allows a similar geometric \
interpretation as the standard form in the above. To see this, let us \
introduce the rotated basis vector\
\>", "TechNoteText",
 CellID->795124158,ExpressionUUID->"a76c0a38-1200-4d46-81bd-a436c8871156"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      RowBox[{"x", ":", 
        RowBox[{"-", "\[Theta]"}]}]},
     "Ket"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", "\[Theta]", ")"}], 
      TemplateBox[{"0"},
       "Ket"]}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"cos", "(", 
          RowBox[{"\[Theta]", "/", "2"}], ")"}]},
        {
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"sin", "(", 
           RowBox[{"\[Theta]", "/", "2"}], ")"}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{Right}}, "Rows" -> {{Baseline}}}], 
      ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "e5db1058-52d3-4a86-b21d-3666cca585ca"],
 " "
}], "TechNoteText",
 CellID->2008471872,ExpressionUUID->"e615f8cd-4fa8-4546-b74a-745431408df6"],

Cell[TextData[{
 "and the ",
 StyleBox["reflection",
  FontSlant->"Italic"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"x", ":", 
       RowBox[{"-", "\[Theta]"}]}]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "b2ed9196-da63-474a-84cb-1366abcdea3f"],
 " (see also ",
 ButtonBox["Figure 4",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/BlockEncoding#1875896075"],
 " for a geometric interpretation)"
}], "TechNoteText",
 CellID->91305511,ExpressionUUID->"7946081e-902b-4d23-9d26-fabcaea3fa00"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Ket"], 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Bra"]}], "-", "1"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}], "Z", "\[VeryThinSpace]", 
       RowBox[{
        SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}]}], "\[DotEqual]", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "sin", "\[VeryThinSpace]",
            "\[Theta]"}]},
         {
          RowBox[{
          "\[ImaginaryI]", "\[VeryThinSpace]", "sin", "\[VeryThinSpace]", 
           "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "cos"}], "\[VeryThinSpace]", "\[Theta]"}]}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "1570077c-ffcd-4144-a0ce-a2e9647dab05"],
 "."
}], "TechNoteText",
 CellID->1268399419,ExpressionUUID->"998a9dd8-4b43-4720-a261-e3cdfe4083c7"],

Cell["Now, note that", "TechNoteText",
 CellID->2066355011,ExpressionUUID->"02cb822f-c66b-4952-914a-ba0a40269da3"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["G", "x"], "(", "\[Lambda]", ")"}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {"\[Lambda]", 
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}]},
        {
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}], "\[Lambda]"}
       }], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "x"], "(", "\[Lambda]", ")"}], "Z"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], 
       "Z"}], "=", 
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
       RowBox[{"2", "\[Theta]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"f7cdfdb6-60a0-4fe4-a9f3-5e1cbc7e8ebc"],
 "."
}], "TechNoteText",
 CellID->123130593,ExpressionUUID->"6f8a4251-25da-477f-b3a3-bd61d66c15d0"],

Cell[TextData[{
 "In other words, combined with the Pauli ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "e3fb47ed-0b10-42da-8d8f-be102b027670"],
 " operator, the reflection ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], 
   TraditionalForm]],ExpressionUUID->"42106224-67e0-453c-99a3-ed14bf7f3574"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"x", ":", 
       RowBox[{"-", "\[Theta]"}]}]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "8b364abc-ab47-4800-9962-5fc7bd2e8f99"],
 " gives rise to the rotation around the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "9f8cf9e1-b809-45e4-8e57-08d36ecbc8b0"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Theta]"}], TraditionalForm]],ExpressionUUID->
  "15d3c662-19fa-4601-88a1-9593093a6397"]
}], "TechNoteText",
 CellID->1429124490,ExpressionUUID->"01eec193-c65c-4266-9bbe-ef58f617e7e8"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], "Z"}], 
    "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["R", "x"], "(", 
       RowBox[{"-", "\[Theta]"}], ")"}], "Z", "\[VeryThinSpace]", 
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
       RowBox[{"-", "\[Theta]"}], ")"}], "Z"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}], 
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{
         RowBox[{"-", "2"}], "\[Theta]"}], ")"}], "\[DotEqual]", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{
           "\[ImaginaryI]", "\[VeryThinSpace]", "sin", "\[VeryThinSpace]", 
            "\[Theta]"}]},
          {
           RowBox[{
           "\[ImaginaryI]", " ", "sin", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}]}
         }], ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "4b1ba9ae-fe20-4647-9bb6-dfe80283601b"],
 " ."
}], "TechNoteText",
 CellID->1838195382,ExpressionUUID->"f9d52bc2-88b5-4d5d-95a5-7b5e3dfe4a9b"],

