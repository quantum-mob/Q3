           SuperscriptBox["\[Lambda]", "2"]}]], 
         RowBox[{"-", "\[Lambda]"}]}
       }], ")"}], "\[DotEqual]", 
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "y"], "(", "\[Lambda]", ")"}]}], "=", 
    RowBox[{
     SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"50f1a912-24c0-4c2e-b35d-46764a187ac8"]
}], "TechNoteText",
