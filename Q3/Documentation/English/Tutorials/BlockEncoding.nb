Cell[CellGroupData[{

Cell["Geometric View: Relation to the Grover Rotation", "Subsection",
 CellID->1339493437,ExpressionUUID->"ee794e55-0e83-4a09-8093-f195bca879bd"],

Cell["\<\
The GSLW form of block encoding also allows a similar geometric \
interpretation as the standard form in the above. To see this, let us \
introduce the rotated basis vector\
\>", "TechNoteText",
 CellID->795124158,ExpressionUUID->"4a402e51-d36a-4eca-bfd8-8d96241cd931"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      RowBox[{"x", ":", 
        RowBox[{"-", "\[Theta]"}]}]},
     "Ket"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", "\[Theta]", ")"}], 
      TemplateBox[{"0"},
       "Ket"]}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"cos", "(", 
          RowBox[{"\[Theta]", "/", "2"}], ")"}]},
        {
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"sin", "(", 
           RowBox[{"\[Theta]", "/", "2"}], ")"}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{Right}}, "Rows" -> {{Baseline}}}], 
      ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "3a069ac0-ac07-491a-869d-663f1feb9fd7"],
 " "
}], "TechNoteText",
 CellID->2008471872,ExpressionUUID->"d6809cdd-5c2f-40fc-9ad2-435243a98acc"],

Cell[TextData[{
 "and the ",
 StyleBox["reflection",
  FontSlant->"Italic"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"x", ":", 
       RowBox[{"-", "\[Theta]"}]}]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "c1d54a4f-80cb-4c5d-a011-e207b6e5f5aa"],
 " (see also ",
 ButtonBox["Figure 4",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/BlockEncoding#1875896075"],
 " for a geometric interpretation)"
}], "TechNoteText",
 CellID->91305511,ExpressionUUID->"8bf17ce9-fa1d-4e73-b69a-b652bc6a6658"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Ket"], 
       TemplateBox[{
         RowBox[{"x", ":", 
           RowBox[{"-", "\[Theta]"}]}]},
        "Bra"]}], "-", "1"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}], "Z", "\[VeryThinSpace]", 
       RowBox[{
        SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}]}], "\[DotEqual]", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "sin", "\[VeryThinSpace]",
            "\[Theta]"}]},
         {
          RowBox[{
          "\[ImaginaryI]", "\[VeryThinSpace]", "sin", "\[VeryThinSpace]", 
           "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "cos"}], "\[VeryThinSpace]", "\[Theta]"}]}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a8552bb6-3410-4e26-b6e5-1c226a601ead"],
 "."
}], "TechNoteText",
 CellID->1268399419,ExpressionUUID->"9f8bfe4e-bb1f-4679-85eb-ef5a60b55b0c"],

Cell["Now, note that", "TechNoteText",
 CellID->2066355011,ExpressionUUID->"015fa8b0-8d88-44a7-ba30-7e011dd89d3b"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["G", "x"], "(", "\[Lambda]", ")"}], "\[DotEqual]", 
     RowBox[{"(", GridBox[{
        {"\[Lambda]", 
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}]},
        {
         RowBox[{"\[ImaginaryI]", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["\[Lambda]", "2"]}]]}], "\[Lambda]"}
       }], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "x"], "(", "\[Lambda]", ")"}], "Z"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], 
       "Z"}], "=", 
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
       RowBox[{"2", "\[Theta]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"898a54c0-3fec-4cc1-bc54-40cfe36f174f"],
 "."
}], "TechNoteText",
 CellID->123130593,ExpressionUUID->"3fc46bdf-308a-48d3-bcc0-f140838c891f"],

Cell[TextData[{
 "In other words, combined with the Pauli ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "86fc212f-851d-4b1a-8eaa-b82c8ad05184"],
 " operator, the reflection ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], 
   TraditionalForm]],ExpressionUUID->"cff328cb-468a-4c4a-a25b-ded85efe7a88"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     RowBox[{"x", ":", 
       RowBox[{"-", "\[Theta]"}]}]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "b375e44e-3da5-4864-99e7-c671a6c801b1"],
 " gives rise to the rotation around the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "7c9c68c7-54d6-4574-a740-7708a2c9f3df"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Theta]"}], TraditionalForm]],ExpressionUUID->
  "23e910fd-0b6f-462d-8012-47b6e42bbcd6"]
}], "TechNoteText",
 CellID->1429124490,ExpressionUUID->"c46e6934-653c-4d2a-9537-c95f58564bf8"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "x"], "(", "cos\[Theta]", ")"}], "Z"}], 
    "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["R", "x"], "(", 
       RowBox[{"-", "\[Theta]"}], ")"}], "Z", "\[VeryThinSpace]", 
      RowBox[{
       SubsuperscriptBox["R", "x", "\[Dagger]"], "(", 
       RowBox[{"-", "\[Theta]"}], ")"}], "Z"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}], 
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{"-", "\[Theta]"}], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["R", "x"], "(", 
        RowBox[{
         RowBox[{"-", "2"}], "\[Theta]"}], ")"}], "\[DotEqual]", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{
           "\[ImaginaryI]", "\[VeryThinSpace]", "sin", "\[VeryThinSpace]", 
            "\[Theta]"}]},
          {
           RowBox[{
           "\[ImaginaryI]", " ", "sin", "\[VeryThinSpace]", "\[Theta]"}], 
           RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}]}
         }], ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "ea088682-2fe2-4100-9fa9-3e3187588038"],
 " ."
}], "TechNoteText",
 CellID->1838195382,ExpressionUUID->"b461d061-b91b-4bb6-9515-35c40f69ed14"],

