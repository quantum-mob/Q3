Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "M"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", "V"}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       UnderscriptBox["\[CapitalSigma]", "k"], 
       RowBox[{
        RowBox[{
         SubscriptBox["\[CapitalOmega]", "y"], "(", 
         SubscriptBox["\[Lambda]", "k"], ")"}], "\[CircleTimes]", 
        TemplateBox[{"k"},
         "Ket"]}], 
       TemplateBox[{"k"},
        "Bra"]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"I", "\[CircleTimes]", 
       SuperscriptBox["W", "\[Dagger]"]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"d1c7530d-d3af-4ed3-bbaf-12378a0dda31"],
 ","
}], "TechNoteText",
