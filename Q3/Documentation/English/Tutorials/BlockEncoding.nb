Cell[TextData[{
 "The reflection ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}], 
   TraditionalForm]],ExpressionUUID->"1add1125-41d2-4a2f-bc14-2de9da3c0abd"],
 " in the line along the vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[Theta]"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "1f492767-8e4d-4297-a1e3-3d9bf4c148a8"],
 " can be converted to the rotation around the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "365d7f8a-18c6-4a35-9438-83f55a1a8c19"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Theta]"}], TraditionalForm]],ExpressionUUID->
  "c009aed8-6c66-4ef0-b817-31002e953d39"],
 " by multiplying the Pauli ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "7ce2ef19-a7cb-49e4-92ba-7e82be5cd3be"],
 " operator. In other words, it is useful to define a Grover-like rotation \
around the ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "eb5e896a-2eca-49a6-b4bf-344dcc3ee069"],
 "-axis by"
}], "TechNoteText",
 CellID->465522068,ExpressionUUID->"dbe2d0c0-edd9-4406-b84a-08a6e13f2cf6"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", "y"], "(", "\[Lambda]", ")"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "y"], "(", "cos\[Theta]", ")"}], "Z"}],
      "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], "Z", "\[VeryThinSpace]", 
       RowBox[{
        SubsuperscriptBox["R", "y", "\[Dagger]"], "(", "\[Theta]", ")"}], 
       "Z"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["R", "y"], "(", "\[Theta]", ")"}], 
        RowBox[{
         SubscriptBox["R", "y"], "(", "\[Theta]", ")"}]}], "=", 
       RowBox[{
        RowBox[{
         SubscriptBox["R", "y"], "(", 
         RowBox[{"2", "\[Theta]"}], ")"}], "\[DotEqual]", 
        RowBox[{"(", GridBox[{
           {
            RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}], 
            RowBox[{
             RowBox[{"-", "sin"}], "\[VeryThinSpace]", "\[Theta]"}]},
           {
            RowBox[{"sin", "\[VeryThinSpace]", "\[Theta]"}], 
            RowBox[{"cos", "\[VeryThinSpace]", "\[Theta]"}]}
          },
          GridBoxAlignment->{"Columns" -> {{Right}}, "Rows" -> {{Baseline}}}],
          ")"}]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "bfb58ec4-df7b-47a8-9f6d-b7974f831a44"],
 " ."
}], "TechNoteText",
 CellID->746470855,ExpressionUUID->"a008bb5b-8a0b-4e99-9be6-082dc5be5284"],

