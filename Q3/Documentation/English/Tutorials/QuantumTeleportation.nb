Cell[TextData[{
 "Alice and Bob generate an entangled pair of qubits, ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "e7a17b59-d8f4-477e-a760-d55242704e4b"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "4d0232a1-9604-45f3-ac76-5b2fad4ca2b5"],
 ", and share the pair between them. This can be done anytime before the \
procedure actually starts. Bob prepares a quantum state to send in a separate \
qubit ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],ExpressionUUID->
  "16d5a971-ab42-445a-ad56-7ae5a60e0955"],
 "."
}], "ItemNumbered",
 CellID->1787484763,ExpressionUUID->"5d87821d-5541-435d-83be-33569becd884"],

Cell[TextData[{
 "Bob makes a Bell measurement, i.e., the measurement in the Bell basis, on \
his two qubits ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "a4abb2fa-399a-44f0-b2af-eafc5296da50"],
 " and ",
 Cell[BoxData[
  FormBox["C", TraditionalForm]],ExpressionUUID->
  "489bcd45-5728-492a-8f0d-977592a1aea2"],
 "."
}], "ItemNumbered",
 CellID->1107673858,ExpressionUUID->"28a12517-6ad3-471b-a282-448c90cc157e"],

