Cell[TextData[{
 "\tTo see the effect of the function ",
 StyleBox["f",
  FontSlant->"Italic"],
 " on the final state, suppose that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "a46b5445-d6e3-4601-b39a-694dab1497e6"],
 " is a constant function. Then, the output state is given by"
}], "TechNoteText",
 CellID->332005132,ExpressionUUID->"651f0585-2c40-4e9b-a1e1-29c92e41ad2e"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"f", "(", "0", ")"}]], 
      SuperscriptBox["2", "n"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"y", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     TemplateBox[{"y"},
      "Ket"], 
     SuperscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
