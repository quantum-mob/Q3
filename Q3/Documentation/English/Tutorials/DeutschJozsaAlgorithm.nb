Cell[TextData[{
 "\tTo see the effect of the function ",
 StyleBox["f",
  FontSlant->"Italic"],
 " on the final state, suppose that ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "a837c182-edeb-4bde-a524-eb38121610ae"],
 " is a constant function. Then, the output state is given by"
}], "TechNoteText",
 CellID->332005132,ExpressionUUID->"55d839ca-9a19-4418-83f4-531a7d08fa16"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"f", "(", "0", ")"}]], 
      SuperscriptBox["2", "n"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"y", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     TemplateBox[{"y"},
      "Ket"], 
     SuperscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"x", "=", "0"}], 
        RowBox[{
         SuperscriptBox["2", "n"], "-", "1"}]], "(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"x", "\[CenterDot]", "y"}]]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"f", "(", "0", ")"}]], 
     TemplateBox[{"0"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "d2cb34be-2b07-41ca-8889-aefa4a550d07"],
 "."
}], "TechNoteText",
