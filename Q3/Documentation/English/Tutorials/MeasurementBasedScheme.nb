Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{
      SubscriptBox["\[CapitalPsi]", "out"]},
     "Ket"], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", 
       RowBox[{
        SubscriptBox["y", "2"], ",", 
        SubscriptBox["y", "3"]}]], "(", 
      RowBox[{
       SuperscriptBox["Z", 
        SubscriptBox["y", "2"]], 
       TemplateBox[{
         SubscriptBox["x", "1"]},
