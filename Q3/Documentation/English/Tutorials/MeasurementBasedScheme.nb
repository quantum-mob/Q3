Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", 
      SubscriptBox["y", "3"]], 
     RowBox[{
      TemplateBox[{
        SubscriptBox["x", "1"]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{
        SubscriptBox["x", "2"]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{
        SubscriptBox["y", "3"]},
       "Ket"], "\[CircleTimes]", 
      RowBox[{"(", 
       RowBox[{"H", "\[ThinSpace]", 
        SuperscriptBox["Z", 
