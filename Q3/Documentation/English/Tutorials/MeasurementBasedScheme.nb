     RowBox[{
      TemplateBox[{"1"},
       "Ket"], "\[CircleTimes]", 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["Z", "^"], 
        SubscriptBox[
         OverscriptBox["U", "^"], "z"], 
        TemplateBox[{"\[Psi]"},
         "Ket"]}], ")"}]}]}], 
    SqrtBox["2"]], TraditionalForm]],ExpressionUUID->
  "b5da32b7-991b-4e2e-ac91-a2cfeca2ff0b"],
