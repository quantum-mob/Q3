Cell["\<\
\tThe quantum Fourier transform, QFT for short, is a unitary transformation \
defined by the association\
\>", "TechNoteText",
 CellID->280043153,ExpressionUUID->"10f8a9e8-69e6-4c33-a07e-bc9a78ffde22"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "QFT"], 
     TemplateBox[{
       SubscriptBox["X", "y"]},
      "Ket"]}], ":=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     RowBox[{
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      RowBox[{
       TemplateBox[{
         SubscriptBox["X", "x"]},
        "Ket"], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "x", " ", 
         SubscriptBox["p", "y"]}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "f0b8c87c-54ba-4c47-8f62-444a08abc22f"]
}], "TechNoteText",
