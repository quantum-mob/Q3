       TemplateBox[{"1"},
        "Ket"], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"2", "\[Pi]", "\[VeryThinSpace]", 
         SubscriptBox[
          RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "(", 
           RowBox[{"0.", 
            SubscriptBox["y", "n"]}], ")"}], "2"]}]]}]}], 
     SqrtBox["2"]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       TemplateBox[{"0"},
        "Ket"], "+", 
       SuperscriptBox[
        RowBox[{
         TemplateBox[{"1"},
          "Ket"], "(", 
         RowBox[{"-", "1"}], ")"}], 
        SubscriptBox["y", "n"]]}], 
      SqrtBox["2"]], "=", 
     RowBox[{"H", 
      TemplateBox[{
        SubscriptBox["y", "n"]},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "1234fb89-4057-4484-b7f5-e8c4eae871f8"],
