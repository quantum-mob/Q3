Cell["\<\
\tThe quantum Fourier transform, QFT for short, is a unitary transformation \
defined by the association\
\>", "TechNoteText",
 CellID->280043153,ExpressionUUID->"4a5c578f-1fe6-4d5f-b9ca-6eefcad4d654"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "QFT"], 
     TemplateBox[{
       SubscriptBox["X", "y"]},
      "Ket"]}], ":=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     RowBox[{
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      RowBox[{
       TemplateBox[{
         SubscriptBox["X", "x"]},
        "Ket"], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"\[ImaginaryI]", " ", "x", " ", 
         SubscriptBox["p", "y"]}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "ba3dbc26-5e45-4f1d-8771-c362495346d3"]
}], "TechNoteText",
 CellID->1548601894,ExpressionUUID->"b707f8cb-05d6-4cfb-a3a4-886ed7d38f80"],

