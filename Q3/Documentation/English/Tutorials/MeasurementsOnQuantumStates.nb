Cell[TextData[{
 "It is instructive to expand the interaction unitary operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "2d4f4d28-1d1e-4a7a-ac4e-d3ec5e1a77ff"],
 " int using the spectral decomposition of the observable ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "14bf77bf-55b3-4846-a3cc-ba3f9669099e"],
 " as"
}], "TechNoteText",
 CellID->798213346,ExpressionUUID->"56e56eae-321c-4c3b-88de-e4d09228495f"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "int"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "a"], 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "g", " ", "a", " ", "P"}]], "\[CircleTimes]", 
       TemplateBox[{"a"},
        "Ket"]}], 
      TemplateBox[{"a"},
       "Bra"]}]}]}], TraditionalForm]],ExpressionUUID->
  "66da4dae-4d38-43af-b867-bdc32dd869a4"]
}], "TechNoteText",
 CellID->241363413,ExpressionUUID->"fbd1a920-fce4-4363-b494-5f906a4a5613"],

Cell[TextData[{
 "The operator ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "a"], ":=", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "g", " ", "a", " ", "P"}], ")"}]}],
    TraditionalForm]],ExpressionUUID->
  "3f203292-0d1b-4d75-b293-d7f9f68b8d09"],
 " represents the translation by an amount ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", " ", "a"}], TraditionalForm]],ExpressionUUID->
  "17c7bc67-b825-405e-941a-0c2a129250e5"],
 " depending on eigenvalue ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "f9a00099-01ee-413e-9f69-13e0012c1a5d"],
 " of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "a812ba8a-40e5-4878-8e2c-d94fcf336257"],
 ". To clarify the physical interpretation, we rewrite the interaction\n \
unitary operator into"
}], "TechNoteText",
 CellID->2145756755,ExpressionUUID->"c53293b2-862f-4613-9ad0-d655ace61fcd"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "int"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "a"], 
     RowBox[{
      RowBox[{
       SubscriptBox["T", "a"], "\[CircleTimes]", 
       TemplateBox[{"a"},
        "Ket"]}], 
      TemplateBox[{"a"},
       "Bra"]}]}]}], TraditionalForm]],ExpressionUUID->
  "e8c9d4dc-75e7-49c9-b06e-ea0c78a2aaa4"],
 "."
}], "TechNoteText",
