Cell[TextData[{
 "It is instructive to expand the interaction unitary operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "adf72b91-f9ad-491f-bff3-546d0345c408"],
 " int using the spectral decomposition of the observable ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "0b4c3f76-d51d-42ff-a72c-bf3d00b16247"],
 " as"
}], "TechNoteText",
 CellID->798213346,ExpressionUUID->"c9129c60-6443-4c1f-ac4c-275d087eb7b5"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "int"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "a"], 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "g", " ", "a", " ", "P"}]], "\[CircleTimes]", 
       TemplateBox[{"a"},
        "Ket"]}], 
      TemplateBox[{"a"},
       "Bra"]}]}]}], TraditionalForm]],ExpressionUUID->
  "8f63e22e-5ee4-454e-9e3d-d904971e9e91"]
}], "TechNoteText",
 CellID->241363413,ExpressionUUID->"9f6238be-045f-4c40-a074-77dc7db24a40"],

Cell[TextData[{
 "The operator ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "a"], ":=", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "g", " ", "a", " ", "P"}], ")"}]}],
    TraditionalForm]],ExpressionUUID->
  "6a5438ca-0304-4a57-8ea4-c03842d9024b"],
 " represents the translation by an amount ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", " ", "a"}], TraditionalForm]],ExpressionUUID->
  "71221f06-2ea5-4da6-97d6-77806576af2d"],
 " depending on eigenvalue ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "d153afa3-6397-47bc-9164-b69d791356fc"],
 " of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "e7894a61-2ad5-409c-87f4-86dfd3525c30"],
 ". To clarify the physical interpretation, we rewrite the interaction\n \
unitary operator into"
}], "TechNoteText",
 CellID->2145756755,ExpressionUUID->"547b1c49-5d61-4be8-89df-055741107fda"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "int"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "a"], 
     RowBox[{
      RowBox[{
       SubscriptBox["T", "a"], "\[CircleTimes]", 
       TemplateBox[{"a"},
        "Ket"]}], 
      TemplateBox[{"a"},
       "Bra"]}]}]}], TraditionalForm]],ExpressionUUID->
  "bb746674-d77c-4904-b1b4-79c3e9d83269"],
 "."
}], "TechNoteText",
 CellID->1040897214,ExpressionUUID->"334613fb-5ca7-4b19-b64c-50baf45b7cf8"],

