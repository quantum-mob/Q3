Cell[TextData[{
 "We consider a simple example. The density operator describes an ensemble of \
two pure states ",
 Cell[BoxData[
  TemplateBox[{"0"},
   "Ket"]],ExpressionUUID->"ff36a6a0-6c26-46cf-952b-ea6a97502ef3"],
 " and ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{"+"},
    "Ket"], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{"0"},
       "Ket"], "+", 
      TemplateBox[{"1"},
       "Ket"]}], ")"}], "/", 
    SqrtBox["2"]}]}]],ExpressionUUID->
  "dc564b33-7f6f-4a2c-9020-9e0129f5c498"],
 "."
}], "TechNoteMathCaption",
 CellID->1058952787,ExpressionUUID->"d1862400-1a10-447d-8ddf-4f35b6abee68"],

