Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Rho]", "A"], "=", 
    RowBox[{
     UnderscriptBox["Tr", "B"], 
     SubscriptBox["\[Rho]", 
      RowBox[{"A", "\[VeryThinSpace]", "B"}]]}]}], TraditionalForm]],
  ExpressionUUID->"3c93df68-4ae6-4643-8c06-8864cba1416c"],
 " ,    ",
 Cell[BoxData[
