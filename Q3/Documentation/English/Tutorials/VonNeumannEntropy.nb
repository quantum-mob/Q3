 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      RowBox[{"S", "(", 
       SubscriptBox["\[Rho]", "A"], ")"}], "-", 
      RowBox[{"S", "(", 
       SubscriptBox["\[Rho]", "B"], ")"}]}], "\[RightBracketingBar]"}], 
    "\[LessEqual]", 
    RowBox[{"S", "(", 
     SubscriptBox["\[Rho]", 
      RowBox[{"A", "\[VeryThinSpace]", "B"}]], ")"}]}], TraditionalForm]],
  ExpressionUUID->"d7367da4-0d05-4840-a2a3-f5fb8ee3af31"],
 " ."
}], "TechNoteText",
 CellID->1049858610,ExpressionUUID->"bdf2cc3e-eaab-4b6f-8b10-0a7db0e62804"],

