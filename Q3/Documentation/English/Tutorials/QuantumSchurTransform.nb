Cell[TextData[{
 "\tAt the moment, the vectors in the Young-Yamanouchi basis do not take any \
particular form of expression. All we require is that under the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"e926b5fe-c882-4591-a101-4844bad8900d"],
 ", the basis vectors transform as"
}], "TechNoteText",
 CellID->1155245835,ExpressionUUID->"d45d6292-3338-487f-9267-4c7eed49f4fe"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["\[CapitalPi]", "^"], "(", "S", ")"}], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         SubscriptBox[
          RowBox[{"(", 
            SubscriptBox["y", "\[Lambda]"], ")"}], 
          RowBox[{"n", "-", "1"}]]}]},
      "Ket"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "x"], 
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Lambda]", ",", 
          SubscriptBox[
           RowBox[{"(", 
             SubscriptBox["x", "\[Lambda]"], ")"}], 
           RowBox[{"n", "-", "1"}]]}]},
       "Ket"], 
      RowBox[{
       SubsuperscriptBox["\[CapitalPi]", 
        RowBox[{"x", " ", "y"}], "\[Lambda]"], "(", "S", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"18410c58-3bc0-4b83-aa6d-ed5f7cfc21fc"],
 ","
}], "DisplayMath",
 CellID->1800144916,ExpressionUUID->"bd1c9c5e-8674-4e2c-9d49-535b6a4bbe52"],

