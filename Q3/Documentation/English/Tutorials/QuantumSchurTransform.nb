Cell[TextData[{
 "\tAt the moment, the vectors in the Young-Yamanouchi basis do not take any \
particular form of expression. All we require is that under the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"025edf39-840b-45bb-9b47-f80f213f8a7a"],
 ", the basis vectors transform as"
}], "TechNoteText",
 CellID->1155245835,ExpressionUUID->"b04ac415-f956-4894-81c9-a56c8969ee9b"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["\[CapitalPi]", "^"], "(", "S", ")"}], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         SubscriptBox[
          RowBox[{"(", 
            SubscriptBox["y", "\[Lambda]"], ")"}], 
          RowBox[{"n", "-", "1"}]]}]},
      "Ket"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "x"], 
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Lambda]", ",", 
          SubscriptBox[
           RowBox[{"(", 
             SubscriptBox["x", "\[Lambda]"], ")"}], 
           RowBox[{"n", "-", "1"}]]}]},
       "Ket"], 
      RowBox[{
       SubsuperscriptBox["\[CapitalPi]", 
        RowBox[{"x", " ", "y"}], "\[Lambda]"], "(", "S", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"517f8e88-7ff6-4db9-bdef-6843292c3086"],
 ","
}], "DisplayMath",
 CellID->1800144916,ExpressionUUID->"9f20667f-5142-4e32-b08e-a6f39ca677b9"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
