Cell["\<\
Suppose that Alice and Bob are located far away from each other, and there is \
no quantum channel available between them. Fortunately, they had shared and \
are maintaining a maximally entangled pair of qubits, and there is a \
classical channel available for communication between them.\
\>", "TechNoteText",
 CellID->497326297,ExpressionUUID->"bfad1894-8335-4930-aa4e-27d623c71ff5"],

Cell[TextData[{
 "Alice wants to implement a ",
 ButtonBox["CNOT",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/CNOT"],
 " gate targeted on Bob's qubit, not the one involved in the shared entangled \
pair, controlled by her own qubit. The protocol is summarized in the \
following quantum circuit."
}], "TechNoteText",
 CellID->556719555,ExpressionUUID->"2d531a4a-713c-4967-bab7-30864682587a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Let", "[", 
   RowBox[{"Binary", ",", "x", ",", "m"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"in", "=", 
   RowBox[{"KetRegulate", "[", 
    RowBox[{
     RowBox[{"Ket", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"S", "[", "1", "]"}], "->", 
        RowBox[{"x", "[", "1", "]"}]}], ",", 
       RowBox[{
        RowBox[{"S", "[", "4", "]"}], "->", 
        RowBox[{"x", "[", "4", "]"}]}]}], "]"}], ",", 
     RowBox[{"S", "@", 
      RowBox[{"{", 
       RowBox[{"1", ",", "2", ",", "3", ",", "4"}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"out", "=", 
   RowBox[{"Ket", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"S", "[", "1", "]"}], "->", 
      RowBox[{"x", "[", "1", "]"}]}], ",", 
     RowBox[{
      RowBox[{"S", "@", 
       RowBox[{"{", 
        RowBox[{"2", ",", "3"}], "}"}]}], "->", 
      RowBox[{"m", "@", 
       RowBox[{"{", 
        RowBox[{"2", ",", "3"}], "}"}]}]}], ",", 
     RowBox[{
      RowBox[{"S", "[", "4", "]"}], "->", 
      RowBox[{"Mod", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "[", "1", "]"}], "+", 
         RowBox[{"x", "[", "4", "]"}]}], ",", "2"}], "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"core", "=", 
