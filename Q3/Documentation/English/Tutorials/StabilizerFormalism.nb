Cell[TextData[{
 "Every element of the Pauli group squares to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "I"}], TraditionalForm]],ExpressionUUID->
  "c533bd57-5e94-493e-be32-b31f1ccae9e6"],
 ". The elements that square to ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],ExpressionUUID->
  "42b542c4-4809-4f0a-afe4-c99c223a84b2"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "1"}], TraditionalForm]],ExpressionUUID->
  "dbc81fc3-92a2-4538-ae5d-c770e1353084"],
 " whereas those that square to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Minus]", "I"}], TraditionalForm]],ExpressionUUID->
  "ea53a6fe-0f11-4c6c-b370-fc2fb6ba463c"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], TraditionalForm]],
  ExpressionUUID->"1dff8b80-0488-4899-8714-c2a207542901"],
 "."
}], "Item",
