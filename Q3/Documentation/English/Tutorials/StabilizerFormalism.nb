Cell[TextData[{
 "Ignoring the phase factors leads to a drastically simple structure of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"2db1937c-7cf5-4f01-9399-49d79f3318e9"],
 ", compared to the full Pauli group  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"79cd7178-a8c9-415b-ab24-b2236b759ac2"],
 ". For example, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", " ", "Y"}], TraditionalForm]],ExpressionUUID->
  "02fa9779-b491-401f-8bf1-2a3b7f4e8876"],
 " is now equal to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", " ", "X"}], TraditionalForm]],ExpressionUUID->
  "cd157e17-90e6-40b5-bc47-064ddd28dda4"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", "X\[ScriptCapitalZ]", ")"}], 
     RowBox[{"(", "Y\[ScriptCapitalZ]", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"X", " ", "Y"}], ")"}], "\[ScriptCapitalZ]"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Y", " ", "X"}], ")"}], "\[ScriptCapitalZ]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"49f9a611-0d16-42d1-8579-354a84f44d8e"],
 ". This means that unlike the full Pauli group  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"1e79af07-217d-4c35-b417-f7bb4ec88aa9"],
 ", the Pauli group  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"dc77cc82-74b1-4edb-95ce-d83143cfa7c1"],
 " is ",
 StyleBox["Abelian",
  FontSlant->"Italic"],
 "."
