Cell[TextData[{
 "Every element of the Pauli group squares to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "I"}], TraditionalForm]],ExpressionUUID->
  "c5ebeeeb-33de-4a65-aec3-e1b4e47a6e69"],
 ". The elements that square to ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],ExpressionUUID->
  "e1865302-ba2c-4161-9301-bfab98cdb419"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "1"}], TraditionalForm]],ExpressionUUID->
  "04eeacd4-3987-4fe0-a01f-c4a28f01a307"],
 " whereas those that square to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Minus]", "I"}], TraditionalForm]],ExpressionUUID->
  "f8506c54-42b0-49ba-b959-9ead58fb597e"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], TraditionalForm]],
  ExpressionUUID->"068b4d8f-8c22-42f3-adb3-2a5c96aa9190"],
 "."
}], "Item",
 CellID->165807436,ExpressionUUID->"40ff02c9-2715-42e5-b33d-c45300cb1a59"],

