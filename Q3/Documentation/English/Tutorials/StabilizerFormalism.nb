Cell[TextData[{
 "Since they generate the ",
 ButtonBox["full Clifford group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullCliffordGroup"],
 ", the ",
 ButtonBox["Hadamard",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/Hadamard"],
 ", ",
 ButtonBox["quadrant",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/quadrant"],
 " phase, and ",
 ButtonBox["CNOT",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/CNOT"],
 " gates are called the ",
 StyleBox["elementary Clifford gates",
  FontSlant->"Italic"],
 " or simply Clifford gates. Quantum circuits consisting of elementary \
Clifford gates are called ",
 StyleBox["Clifford circuits",
  FontSlant->"Italic"],
 "."
}], "TechNoteText",
 CellID->67910332,ExpressionUUID->"b85ba216-03ba-4e71-85f9-7e431ae83961"],

Cell[TextData[{
 "As in the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullPauliGroup"],
 ", elements in the ",
 ButtonBox["full Clifford group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullCliffordGroup"],
 " appear redundantly and differ only in global phase factors."
}], "TechNoteText",
 CellID->1055133367,ExpressionUUID->"4986cdc2-f24a-4655-ac60-69ffbe4481f2"],

