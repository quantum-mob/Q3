 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "1"], TraditionalForm]],ExpressionUUID->
  "4658e6c7-73ca-4be0-bcae-feb6cdf29ef9"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["P", "2"], TraditionalForm]],ExpressionUUID->
  "a9cb433c-a3ca-47a3-86dc-1e033f146cad"],
 " be ",
