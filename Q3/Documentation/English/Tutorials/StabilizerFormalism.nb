Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"S", "(", "U", ")"}], "\[CenterDot]", "J", "\[CenterDot]", 
     RowBox[{
      SuperscriptBox["S", "\[Dagger]"], "(", "U", ")"}]}], "=", "J"}], 
   TraditionalForm]],ExpressionUUID->"495a1e06-edb3-483b-99b7-35e2c92a5c14"],
 ","
}], "DisplayMath",
