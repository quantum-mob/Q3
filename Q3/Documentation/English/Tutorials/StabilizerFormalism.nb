Cell[TextData[{
 "Every element of the Pauli group squares to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "I"}], TraditionalForm]],ExpressionUUID->
  "a22fcbbb-819a-4a15-8d33-b5fa797c154c"],
 ". The elements that square to ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],ExpressionUUID->
  "7b7fcafc-d282-4106-9773-fce2deb3b1ea"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "1"}], TraditionalForm]],ExpressionUUID->
  "e42a97f3-49fd-41f4-98de-525341957f04"],
 " whereas those that square to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Minus]", "I"}], TraditionalForm]],ExpressionUUID->
  "ae5112f1-bb5a-421e-b33f-2fef216ec79c"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], TraditionalForm]],
  ExpressionUUID->"c418c218-b5d2-450f-b7b1-879fd0f066bd"],
 "."
}], "Item",
