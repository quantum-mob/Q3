 StyleBox[ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/PauliGroup"],
  FontSlant->"Italic"],
 StyleBox[".",
  FontSlant->"Italic"]
}], "TechNoteText",
 CellID->1331686937,ExpressionUUID->"48a82740-0251-41ee-b20a-67ac57d7924a"],

