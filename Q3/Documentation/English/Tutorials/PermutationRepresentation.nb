Cell["Demonstration", "Subsection",
 CellID->1773316304,ExpressionUUID->"a3000a49-5ccf-44ea-a52b-e676641e9d5d"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"$n", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"cyc", "=", 
  RowBox[{"Cycles", "@", 
   RowBox[{"{", 
    RowBox[{"Range", "@", "$n"}], "}"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"prm", "=", 
