 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "21e50513-7057-4a97-b98e-4fe1c9358e99"],
 " acts on a set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalS]", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["s", "1"], ",", 
      RowBox[{
       SubscriptBox["s", "2"], "\[Ellipsis]"}], ",", 
      SubscriptBox["s", "n"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "22f07253-658d-4600-8cfd-ca556dc1c6ee"],
