 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "42693b83-fa92-4144-a577-e52661e0b47a"],
 " acts on a set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalS]", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["s", "1"], ",", 
      RowBox[{
       SubscriptBox["s", "2"], "\[Ellipsis]"}], ",", 
      SubscriptBox["s", "n"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "9729d142-a569-40db-82db-3642a34460e7"],
