 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["s", "1"]},
       "Ket"], ",", 
      TemplateBox[{
        SubscriptBox["s", "2"]},
       "Ket"], ",", "\[Ellipsis]", ",", 
      TemplateBox[{
        SubscriptBox["s", "n"]},
       "Ket"]}], "}"}], ","}], TraditionalForm]],ExpressionUUID->
  "13addc7a-63fd-43ff-af3c-dd4169dfb930"],
