 Cell[BoxData[
  FormBox[
   RowBox[{"G", "\[Element]", "\[ScriptCapitalG]"}], TraditionalForm]],
  ExpressionUUID->"57fd6993-52f6-4e68-af58-6dce0372abfa"],
