Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "\[DotEqual]", 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"\[LeftDoubleBracketingBar]", 
        RowBox[{"\[Alpha]", 
         SubscriptBox["\[LeftDoubleBracketingBar]", "1"]}]}]]], 
     RowBox[{"(", GridBox[{
        {
