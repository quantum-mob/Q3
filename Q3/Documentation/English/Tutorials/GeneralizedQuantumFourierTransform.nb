Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["L", "^"], "(", "g", ")"}], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
      "Ket"]}], ":=", 
    RowBox[{
