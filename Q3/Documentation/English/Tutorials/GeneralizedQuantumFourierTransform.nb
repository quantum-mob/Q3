Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalH]", TraditionalForm]],ExpressionUUID->
  "b42b61fe-b8c4-43ee-b81b-bc43e5fd5364"],
