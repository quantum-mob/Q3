 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "7e2f4de1-6777-4779-996b-2c6ec55aafd7"],
 "."
}], "TechNoteText",
 CellID->1175103296,ExpressionUUID->"87da3388-a59b-458d-9ae9-a95f78e5162c"]
