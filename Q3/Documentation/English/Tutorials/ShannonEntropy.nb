       RowBox[{"p", "(", "y", ")"}]], "log", 
      FractionBox[
       RowBox[{"p", 
        RowBox[{"(", 
         RowBox[{"x", ",", "y"}], ")"}]}], 
       RowBox[{"p", "(", "y", ")"}]]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         UnderscriptBox["\[CapitalSigma]", 
          RowBox[{"x", ",", "y"}]]}], 
        RowBox[{"p", "(", 
         RowBox[{"x", ",", "y"}], ")"}], "log", "\[VeryThinSpace]", 
        RowBox[{"p", "(", 
         RowBox[{"x", ",", "y"}], ")"}]}], "+", 
