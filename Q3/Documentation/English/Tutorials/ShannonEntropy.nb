Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{"X", "|", "Y"}], " ", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Minus]", 
       UnderscriptBox["\[CapitalSigma]", "y"]}], 
      RowBox[{"p", "(", "y", ")"}], 
      UnderscriptBox["\[CapitalSigma]", "x"], 
      FractionBox[
       RowBox[{"p", "(", 
