Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p", "(", "x", ")"}], "}"}], "\[DoubleVerticalBar]", 
      RowBox[{"{", 
       RowBox[{"q", "(", "x", ")"}], "}"}]}], ")"}], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "x"], 
      RowBox[{"p", "(", "x", ")"}], " ", "log", " ", 
      FractionBox[
       RowBox[{"p", "(", "x", ")"}], 
       RowBox[{"q", "(", "x", ")"}]]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"H", "(", 
        RowBox[{"{", 
         RowBox[{"p", "(", "x", ")"}], "}"}], ")"}]}], "-", 
      RowBox[{
       UnderscriptBox["\[CapitalSigma]", "x"], 
       RowBox[{"p", "(", "x", ")"}], "log", "\[VeryThinSpace]", 
       RowBox[{"q", "(", "x", ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "f8b88e9a-bb59-4451-9bd5-bb0346d64668"],
 "."
}], "TechNoteText",
 CellID->1821852384,ExpressionUUID->"2c00e90c-90c3-4b62-af86-7193dfa52d47"],

