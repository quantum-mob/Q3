Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p", "(", "x", ")"}], "}"}], "\[DoubleVerticalBar]", 
      RowBox[{"{", 
       RowBox[{"q", "(", "x", ")"}], "}"}]}], ")"}], ":=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "x"], 
      RowBox[{"p", "(", "x", ")"}], " ", "log", " ", 
      FractionBox[
       RowBox[{"p", "(", "x", ")"}], 
       RowBox[{"q", "(", "x", ")"}]]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"H", "(", 
        RowBox[{"{", 
         RowBox[{"p", "(", "x", ")"}], "}"}], ")"}]}], "-", 
      RowBox[{
       UnderscriptBox["\[CapitalSigma]", "x"], 
       RowBox[{"p", "(", "x", ")"}], "log", "\[VeryThinSpace]", 
       RowBox[{"q", "(", "x", ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "f61aa89d-5add-4a61-b3b8-f7814b82d819"],
 "."
}], "TechNoteText",
 CellID->1821852384,ExpressionUUID->"23c1ec57-7e2c-419a-8b9d-5a2d66187f0b"],

