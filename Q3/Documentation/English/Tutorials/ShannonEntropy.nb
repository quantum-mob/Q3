Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"H", "(", 
     RowBox[{"X", "|", "Y"}], " ", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Minus]", 
       UnderscriptBox["\[CapitalSigma]", "y"]}], 
      RowBox[{"p", "(", "y", ")"}], 
      UnderscriptBox["\[CapitalSigma]", "x"], 
      FractionBox[
       RowBox[{"p", "(", 
        RowBox[{"x", ",", "y"}], ")"}], 
       RowBox[{"p", "(", "y", ")"}]], "log", 
      FractionBox[
       RowBox[{"p", 
        RowBox[{"(", 
         RowBox[{"x", ",", "y"}], ")"}]}], 
       RowBox[{"p", "(", "y", ")"}]]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         UnderscriptBox["\[CapitalSigma]", 
          RowBox[{"x", ",", "y"}]]}], 
        RowBox[{"p", "(", 
         RowBox[{"x", ",", "y"}], ")"}], "log", "\[VeryThinSpace]", 
        RowBox[{"p", "(", 
         RowBox[{"x", ",", "y"}], ")"}]}], "+", 
       RowBox[{
        UnderscriptBox["\[CapitalSigma]", "y"], 
        RowBox[{"p", "(", "y", ")"}], "log", "\[VeryThinSpace]", 
        RowBox[{"p", "(", "y", ")"}]}]}], "=", 
      RowBox[{
       RowBox[{"H", "(", 
        RowBox[{"X", ",", "Y"}], ")"}], "-", 
