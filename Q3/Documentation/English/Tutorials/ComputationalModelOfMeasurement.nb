       RowBox[{
        RowBox[{
         TemplateBox[{"x"},
          "Ket"], " ", ":", " ", "x"}], "=", "0"}], ",", "1", ",", 
       RowBox[{".", " ", ".", " ", "."}], " ", ",", 
       RowBox[{
        SuperscriptBox["2", "n"], "\[Minus]", "1"}]}], "}"}], "."}]}], 
