      "RowDefault"]}],
    QuantumMob`Q3`Multiply[
     $CellContext`S[2, 3], 
     $CellContext`S[3, 3]]]}], "}"}]], "Output",
 CellLabel->"Out[4]=",
 CellID->1355232688,ExpressionUUID->"ff5241b7-d5e9-41e2-b433-432c678b5e2c"]
}, Open  ]],

Cell[TextData[{
 "This is a set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SubscriptBox["E", "\[Mu]"], "}"}], TraditionalForm]],ExpressionUUID->
  "934e000a-ce0a-49ba-8e86-01b35f5bd7d0"],
 " of error operators correctable by the code."
}], "TechNoteMathCaption",
 CellID->509261491,ExpressionUUID->"04dce7fc-ae07-41af-9df4-479c6052d862"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"err", "=", 
   RowBox[{"{", 
    RowBox[{"1", ",", 
     RowBox[{"S", "[", 
      RowBox[{"1", ",", "1"}], "]"}], ",", 
     RowBox[{"S", "[", 
      RowBox[{"2", ",", "1"}], "]"}], ",", 
     RowBox[{"S", "[", 
      RowBox[{"3", ",", "1"}], "]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"err", "//", "PauliForm"}]}], "Input",
 CellLabel->"In[5]:=",
 CellID->1296505570,ExpressionUUID->"faf1ef8b-d7dd-465e-8d56-7872d0dc7f76"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   InterpretationBox[
    RowBox[{"\<\"I\"\>", "\[CircleTimes]", "\<\"I\"\>", 
     "\[CircleTimes]", "\<\"I\"\>"}],
    1], ",", 
   InterpretationBox[
    RowBox[{
     TemplateBox[{"\"X\""},
      "RowDefault"], "\[CircleTimes]", "\<\"I\"\>", 
     "\[CircleTimes]", "\<\"I\"\>"}],
    $CellContext`S[1, 1]], ",", 
   InterpretationBox[
    RowBox[{"\<\"I\"\>", "\[CircleTimes]", 
     TemplateBox[{"\"X\""},
      "RowDefault"], "\[CircleTimes]", "\<\"I\"\>"}],
    $CellContext`S[2, 1]], ",", 
   InterpretationBox[
    RowBox[{"\<\"I\"\>", "\[CircleTimes]", "\<\"I\"\>", "\[CircleTimes]", 
     TemplateBox[{"\"X\""},
      "RowDefault"]}],
    $CellContext`S[3, 1]]}], "}"}]], "Output",
 CellLabel->"Out[6]=",
 CellID->1184227134,ExpressionUUID->"f6f60df5-13ad-453f-b9dd-2af608f5e665"]
}, Open  ]],

Cell[TextData[{
 "To check if the errors are indeed correctable by the code, we need to \
examine all combinations ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["E", "\[Mu]", "\[Dagger]"], 
