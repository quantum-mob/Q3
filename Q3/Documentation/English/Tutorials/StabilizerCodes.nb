Cell[CellGroupData[{

Cell["Bit-Flip Code", "Subsection",
 CellTags->"Section17",
 CellID->2108474791,ExpressionUUID->"2717e4d1-1f38-4c3a-83c0-9ebfdc97a4cc"],

Cell[TextData[{
 "Consider the bit-flip code. The stabilizer ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalS]", TraditionalForm]],ExpressionUUID->
  "d2fee098-2005-471e-b6fa-9deada3f459c"],
 " of the code space is generated by these generators."
}], "TechNoteMathCaption",
 CellID->601916435,ExpressionUUID->"a5db5e22-c5bf-4e13-b43c-714961b5842d"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"gnr", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"S", "[", 
       RowBox[{"1", ",", "3"}], "]"}], "**", 
      RowBox[{"S", "[", 
       RowBox[{"2", ",", "3"}], "]"}]}], ",", 
     RowBox[{
      RowBox[{"S", "[", 
       RowBox[{"2", ",", "3"}], "]"}], "**", 
      RowBox[{"S", "[", 
       RowBox[{"3", ",", "3"}], "]"}]}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"gnr", "//", "PauliForm"}]}], "Input",
 CellLabel->"In[3]:=",
 CellID->99339714,ExpressionUUID->"4d5c6fc4-b8b3-4061-955b-4fe5db7503f4"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   InterpretationBox[
    RowBox[{
     TemplateBox[{"\"Z\""},
      "RowDefault"], "\[CircleTimes]", 
     TemplateBox[{"\"Z\""},
      "RowDefault"], "\[CircleTimes]", "\<\"I\"\>"}],
    QuantumMob`Q3`Multiply[
     $CellContext`S[1, 3], 
     $CellContext`S[2, 3]]], ",", 
   InterpretationBox[
    RowBox[{"\<\"I\"\>", "\[CircleTimes]", 
     TemplateBox[{"\"Z\""},
      "RowDefault"], "\[CircleTimes]", 
