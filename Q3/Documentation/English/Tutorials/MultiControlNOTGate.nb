Cell[TextData[{
 "The multi - control NOT gate is similar to the ",
 ButtonBox["CNOT gate",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/TwoQubitGates#1585062394"],
 " on two qubits, but it is controlled by a register of multiple qubits \
rather than by a single qubit . Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"c"},
     "Ket"], "\[Congruent]", 
    TemplateBox[{
      RowBox[{
        SubscriptBox["c", "1"], 
        SubscriptBox["c", "2"], 
        SubscriptBox["\[CenterEllipsis]c", "n"]}]},
     "Ket"]}], TraditionalForm]],ExpressionUUID->
  "e51bf781-c078-4a99-9053-2a0b24aa5d49"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"t"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "6d7b3d5b-8b32-42e5-bb84-555bc1243191"],
 " be the computational basis states of the control register and the target \
qubit, respectively . Under the multi-control NOT gate, they transform as"
}], "TechNoteText",
 CellID->1939700080,ExpressionUUID->"182d46d0-2958-45c0-a5a6-452a85fff75c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"t"},
      "Ket"]}], "\[RightTeeArrow]", 
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["c", "1"], 
             SubscriptBox["c", "2"], 
             SubscriptBox["\[CenterEllipsis]c", "n"]}], ")"}], 
         "\[CirclePlus]", "t"}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "eaadac97-2aa3-43d3-89e1-8eb9f6ecea27"]
}], "TechNoteText",
 CellID->2119921909,ExpressionUUID->"55d28631-1af8-4410-a9ce-6593e3274824"],

