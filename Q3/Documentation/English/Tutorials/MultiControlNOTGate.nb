Cell[TextData[{
 "The multi - control NOT gate is similar to the ",
 ButtonBox["CNOT gate",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/TwoQubitGates#1585062394"],
 " on two qubits, but it is controlled by a register of multiple qubits \
rather than by a single qubit . Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"c"},
     "Ket"], "\[Congruent]", 
    TemplateBox[{
      RowBox[{
        SubscriptBox["c", "1"], 
        SubscriptBox["c", "2"], 
        SubscriptBox["\[CenterEllipsis]c", "n"]}]},
     "Ket"]}], TraditionalForm]],ExpressionUUID->
  "6c3141e7-aea1-4cba-8815-c1e613922344"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"t"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "30c69610-b56d-4f60-a692-93140cb28ff4"],
 " be the computational basis states of the control register and the target \
qubit, respectively . Under the multi-control NOT gate, they transform as"
}], "TechNoteText",
 CellID->1939700080,ExpressionUUID->"c1185bf7-da06-4be0-9170-9b6934e13f0f"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"t"},
      "Ket"]}], "\[RightTeeArrow]", 
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["c", "1"], 
             SubscriptBox["c", "2"], 
             SubscriptBox["\[CenterEllipsis]c", "n"]}], ")"}], 
         "\[CirclePlus]", "t"}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "080e1eae-8f84-4085-8b2c-857e52523638"]
}], "TechNoteText",
 CellID->2119921909,ExpressionUUID->"dfc0fe73-ee5f-44e3-9b18-0879ed359431"],

