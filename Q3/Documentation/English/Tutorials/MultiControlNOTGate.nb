Cell[TextData[{
 "The multi - control NOT gate is similar to the ",
 ButtonBox["CNOT gate",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/TwoQubitGates#1585062394"],
 " on two qubits, but it is controlled by a register of multiple qubits \
rather than by a single qubit . Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"c"},
     "Ket"], "\[Congruent]", 
    TemplateBox[{
      RowBox[{
        SubscriptBox["c", "1"], 
        SubscriptBox["c", "2"], 
        SubscriptBox["\[CenterEllipsis]c", "n"]}]},
     "Ket"]}], TraditionalForm]],ExpressionUUID->
  "b79d5c9d-f275-45db-acc0-dbfc3799eceb"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"t"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "d639affd-906a-4134-bfd1-95f6763e7b07"],
 " be the computational basis states of the control register and the target \
qubit, respectively . Under the multi-control NOT gate, they transform as"
}], "TechNoteText",
 CellID->1939700080,ExpressionUUID->"1b7ad283-1787-4476-907d-b95e24eacca0"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"t"},
      "Ket"]}], "\[RightTeeArrow]", 
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["c", "1"], 
             SubscriptBox["c", "2"], 
             SubscriptBox["\[CenterEllipsis]c", "n"]}], ")"}], 
         "\[CirclePlus]", "t"}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "b8b77d71-82d1-443e-a390-278d2391e7f8"]
}], "TechNoteText",
 CellID->2119921909,ExpressionUUID->"018ea70c-9210-4649-a2e5-32133b0b71fc"],

Cell["or  equivalently,", "TechNoteText",
