Cell[BoxData[{
 RowBox[{
  RowBox[{"tofa", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Toffoli", "[", 
      RowBox[{
       RowBox[{"S", "[", 
        RowBox[{"$n", "-", "j", "+", "1"}], "]"}], ",", 
       RowBox[{"S", "[", 
        RowBox[{"$n", "+", "$m", "-", "j", "+", "1"}], "]"}], ",", 
       RowBox[{"S", "[", 
        RowBox[{"tt", "-", "j", "+", "1"}], "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "$m"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tofb", "=", 
   RowBox[{"Toffoli", "[", 
    RowBox[{
     RowBox[{"S", "[", "1", "]"}], ",", 
     RowBox[{"S", "[", "2", "]"}], ",", 
     RowBox[{"S", "[", 
      RowBox[{"$n", "+", "1"}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tofc", "=", 
   RowBox[{"Rest", "@", "tofa"}]}], ";"}]}], "Input",
