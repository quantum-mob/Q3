Cell[TextData[{
 "The multi - control NOT gate is similar to the ",
 ButtonBox["CNOT gate",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/TwoQubitGates#1585062394"],
 " on two qubits, but it is controlled by a register of multiple qubits \
rather than by a single qubit . Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"c"},
     "Ket"], "\[Congruent]", 
    TemplateBox[{
      RowBox[{
        SubscriptBox["c", "1"], 
        SubscriptBox["c", "2"], 
        SubscriptBox["\[CenterEllipsis]c", "n"]}]},
     "Ket"]}], TraditionalForm]],ExpressionUUID->
  "12ec5832-f6b8-4859-a843-9e4426b1e617"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"t"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "701175ff-ff33-44d5-85f9-a73883a41529"],
 " be the computational basis states of the control register and the target \
qubit, respectively . Under the multi-control NOT gate, they transform as"
}], "TechNoteText",
 CellID->1939700080,ExpressionUUID->"6a005388-d6cd-47da-85a9-309a13c5f923"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"t"},
      "Ket"]}], "\[RightTeeArrow]", 
    RowBox[{
     TemplateBox[{"c"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             SubscriptBox["c", "1"], 
             SubscriptBox["c", "2"], 
             SubscriptBox["\[CenterEllipsis]c", "n"]}], ")"}], 
         "\[CirclePlus]", "t"}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "73d39a15-8c95-42db-888b-20823d519369"]
}], "TechNoteText",
 CellID->2119921909,ExpressionUUID->"42b91da3-8a23-4b51-ad22-65a0ea437df2"],

