  FormBox[
   TemplateBox[{
     SuperscriptBox["0", 
      RowBox[{"\[CircleTimes]", "m"}]]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "2a17ecac-1fa6-49d7-afbc-e349b0f4c1c2"],
