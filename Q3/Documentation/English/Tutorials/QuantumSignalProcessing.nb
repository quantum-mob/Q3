Cell[TextData[{
 "Note that the target polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "target"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"ce3a14ff-4148-48d6-b485-434ebfd67459"],
 " is specified in terms of the ",
 ButtonBox["Chebyshev coefficients",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/ChebyshevCoefficients"],
 "; that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "target"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"b7056705-4f36-4de6-94b5-21b51e79ddc3"],
 " is assumed to be expanded in the ",
 ButtonBox["Chebyshev series",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/ChebyshevSeries"],
 ". This is natural because\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"[", 
      RowBox[{"W", "(", "x", ")"}], "]"}], "k"], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["T", "k"], "(", "x", ")"}], 
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"]}]], 
         RowBox[{
          SubscriptBox["U", 
           RowBox[{"k", "-", "1"}]], "(", "x", ")"}]}]},
       {
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"]}]], 
         RowBox[{
          SubscriptBox["U", 
           RowBox[{"k", "-", "1"}]], "(", "x", ")"}]}], 
        RowBox[{
         SubscriptBox["T", "k"], "(", "x", ")"}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "073ef32b-7e83-43de-ba06-a8c27e00d377"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["T", "k"], "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "8c276138-c4ee-4986-9166-1d6a1fff34e6"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "k"], "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "d2a85d12-8521-4254-bace-eec1d9bf51d0"],
 " are the Chebyshev polynomials of the first and second kinds, respectively,\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["T", "k"], "(", "x", ")"}], ":=", 
    RowBox[{"cos", "(", 
     RowBox[{"k", "\[VeryThinSpace]", "\[Theta]"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"248f1f89-dedc-4674-9448-33177908da66"],
 ",\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "k"], "(", "x", ")"}], ":=", 
    RowBox[{
     RowBox[{"sin", "(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "+", "1"}], ")"}], "\[Theta]"}], ")"}], "/", 
     RowBox[{"sin", "(", "\[Theta]", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"9f1f4940-19d4-49d6-9ef8-9d8bb8a67e56"],
 "\[LineSeparator]with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"cos", "(", "\[Theta]", ")"}], ":=", "x"}], TraditionalForm]],
  ExpressionUUID->"e3d4c8ce-a12f-4294-b0f1-258fe17f677e"],
 "."
}], "TechNoteText",
 CellID->44302011,ExpressionUUID->"fc7ad702-a7d1-48a4-a974-a046765e76f4"]
}, Open  ]],

