Cell[TextData[{
 "The pair ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["\[CurlyPhi]", "0"], ",", 
       SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["\[CurlyPhi]", 
        RowBox[{
         SubscriptBox["n", "d"], "-", "1"}]]}], "}"}], ",", 
