 CellID->1135653224,ExpressionUUID->"0276891f-cff2-4f81-ae8f-c9f449e96338"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalF]", "(", "\[Rho]", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["E", "0"], "\[Rho]", "\[VeryThinSpace]", 
      SubsuperscriptBox["E", "0", "\[Dagger]"]}], "+", 
     RowBox[{
      SubscriptBox["E", "1"], "\[Rho]", "\[VeryThinSpace]", 
      SubsuperscriptBox["E", "1", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"f5f93f8a-8823-47df-b95a-808ef71122d1"],
 " ."
}], "TechNoteText",
 CellID->89081232,ExpressionUUID->"1605d48c-86dd-4ac5-8d8c-94100511ba0e"],

Cell["Note  that  they  are  not  orthogonal,", "TechNoteText",
