Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["Tr", "\[ScriptCapitalE]"], " ", "[", 
     RowBox[{
      RowBox[{"U", "(", 
       RowBox[{
        TemplateBox[{"\[Psi]"},
         "Ket"], 
        RowBox[{
         TemplateBox[{"\[Psi]"},
          "Bra"], "\[CircleTimes]", 
         TemplateBox[{
           SubscriptBox["\[Epsilon]", "0"]},
          "Ket"]}], 
        TemplateBox[{
          SubscriptBox["\[Epsilon]", "0"]},
         "Bra"]}], ")"}], 
      SuperscriptBox["U", "\[Dagger]"]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], 
      RowBox[{
       UnderscriptBox["Tr", "\[ScriptCapitalE]"], "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["F", "\[Mu]"], 
           TemplateBox[{"\[Psi]"},
            "Ket"], 
           TemplateBox[{"\[Psi]"},
            "Bra"], 
           SubsuperscriptBox["F", "\[Nu]", "\[Dagger]"]}], ")"}], "\[CircleTimes]", 
         TemplateBox[{
           SubscriptBox["\[Epsilon]", "\[Mu]"]},
          "Ket"]}], 
        TemplateBox[{
          SubscriptBox["\[Epsilon]", "\[Nu]"]},
         "Bra"]}], "]"}]}], "=", 
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "\[Mu]"], 
      SubscriptBox["F", "\[Mu]"], 
      TemplateBox[{"\[Psi]"},
       "Ket"], 
      TemplateBox[{"\[Psi]"},
       "Bra"], 
      SubsuperscriptBox["F", "\[Nu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"aa49ed51-cdbd-48d5-a958-e1ef17c96218"],
 " ."
}], "TechNoteText",
 CellID->785717990,ExpressionUUID->"dbfa2e9a-054b-410a-8f09-ee492bcd94bb"],

Cell[TextData[{
 "This relation is linear and holds for an arbitrary vector ",
