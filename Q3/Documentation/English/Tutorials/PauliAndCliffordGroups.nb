Cell[TextData[{
 "\tIgnoring the phase factors leads to a drastically simple structure of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"98e53b17-90e0-4890-9619-d826d8562e05"],
 ", compared to the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"fecfbe9d-b4ed-4d8e-8c06-e6ad7b9966c2"],
 ". For example, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", " ", "Y"}], TraditionalForm]],ExpressionUUID->
  "a1517533-852f-4b93-b9b8-e206d9f383c3"],
 " is now equal to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", " ", "X"}], TraditionalForm]],ExpressionUUID->
  "0a9e7f2c-ccbf-44a2-9eaf-60acf00eb15f"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", "X\[ScriptCapitalZ]", ")"}], 
     RowBox[{"(", "Y\[ScriptCapitalZ]", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"X", " ", "Y"}], ")"}], "\[ScriptCapitalZ]"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Y", " ", "X"}], ")"}], "\[ScriptCapitalZ]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"90075010-23e2-41ae-abf6-d29bb348282f"],
 ". This means that unlike the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"cfd48343-2b08-441d-8dfb-597826bcf821"],
 ", the ",
 ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/PauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"81ae0143-8ae4-4eb0-856f-1a52699bd902"],
 " is ",
 StyleBox["Abelian",
  FontSlant->"Italic"],
 "."
}], "TechNoteText",
 CellID->360346922,ExpressionUUID->"049fc1d1-78ea-4bae-9a1a-afc87f0ce292"],

