Cell[TextData[{
 "\tIgnoring the phase factors leads to a drastically simple structure of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"936e111d-97f4-4a33-be5a-0ed399dcbbef"],
 ", compared to the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"8924ed83-565b-4503-a52e-469ddef685b6"],
 ". For example, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", " ", "Y"}], TraditionalForm]],ExpressionUUID->
  "824ca853-cd82-4316-b863-dd2f69371c42"],
 " is now equal to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", " ", "X"}], TraditionalForm]],ExpressionUUID->
  "1d93175e-9e03-4669-a814-2de31fe34a63"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", "X\[ScriptCapitalZ]", ")"}], 
     RowBox[{"(", "Y\[ScriptCapitalZ]", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"X", " ", "Y"}], ")"}], "\[ScriptCapitalZ]"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Y", " ", "X"}], ")"}], "\[ScriptCapitalZ]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"ef611fcc-9328-42c3-ba83-7976d28c9a02"],
 ". This means that unlike the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"65bf7c5a-a8b8-4bf4-8e91-b3a0ae25704b"],
 ", the ",
 ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/PauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"febcb462-12bb-4d8c-893b-b8ea8790f84e"],
 " is ",
 StyleBox["Abelian",
  FontSlant->"Italic"],
 "."
}], "TechNoteText",
 CellID->360346922,ExpressionUUID->"3081b24a-7e5a-4eb7-a2c1-f50ab4f210ea"],

