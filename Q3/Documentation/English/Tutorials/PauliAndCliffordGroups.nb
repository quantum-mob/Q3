Cell[TextData[{
 "Every element of the Pauli group squares to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "I"}], TraditionalForm]],ExpressionUUID->
  "4ec4d147-5b9d-4d69-8243-90defb05df31"],
 " . The elements that square to ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],ExpressionUUID->
  "b992aaee-c9db-47ba-a10e-dcad05f1395f"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "1"}], TraditionalForm]],ExpressionUUID->
  "ac78a42c-4747-49dd-a1f0-74fec46682fd"],
 " whereas those that square to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Minus]", "I"}], TraditionalForm]],ExpressionUUID->
  "51b16525-d6da-470b-8bf8-f0dce60aa202"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], TraditionalForm]],
  ExpressionUUID->"1a8c1c79-1592-4507-b884-f54e7ad08366"],
 "."
}], "Item",
 CellID->490769776,ExpressionUUID->"44269d1a-1464-4aed-bdb9-18f5d185527e"],

