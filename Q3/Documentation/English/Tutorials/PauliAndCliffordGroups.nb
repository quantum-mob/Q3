     SubsuperscriptBox["\[Product]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        RowBox[{"i", " ", "j"}]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        RowBox[{"i", " ", "j"}]]]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"a428d152-96d6-4f61-9457-e47c0b920604"]], \
"DisplayMath",
