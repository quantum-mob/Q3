Cell[TextData[{
 "with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "j"], ",", " ", 
    SubscriptBox["q", "j"], ",", " ", 
    SubscriptBox["a", 
     RowBox[{"i", " ", "j"}]], ",", " ", 
