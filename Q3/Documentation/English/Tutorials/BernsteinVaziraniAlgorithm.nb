Cell[TextData[{
 "Suppose that we are given a binary function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"], "\[RightArrow]", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}]}], TraditionalForm]],ExpressionUUID->
  "6017e571-7a6b-4d24-ba89-7d6417690330"],
 ", the value of which is determined by a secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "18090486-973d-4638-8dc3-15cb9eae4edc"],
 " of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "1e583994-1da9-4cb8-9b2c-878fc329083e"],
 " bits"
}], "TechNoteText",
 CellID->756185225,ExpressionUUID->"a93333b9-b616-4df8-845b-06bb7facaa8f"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{"x", "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"x", "\[CenterDot]", "s"}], "   ", 
      RowBox[{"(", 
       RowBox[{"mod", " ", "2"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"68605456-39ee-4412-a333-3b57bf3f75af"],
 "."
}], "DisplayMath",
 CellID->396853629,ExpressionUUID->"5b62f8b3-8acd-4649-a658-56ed48bb8fd8"],

Cell[TextData[{
 "The Bernstein-Vazirani problem finds the secret bit string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "e2ff06fb-672d-4c2f-af8e-0f28a98b7a77"],
 " by making queries to the oracle ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "c5cc9669-538b-430d-8703-6126aab9916c"],
 ". Classically, one needs ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "acd37773-b0ba-4358-a2d0-b90728f63773"],
 " queries to the function f to infer the secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "f57d3f7e-462e-4d44-a2d5-6c2a651e5208"],
 "."
}], "TechNoteText",
 CellID->1982218668,ExpressionUUID->"c2180209-43dc-42b6-abfd-011dda9a2ed9"],

