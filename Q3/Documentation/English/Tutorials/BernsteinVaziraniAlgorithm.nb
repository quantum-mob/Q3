Cell[TextData[{
 "Suppose that we are given a binary function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"], "\[RightArrow]", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}]}], TraditionalForm]],ExpressionUUID->
  "81adddca-293f-4275-a351-49dd11159934"],
 ", the value of which is determined by a secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "913c2b7a-3be9-49e9-b8dc-c2c015348bd7"],
 " of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "4b23b247-f74b-4703-b7ed-b178db954860"],
 " bits"
}], "TechNoteText",
 CellID->756185225,ExpressionUUID->"024aa9a2-42aa-406e-afac-65e4e63ccc03"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{"x", "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"x", "\[CenterDot]", "s"}], "   ", 
      RowBox[{"(", 
       RowBox[{"mod", " ", "2"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"a341b87d-0474-4192-8b7d-2e116f4977e9"],
 "."
}], "DisplayMath",
 CellID->396853629,ExpressionUUID->"56446d11-06a9-4494-af5b-54dd713f50ad"],

Cell[TextData[{
 "The Bernstein-Vazirani problem finds the secret bit string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "dd2ff44e-8f53-4a1f-a985-7e69a545e257"],
 " by making queries to the oracle ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "d793befc-c8a9-4ddf-bd44-e72568344aff"],
 ". Classically, one needs ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "411a7185-cbc5-4cc6-beb6-ad5e1d28c20d"],
 " queries to the function f to infer the secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "8a326a97-7642-42ff-a129-24f272dc0c08"],
 "."
}], "TechNoteText",
