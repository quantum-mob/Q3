   RowBox[{"\[ScriptCapitalV]", "\[CircleTimes]", "\[ScriptCapitalW]"}], 
   TraditionalForm]],ExpressionUUID->"1dfa1dc2-cd24-486b-8e8f-8b8e7b9d3305"],
