  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"I", "+", 
       RowBox[{"x", " ", 
        SuperscriptBox["\[Sigma]", "x"]}], "+", 
       RowBox[{"y", " ", 
        SuperscriptBox["\[Sigma]", "y"]}], "+", 
       RowBox[{"z", " ", 
