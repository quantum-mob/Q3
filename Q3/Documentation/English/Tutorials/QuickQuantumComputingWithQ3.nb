  FormBox[
   RowBox[{"U", "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{
       SubscriptBox["\[Mu]", "1"], "=", "0"}], "4"], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{
        SubscriptBox["\[Mu]", "2"], "=", "0"}], "4"], 
      RowBox[{"\[CenterEllipsis]", 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{
          SubscriptBox["\[Mu]", "n"], "=", "0"}], "4"], 
        RowBox[{
         RowBox[{
          SuperscriptBox["S", 
           SubscriptBox["\[Mu]", "1"]], "\[CircleTimes]", 
          SuperscriptBox["S", 
           SubscriptBox["\[Mu]", "2"]], "\[CircleTimes]", "\[Ellipsis]", "\[CircleTimes]", 
          SuperscriptBox["S", 
           SubscriptBox["\[Mu]", "n"]]}], 
         SubscriptBox["c", 
          RowBox[{
           RowBox[{
            RowBox[{
             SubscriptBox["\[Mu]", "1"], 
             SubscriptBox["\[Mu]", "2"]}], "..."}], 
           SubscriptBox["\[Mu]", "n"]}]]}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"7fff6d14-3f18-4334-a750-33098f8287a8"],
 ","
