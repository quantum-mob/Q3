      RowBox[{"\[Rho]", "(", "0", ")"}], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"+", "\[ImaginaryI]"}], "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
        SubsuperscriptBox["H", "non", "\[Dagger]"]}]]}], "]"}], "=", 
    RowBox[{
     RowBox[{"Tr", "[", 
      RowBox[{
       RowBox[{"\[Rho]", "(", "0", ")"}], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "G"}], "+", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
          "H"}]}]], 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "G"}], "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
          "H"}]}]]}], "]"}], "<", "1"}]}], TraditionalForm]],ExpressionUUID->
  "a6874959-c47a-460a-a61a-91d3a5b2159d"],
 " ,"
}], "TechNoteText",
