Cell[TextData[{
 "We have ignored the term of the second and higher orders in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"7de6283c-d366-46c6-9073-508e466bf44e"],
 "."
}], "TechNoteText",
 CellID->853409526,ExpressionUUID->"b435d6dd-a6a4-4635-acbd-dc0f2d0c4c2f"],

Cell[TextData[{
 "\tConceptually, the key observation is that each term in ",
 ButtonBox["Equation (19)",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/QuantumMasterEquation#984363513"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", "non"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
    RowBox[{"\[Rho]", "(", "t", ")"}], 
    RowBox[{
     SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"95e74b18-2706-4313-a9fc-c5c7f620a4e6"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", " ", 
    SubscriptBox["L", "\[Mu]"], 
    RowBox[{"\[Rho]", "(", "t", ")"}], 
    SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}], TraditionalForm]],
  ExpressionUUID->"0461108d-1a0e-4b95-b1b6-dda68b4dab35"],
 ", represents a possible quantum state that is determined randomly with \
probability \t"
}], "TechNoteText",
 CellID->210625169,ExpressionUUID->"4f8ae520-e79e-4cd6-9242-99ceca213399"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "0"], ":=", 
    RowBox[{"Tr", "[", 
     RowBox[{
      RowBox[{
       SubscriptBox["U", "non"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      RowBox[{
       SubsuperscriptBox["U", "non", "\[Dagger]"], "(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "]"}]}], 
   TraditionalForm]],ExpressionUUID->"649ade6f-a2b3-44e6-a784-eb04040c21d3"]
}], "TechNoteText",
