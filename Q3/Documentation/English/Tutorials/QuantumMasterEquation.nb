Cell["or", "TechNoteText",
 CellID->103624565,ExpressionUUID->"fe3b1e9f-5728-484a-a50e-199175bb1fc4"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "\[Mu]"], ":=", 
    RowBox[{"Tr", "[", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", " ", 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}], "]"}]}], 
