Cell[TextData[{
 "Within the Markov approximation, the density operator ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "(", "t", ")"}], TraditionalForm]],ExpressionUUID->
  "0336715e-14f4-41e2-a11e-547d8c9ea918"],
 " satisfies a differential equation of the form,"
}], "TechNoteText",
 CellID->1322257763,ExpressionUUID->"3329f933-3f39-4732-843e-8705c95be767"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d\[Rho]", 
     RowBox[{"d", "\[VeryThinSpace]", "t"}]], "=", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[Rho]", ")"}]}], TraditionalForm]],
  ExpressionUUID->"3cf3e1ba-9a9c-47c7-b7e9-e15fdd5da26f"],
 ","
}], "TechNoteText",
 CellFrameLabels->{{None, 
