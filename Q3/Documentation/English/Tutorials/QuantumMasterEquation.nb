Cell[TextData[{
 "This means that to generate a trajectory of quantum state in the Hilbert \
space, we may just follow the spirit of Monte Carlo method and randomly apply \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "\[VeryThinSpace]", 
     SubscriptBox["F", "0"]}], ":=", 
    RowBox[{
     SubscriptBox["U", "non"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"39575484-5d2d-4f0f-a86b-9ecc03f6451a"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "\[VeryThinSpace]", 
     SubscriptBox["F", "\[Mu]"]}], ":=", 
    RowBox[{
     SqrtBox[
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]], 
     SubscriptBox["L", "\[Mu]"]}]}], TraditionalForm]],ExpressionUUID->
  "03c9ac77-47fe-448c-8f14-ae54364e8633"],
 " with aforementioned probability at each time step ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", "k"], ":=", 
    RowBox[{"k", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", "t"}]}], 
   TraditionalForm]],ExpressionUUID->"55c404f7-c596-4327-a1d8-126f3d36c665"],
 ". For example, starting with an initial pure state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"in"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "c6b120ae-3bec-45a8-94ad-75c76577c75a"],
 ","
}], "TechNoteText",
 CellID->1690749513,ExpressionUUID->"5d1aeb30-3e1d-447e-90b9-bda38e126fe9"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "0"], ")"}]},
      "Ket"], "=", 
     TemplateBox[{"in"},
      "Ket"]}], ",", "\[LineSeparator]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "1"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "2"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "3"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "4"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "1"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", "\[Ellipsis]"}], 
   TraditionalForm]],ExpressionUUID->"cc44f130-6f31-4d74-92d0-e0baf15653cd"]
}], "TechNoteText",
 CellFrameLabels->{{None, 
