Cell[TextData[{
 "where the additional factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "\[ImaginaryI]"}], TraditionalForm]],ExpressionUUID->
  "4b50fa7c-211b-4372-9005-5011fbf7b90e"],
 " is completely arbitrary at this stage but is introduced here for later \
convenience. The rest should vanish ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], "\[RightArrow]", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "d344e42f-3172-408b-a820-d2720f4ee8f3"],
 " for any ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "e3a28ab7-c6b4-45e7-88ae-e9e064882d7a"],
 ". Since we physically expect that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "'"}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{"\[Rho]", "(", "t", ")"}], "+", 
     RowBox[{"\[ScriptCapitalO]", "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"0110e0e1-28ea-478f-a257-2d14b54aab48"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "\[Mu]"], "(", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], TraditionalForm]],
  ExpressionUUID->"f51e03f3-6512-4b38-aaec-409ecec90d7f"],
 " must vanish like ",
 Cell[BoxData[
  FormBox[
   SqrtBox[
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]], TraditionalForm]],
  ExpressionUUID->"43f19378-6d37-4430-9a69-f8033783c157"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"a7c6a84a-43c3-4c63-b165-63df447f63ea"],
 " so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["F", "\[Mu]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
     RowBox[{"\[Rho]", "(", "t", ")"}], 
     RowBox[{
      SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "\[TildeTilde]", 
    RowBox[{"\[ScriptCapitalO]", "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"9f04ab77-b8f6-40e0-a6c3-d34ccd2b8624"],
 ". We put"
}], "TechNoteText",
 CellID->719905097,ExpressionUUID->"98d08130-75ce-42dd-a635-8a4739ed4849"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], "\[TildeTilde]", 
