 CounterIncrements->"DisplayFormulaNumbered",
 CellID->1204485777,ExpressionUUID->"f387371e-f4b4-42e7-bafa-738119dd3b6a"],

Cell[TextData[{
 "This means that to generate a trajectory of quantum state in the Hilbert \
space, we may just follow the spirit of Monte Carlo method and randomly apply \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "\[VeryThinSpace]", 
     SubscriptBox["F", "0"]}], ":=", 
    RowBox[{
     SubscriptBox["U", "non"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"bf7f4bd4-a2ce-4a6f-b1a9-8f2f848ab395"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "\[VeryThinSpace]", 
     SubscriptBox["F", "\[Mu]"]}], ":=", 
    RowBox[{
     SqrtBox[
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]], 
     SubscriptBox["L", "\[Mu]"]}]}], TraditionalForm]],ExpressionUUID->
  "739a607f-4980-4d12-9335-518fa1c747f3"],
 " with aforementioned probability at each time step ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", "k"], ":=", 
    RowBox[{"k", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", "t"}]}], 
   TraditionalForm]],ExpressionUUID->"5e0f3d6c-fd5b-4203-b124-773420e22a44"],
 ". For example, starting with an initial pure state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"in"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "19c1ce23-a59d-4709-93fd-c63fcfedd025"],
 ","
}], "TechNoteText",
 CellID->1690749513,ExpressionUUID->"84a5cc02-df33-408e-ae51-fd259fbb834c"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "0"], ")"}]},
      "Ket"], "=", 
     TemplateBox[{"in"},
      "Ket"]}], ",", "\[LineSeparator]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "1"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "2"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "3"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     TemplateBox[{
       RowBox[{"\[Psi]", "(", 
         SubscriptBox["t", "4"], ")"}]},
      "Ket"], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "1"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "2"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[Delta]", "\[VeryThinSpace]", 
        SubscriptBox["F", "0"]}], ")"}], 
      TemplateBox[{"in"},
       "Ket"]}]}], ",", "\[IndentingNewLine]", "\[Ellipsis]"}], 
   TraditionalForm]],ExpressionUUID->"f6311329-24db-448c-9b18-16626fe0e6f0"]
}], "TechNoteText",
 CellFrameLabels->{{None, 
    Cell[
     TextData[{"(", 
