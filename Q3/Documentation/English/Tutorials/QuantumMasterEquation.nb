Cell[TextData[{
 "which resembles the unitary dynamics with Hamiltonian ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "8658b9a4-3f04-4b9e-aeeb-ee6e78d4a56d"],
 " replaced with the effective non-Hermitian Hamiltonian "
}], "TechNoteText",
 CellID->421227385,ExpressionUUID->"37f3ba36-62be-4874-9bbc-df7c05e5bc7e"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "non"], ":=", 
    RowBox[{"H", "-", 
     RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "G"}]}]}], 
