Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["L", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "51c7d676-9591-484b-a7ce-48d6e041f92f"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "563922d9-4b21-4488-ad8b-455cdbc8f220"],
 ") directly proportional to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "bad6e714-0569-49cf-b284-bc17bc79faf1"],
 ", they are all traceless and mutually orthogonal; i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "[", 
     RowBox[{
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"], 
      SubscriptBox["L", "\[Mu]"]}], "]"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"02e42da7-fcb4-498f-b570-f3cb056bdcdc"],
 " for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "\[Nu]"}], TraditionalForm]],
  ExpressionUUID->"34d968c0-fafe-4438-ad7d-f48c266dce29"],
 ". Putting these approximations back into the equation for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "(", 
    RowBox[{"t", "'"}], ")"}], TraditionalForm]],ExpressionUUID->
  "173eadcb-4aa2-4fa1-8f32-00b0ee0ea2a4"],
 " at the beginning of this section leads to"
}], "TechNoteText",
 CellID->284265725,ExpressionUUID->"792ed1e0-a7ed-473c-9649-b67e807cb2eb"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{"\[Rho]", "(", "t", ")"}], "-", 
     RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
      RowBox[{"t", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["L", "0"], 
         RowBox[{"\[Rho]", "(", "t", ")"}]}], "-", 
        RowBox[{
         RowBox[{"\[Rho]", "(", "t", ")"}], 
         SubsuperscriptBox["L", "0", "\[Dagger]"]}]}], "]"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
