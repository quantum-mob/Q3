Cell[TextData[{
 "\tAt this stage, the vectors in the Gelfand-Zetlin basis do not take any \
particular form of expression. All we require is that under the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", "\[Element]", 
    SubscriptBox["\[ScriptCapitalU]", "d"]}], TraditionalForm]],
  ExpressionUUID->"c551b281-a4a4-4a38-b7d3-063e1ecd1b3e"],
 ", the basis vectors transform as"
}], "Text",
 CellID->1427798485,ExpressionUUID->"c64d8bc1-aa31-4aff-97d1-ed50a6abd307"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox[
       OverscriptBox["\[CapitalOmega]", "^"], "\[Lambda]"], "(", "U", ")"}], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         SubscriptBox[
          RowBox[{"(", 
            SubscriptBox["w", "\[Lambda]"], ")"}], 
          RowBox[{"d", "-", "1"}]]}]},
      "Ket"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "v"], 
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Lambda]", ",", 
          SubscriptBox[
           RowBox[{"(", 
             SubscriptBox["v", "\[Lambda]"], ")"}], 
           RowBox[{"d", "-", "1"}]]}]},
       "Ket"], 
      RowBox[{
       SubsuperscriptBox["\[CapitalOmega]", 
        StyleBox["vw",
         FontSlant->"Italic"], "\[Lambda]"], "(", "U", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"52bf245d-e06e-4b68-8d7c-0cac17005554"],
 ","
}], "DisplayMath",
 CellID->24250654,ExpressionUUID->"4a4a0efa-357f-46bf-8eb0-04f24974c403"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[CapitalOmega]", 
     StyleBox["vw",
      FontSlant->"Italic"], "\[Lambda]"], "(", "U", ")"}], TraditionalForm]],
  ExpressionUUID->"4f96e49e-c4c1-41bd-be3d-cdf294630bcd"],
 " is the ",
 Cell[BoxData[
  FormBox[
