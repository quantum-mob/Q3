Cell[TextData[{
 "\tAt the moment, the vectors in the Young-Yamanouchi basis do not take any \
particular form of expression. All we require is that under the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"e7a6257b-f7ad-4073-a41f-40c60b46874e"],
 ", the basis vectors transform as"
}], "TechNoteText",
 CellID->1155245835,ExpressionUUID->"17767a3e-4ac1-42b6-b7f7-aa39a9391dff"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["\[CapitalPi]", "^"], "(", "S", ")"}], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         SubscriptBox[
          RowBox[{"(", 
            SubscriptBox["y", "\[Lambda]"], ")"}], 
          RowBox[{"n", "-", "1"}]]}]},
      "Ket"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "x"], 
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Lambda]", ",", 
          SubscriptBox[
           RowBox[{"(", 
             SubscriptBox["x", "\[Lambda]"], ")"}], 
           RowBox[{"n", "-", "1"}]]}]},
       "Ket"], 
      RowBox[{
       SubsuperscriptBox["\[CapitalPi]", 
        RowBox[{"x", " ", "y"}], "\[Lambda]"], "(", "S", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"4da17b03-528b-4af8-af2b-0dd144c5319c"],
 ","
}], "DisplayMath",
 CellID->1800144916,ExpressionUUID->"8ebe5016-fa40-4c27-9de8-28e2679e176b"],

