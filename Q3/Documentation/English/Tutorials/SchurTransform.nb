 Cell[BoxData[
  RowBox[{"\[Lambda]", "=", 
   RowBox[{"(", 
    RowBox[{"2", ",", "1"}], ")"}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "b4d41ce9-5f3e-4152-a87a-c20c948a4c65"],
 "."
}], "Text",
