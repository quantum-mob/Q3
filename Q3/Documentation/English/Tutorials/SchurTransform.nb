 "."
}], "DisplayMath",
 CellID->236677184,ExpressionUUID->"108dfb81-ba61-466d-b641-3c066e682560"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["w", "\[Lambda]"], ")"}], 
    RowBox[{"n", "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "f2941516-5159-40b6-8ebf-23151effe987"],
