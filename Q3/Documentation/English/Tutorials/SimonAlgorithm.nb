Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[CenterDot]", "s"}], TraditionalForm]],ExpressionUUID->
  "d96ccfdb-b987-432a-9a70-72e622b06b05"],
 " is odd, that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "\[CenterDot]", "s"}], "=", 
    RowBox[{"1", " ", 
     RowBox[{"(", 
      RowBox[{"mod", " ", "2"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "bcf2f876-9ca2-48b9-a678-2e48776f82ac"],
 ", then ",
