  RowBox[{
   RowBox[{"\[ScriptCapitalZ]", 
    RowBox[{"(", "g", ")"}]}], "=", 
   RowBox[{"|", "\[ScriptCapitalG]", "|", 
    RowBox[{"/", 
     RowBox[{"|", 
      RowBox[{"\[ScriptCapitalC]", 
       RowBox[{"(", "g", ")"}]}], "|"}]}]}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "e3577229-7a7b-483d-97e3-37bc55ea038d"],
