  RowBox[{
   RowBox[{"\[ScriptCapitalZ]", 
    RowBox[{"(", "g", ")"}]}], "=", 
   RowBox[{"|", "\[ScriptCapitalG]", "|", 
    RowBox[{"/", 
     RowBox[{"|", 
      RowBox[{"\[ScriptCapitalC]", 
       RowBox[{"(", "g", ")"}]}], "|"}]}]}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "74270003-e556-4249-9c78-9d3f52626d46"],
