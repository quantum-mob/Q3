Cell[TextData[{
 "For a Gelfand pattern,\[LineSeparator]    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", "m", ")"}], "d"], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["m", 
         RowBox[{"d", "\[InvisibleSpace]", "1"}]], 
        SubscriptBox["m", 
         RowBox[{"d", "\[InvisibleSpace]", "2"}]], "\[Ellipsis]", 
        SubscriptBox["m", 
         RowBox[{"d", "\[VeryThinSpace]", "d"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[AscendingEllipsis]", 
        " "},
       {
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "1"}]], 
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "2"}]], " ", " "},
       {
        SubscriptBox["m", 
         RowBox[{"1", "\[InvisibleSpace]", "1"}]], " ", " ", " "}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "d32efea3-bd16-4957-80d1-dddda40ed606"],
 ",\[LineSeparator]the partial hooks are given by ",
 Cell[BoxData[
  StyleBox[
   RowBox[{
    SubscriptBox["p", "ij"], "=", 
    RowBox[{
     SubscriptBox["m", "ij"], "+", 
     RowBox[{"(", 
      RowBox[{"i", "-", "j"}], ")"}]}]}],
   FontSlant->"Italic"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "9ad537c0-0f98-4052-a6b5-78bccd50f30b"],
 "."
}], "Notes",
 CellID->1638969956,ExpressionUUID->"ff3c9f65-1441-40c3-8070-cb3cb3d8c988"],

Cell[TextData[{
 "Note that the indices of entries are reversed compared with the convention \
of ",
 ButtonBox["Louck (2008)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://doi.org/10.1142/6863"], None}],
 " or  ",
 ButtonBox["Biedenharn and Louck (1968)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://projecteuclid.org/journals/communications-in-mathematical-\
physics/volume-8/issue-2/A-pattern-calculus-for-tensor-operators-in-the-\
unitary-groups/cmp/1103840555.full"], None}],
 ". There, the entries are referred to as\[LineSeparator]    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", "m", ")"}], "d"], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["m", 
         RowBox[{"1", "d"}]], 
        SubscriptBox["m", 
         RowBox[{"2", "d"}]], "\[Ellipsis]", 
        SubscriptBox["m", 
         RowBox[{"d", "\[VeryThinSpace]", "d"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[AscendingEllipsis]", 
        " "},
       {
        SubscriptBox["m", "12"], 
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "2"}]], " ", " "},
       {
        SubscriptBox["m", 
         RowBox[{"1", "\[InvisibleSpace]", "1"}]], " ", " ", " "}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "f60d0bf2-d682-4110-9f1f-129a7509c2cc"],
 ".\[LineSeparator]In this case, the partial hooks are given by ",
 Cell[BoxData[
  StyleBox[
   RowBox[{
    SubscriptBox["p", "ij"], "=", 
    RowBox[{
     SubscriptBox["m", "ij"], "+", 
     RowBox[{"(", 
      RowBox[{"j", "-", "i"}], ")"}]}]}],
   FontSlant->"Italic"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "bd2c40f0-bbae-4092-a85e-504128dab84e"],
 "."
}], "Notes",
