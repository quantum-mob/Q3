Cell[TextData[{
 "For a Gelfand pattern,\[LineSeparator]    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", "m", ")"}], "d"], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["m", 
         RowBox[{"d", "\[InvisibleSpace]", "1"}]], 
        SubscriptBox["m", 
         RowBox[{"d", "\[InvisibleSpace]", "2"}]], "\[Ellipsis]", 
        SubscriptBox["m", 
         RowBox[{"d", "\[VeryThinSpace]", "d"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[AscendingEllipsis]", 
        " "},
       {
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "1"}]], 
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "2"}]], " ", " "},
       {
        SubscriptBox["m", 
         RowBox[{"1", "\[InvisibleSpace]", "1"}]], " ", " ", " "}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "37300df1-eadd-4e2b-b5a1-2e3de7d97126"],
 ",\[LineSeparator]the partial hooks are given by ",
 Cell[BoxData[
  StyleBox[
   RowBox[{
    SubscriptBox["p", "ij"], "=", 
    RowBox[{
     SubscriptBox["m", "ij"], "+", 
     RowBox[{"(", 
      RowBox[{"i", "-", "j"}], ")"}]}]}],
   FontSlant->"Italic"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "633ad773-6417-420b-9d12-bd030eda256f"],
 "."
}], "Notes",
 CellID->1638969956,ExpressionUUID->"4627e549-51eb-4435-97ac-b49e9b973084"],

Cell[TextData[{
 "Note that the indices of entries are reversed compared with the convention \
of ",
 ButtonBox["Louck (2008)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://doi.org/10.1142/6863"], None}],
 " or  ",
 ButtonBox["Biedenharn and Louck (1968)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://projecteuclid.org/journals/communications-in-mathematical-\
physics/volume-8/issue-2/A-pattern-calculus-for-tensor-operators-in-the-\
unitary-groups/cmp/1103840555.full"], None}],
 ". There, the entries are referred to as\[LineSeparator]    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", "m", ")"}], "d"], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["m", 
         RowBox[{"1", "d"}]], 
        SubscriptBox["m", 
         RowBox[{"2", "d"}]], "\[Ellipsis]", 
        SubscriptBox["m", 
         RowBox[{"d", "\[VeryThinSpace]", "d"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[AscendingEllipsis]", 
        " "},
       {
        SubscriptBox["m", "12"], 
        SubscriptBox["m", 
         RowBox[{"2", "\[InvisibleSpace]", "2"}]], " ", " "},
       {
        SubscriptBox["m", 
         RowBox[{"1", "\[InvisibleSpace]", "1"}]], " ", " ", " "}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "fe1cd9ae-817d-4c52-90d3-5736bf39a591"],
 ".\[LineSeparator]In this case, the partial hooks are given by ",
 Cell[BoxData[
  StyleBox[
   RowBox[{
    SubscriptBox["p", "ij"], "=", 
    RowBox[{
     SubscriptBox["m", "ij"], "+", 
     RowBox[{"(", 
      RowBox[{"j", "-", "i"}], ")"}]}]}],
   FontSlant->"Italic"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "be4d9ee4-866b-485e-b4db-b9dc2a0b7a67"],
 "."
}], "Notes",
