Cell[TextData[{
 "A Bogoliubov-de Gennes transformation is defined by (notice the factor of \
1/2)\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      UnderscriptBox["c", "_"], "\[Dagger]"], "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["\[ImaginaryI]", "2"]}], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}], 
      SuperscriptBox[
       UnderscriptBox["c", "_"], "\[Dagger]"], 
      RowBox[{"exp", "(", 
       RowBox[{
        FractionBox["\[ImaginaryI]", "2"], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}]}]}], "=", 
    RowBox[{
     SuperscriptBox[
      UnderscriptBox["c", "_"], "\[Dagger]"], 
     UnderscriptBox["U", "_"]}]}], TraditionalForm]],ExpressionUUID->
  "8fa25996-8b0b-478a-8354-89b39546b006"],
 "\[LineSeparator]or, equivalently,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["c", "_"], " ", "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        FractionBox["\[ImaginaryI]", "2"], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}], 
      UnderscriptBox["c", "_"], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["\[ImaginaryI]", "2"]}], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}]}]}], "=", 
    RowBox[{
     UnderscriptBox["U", "_"], 
     UnderscriptBox["c", "_"]}]}], TraditionalForm]],ExpressionUUID->
  "6f8edb51-9f9b-4771-93fc-023ebffedac4"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["c", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["c", "n"], ",", 
      SubsuperscriptBox["c", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["c", "n", "\[Dagger]"]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"ce58e6ef-e81e-4aee-a405-bbcb08f9be41"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["U", "_"], ":=", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
      UnderscriptBox["H", "_"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a55ecdb0-7455-4e8e-a238-9a3180d62b9d"],
 "  (without the factor of 1/2)."
}], "Notes",
 CellID->796757408,ExpressionUUID->"620ea185-47f8-4593-8916-8b22fac3d16d"],

Cell[TextData[{
 "It is important to keep the proper block structure for any unitary matrix \
",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["U", "_"], TraditionalForm]],ExpressionUUID->
  "11b84b8c-1a77-493e-8a97-a58a43445b3f"],
 " in the Nambu space: The Bogoliubov-de Gennes Hamiltonian matrix ",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["H", "_"], TraditionalForm]],ExpressionUUID->
  "fbffd7ff-4472-4949-97fc-cd4b02da1d41"],
 " has the block structure \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["H", "_"], ":=", 
    RowBox[{"(", GridBox[{
       {"H", "\[CapitalDelta]"},
       {
        SuperscriptBox["\[CapitalDelta]", "\[Dagger]"], 
        RowBox[{"-", 
         SuperscriptBox["H", "*"]}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "7de4e91a-22e1-4dab-9aca-c6396b11223f"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["H", "\[Dagger]"], "=", "H"}], TraditionalForm]],
  ExpressionUUID->"0c21ca5a-6106-4096-bf89-abb5c9b2ec15"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[CapitalDelta]", "T"], "=", 
    RowBox[{"-", "\[CapitalDelta]"}]}], TraditionalForm]],ExpressionUUID->
  "067da612-b70c-4ddd-9db5-fb88484074bb"],
 ", \[LineSeparator]and it implies the block structure of \[LineSeparator]\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["U", "_"], ":=", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
       UnderscriptBox["H", "_"]}], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {"U", "V"},
        {
         SuperscriptBox["V", "*"], 
         SuperscriptBox["U", "*"]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "99a18dfa-ecd2-4ee3-aa43-12a9fdc68953"],
 " \[LineSeparator]in the Bogoliubov-de Gennes transformation matrix."
}], "Notes",
 CellID->1053759868,ExpressionUUID->"ce733f01-38f0-42c7-aa75-594791f346b3"],

