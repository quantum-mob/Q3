Cell[TextData[{
 "A ",
 StyleBox["Bogoliubov-de Gennes transformation",
  FontSlant->"Italic"],
 " is defined by (notice the factor of 1/2)\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      UnderscriptBox["c", "_"], "\[Dagger]"], "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["\[ImaginaryI]", "2"]}], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}], 
      SuperscriptBox[
       UnderscriptBox["c", "_"], "\[Dagger]"], 
      RowBox[{"exp", "(", 
       RowBox[{
        FractionBox["\[ImaginaryI]", "2"], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}]}]}], "=", 
    RowBox[{
     SuperscriptBox[
      UnderscriptBox["c", "_"], "\[Dagger]"], 
     UnderscriptBox["U", "_"]}]}], TraditionalForm]],ExpressionUUID->
  "c23d7d31-0e1d-40d4-b08d-1ee2afaaa425"],
 "\[LineSeparator]or, equivalently,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["c", "_"], " ", "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        FractionBox["\[ImaginaryI]", "2"], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}], 
      UnderscriptBox["c", "_"], 
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["\[ImaginaryI]", "2"]}], "\[VeryThinSpace]", 
        SuperscriptBox[
         UnderscriptBox["c", "_"], "\[Dagger]"], 
        UnderscriptBox["H", "_"], 
        UnderscriptBox[
         RowBox[{"\[VeryThinSpace]", "c"}], "_"]}], ")"}]}]}], "=", 
    RowBox[{
     UnderscriptBox["U", "_"], 
     UnderscriptBox["c", "_"]}]}], TraditionalForm]],ExpressionUUID->
  "bc362ca8-671e-4b26-9651-68e429acf579"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["c", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["c", "n"], ",", 
      SubsuperscriptBox["c", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["c", "n", "\[Dagger]"]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"19f15a04-7a10-4b17-b0c5-09f715c531c6"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["U", "_"], ":=", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
      UnderscriptBox["H", "_"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "b6129b74-99bd-4b38-9648-3f49ca409c4a"],
 "  (without the factor of 1/2)."
}], "Notes",
 CellID->796757408,ExpressionUUID->"22a974d2-f72d-4d08-9697-6927069d014c"],

Cell[TextData[{
 "It is important to keep the proper block structure for any unitary matrix \
",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["U", "_"], TraditionalForm]],ExpressionUUID->
  "9e50384b-ccc3-4447-9d92-6197623d1026"],
 " in the Nambu space: The Bogoliubov-de Gennes Hamiltonian matrix ",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["H", "_"], TraditionalForm]],ExpressionUUID->
  "3b307151-2802-40eb-89fe-69678ad2b452"],
 " has the block structure \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["H", "_"], ":=", 
    RowBox[{"(", GridBox[{
       {"H", "\[CapitalDelta]"},
       {
        SuperscriptBox["\[CapitalDelta]", "\[Dagger]"], 
        RowBox[{"-", 
         SuperscriptBox["H", "*"]}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "3ae6506c-a35c-4d2f-a288-8b032d4acd2c"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["H", "\[Dagger]"], "=", "H"}], TraditionalForm]],
  ExpressionUUID->"06de5504-2047-433a-affa-ba383046e2fe"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[CapitalDelta]", "T"], "=", 
    RowBox[{"-", "\[CapitalDelta]"}]}], TraditionalForm]],ExpressionUUID->
  "43f5b5d4-df43-4ae0-8139-571731df5f1c"],
 ", \[LineSeparator]and it implies the block structure of \[LineSeparator]\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["U", "_"], ":=", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
       UnderscriptBox["H", "_"]}], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {"U", "V"},
        {
         SuperscriptBox["V", "*"], 
         SuperscriptBox["U", "*"]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "3e97ed73-19de-46a6-8713-f330dec28734"],
 " \[LineSeparator]in the Bogoliubov-de Gennes transformation matrix."
}], "Notes",
 CellID->1053759868,ExpressionUUID->"7462f8e3-d67a-41ce-971e-5f8363fd4381"],

