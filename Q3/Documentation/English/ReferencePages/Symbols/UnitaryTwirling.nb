Cell[TextData[{
 "The twirling operation over the Haar measure of the unitary group is leads \
to Choi matrix:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", 
     RowBox[{"i", ",", 
      RowBox[{"k", "|", "j"}], ",", "l"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      TemplateBox[{"i"},
       "Bra"], 
      RowBox[{"\[DoubleStruckCapitalE]", "(", 
       RowBox[{"U", 
        TemplateBox[{"k"},
         "Ket"], 
        TemplateBox[{"l"},
         "Bra"], 
        SuperscriptBox["U", "\[Dagger]"]}], ")"}], 
      TemplateBox[{"j"},
       "Ket"]}], "=", 
     RowBox[{"\[DoubleStruckCapitalE]", "(", 
      RowBox[{
       SubscriptBox["U", 
        RowBox[{"i", "\[VeryThinSpace]", "k"}]], 
       SubsuperscriptBox["U", 
        RowBox[{"j", "\[VeryThinSpace]", "l"}], "*"]}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"a38f2b94-3e7d-4924-baf3-de000db06241"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", 
     RowBox[{"i", ",", 
      RowBox[{"k", "|", "j"}], ",", "l"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Pi]", ",", "\[Sigma]"}]], 
      RowBox[{
       SubscriptBox["\[CapitalGamma]", 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], "(", "\[Pi]", ")"}], 
      RowBox[{
       SubsuperscriptBox["\[CapitalGamma]", 
        RowBox[{"k", "\[VeryThinSpace]", "l"}], "*"], "(", "\[Sigma]", ")"}], 
      
      RowBox[{"Wg", "(", 
       RowBox[{
        SuperscriptBox["\[Pi]", 
         RowBox[{"-", "1"}]], "\[Sigma]"}], ")"}]}], "=", 
     RowBox[{
      SubscriptBox[
       RowBox[{
        SubscriptBox["\[CapitalSigma]", 
         RowBox[{"\[Pi]", ",", "\[Sigma]"}]], "[", 
        RowBox[{
         RowBox[{"\[CapitalGamma]", "(", "\[Pi]", ")"}], "\[CircleTimes]", 
         RowBox[{
          SuperscriptBox["\[CapitalGamma]", "*"], "(", "\[Sigma]", ")"}]}], 
        "]"}], 
       RowBox[{"i", ",", 
        RowBox[{"k", ";", "j"}], ",", "l"}]], 
      RowBox[{"Wg", "(", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[Pi]", 
          RowBox[{"-", "1"}]], "\[Sigma]"}], ",", "d"}], ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"98d4edd4-13d0-437c-aa77-d1e7df99928d"],
 "\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckCapitalE]", "(", "\[CenterDot]", ")"}], 
   TraditionalForm]],ExpressionUUID->"f90f23c3-d2a3-44ef-996b-b490e23233f4"],
 " denotes the expectation value over the Haar measure, ",
