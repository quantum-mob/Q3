Cell[TextData[{
 "Construct the dissipative model for the Hatano-Nelson model. Here, ",
 StyleBox["L",
  FontSlant->"Italic"],
 " is the number of fermion modes (lattice sites), \[Gamma] is the damping \
constant and ",
 StyleBox["J",
  FontSlant->"Italic"],
 " is the hopping amplitude:"
}], "ExampleText",
 CellID->1761114702,ExpressionUUID->"d54cb0d3-a8aa-4f5b-8c6d-869a60ab69af"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$L", "=", "6"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$\[Gamma]", "=", "0.5"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$J", "=", 
    RowBox[{"1", "/", "$\[Gamma]"}]}], ";"}], " "}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[1]:=",
 CellID->171829249,ExpressionUUID->"e9ac150f-9283-4cff-9336-81d74ded4d65"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "HatanoNelsonModel", "]"}], "\[IndentingNewLine]", 
 RowBox[{" ", 
  RowBox[{
   RowBox[{"HatanoNelsonModel", "[", 
    RowBox[{"L_", ",", "J_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ham", ",", "cff", ",", "jmp"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ham", "=", 
       RowBox[{"PlusTopple", "[", 
        RowBox[{
         RowBox[{"-", "J"}], "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"ham", "=", 
       RowBox[{"WickHermitian", "[", "ham", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"cff", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"One", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"One", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
            "]"}]}]}], ")"}], "/", 
        RowBox[{"Sqrt", "[", "2", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"jmp", "=", 
       RowBox[{"WickJump", "@", 
        RowBox[{"Join", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Thread", "[", 
           RowBox[{"cff", "->", "0"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Thread", "[", 
           RowBox[{
            RowBox[{"Conjugate", "[", "cff", "]"}], "->", "1"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ham", ",", "jmp"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[2]:=",
 CellID->1177694704,ExpressionUUID->"84d0efc2-be8d-4b92-be9c-b65ac40022d4"],

