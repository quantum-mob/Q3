Cell["\<\
Now, we consider a projective model for the Hatano-Nelson Hamiltonian.\
\>", "ExampleText",
 CellID->1226856810,ExpressionUUID->"d257b616-c814-49c9-a8e4-3cbb1f16d926"],

Cell[TextData[{
 "Construct the dissipative model for the Hatano-Nelson model. Here, ",
 StyleBox["L",
  FontSlant->"Italic"],
 " is the number of fermion modes (lattice sites), \[Gamma] is the damping \
constant and ",
 StyleBox["J",
  FontSlant->"Italic"],
 " is the hopping amplitude:"
}], "ExampleText",
 CellID->1981503329,ExpressionUUID->"a5739720-c133-40d2-a69c-086adb77e6f2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$L", "=", "6"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$\[Gamma]", "=", "0.5"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$J", "=", 
    RowBox[{"1", "/", "$\[Gamma]"}]}], ";"}], " "}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[1]:=",
 CellID->2097536474,ExpressionUUID->"d0a2cac5-e389-4059-94ab-9b3934ae6253"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "HatanoNelsonModel", "]"}], "\[IndentingNewLine]", 
 RowBox[{" ", 
  RowBox[{
   RowBox[{"HatanoNelsonModel", "[", 
    RowBox[{"L_", ",", "J_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ham", ",", "cff", ",", "jmp"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ham", "=", 
       RowBox[{"PlusTopple", "[", 
        RowBox[{
         RowBox[{"-", "J"}], "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"ham", "=", 
       RowBox[{"WickHermitian", "[", "ham", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"cff", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"One", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"One", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
            "]"}]}]}], ")"}], "/", 
        RowBox[{"Sqrt", "[", "2", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"jmp", "=", 
       RowBox[{"WickJump", "@", 
        RowBox[{"Join", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Thread", "[", 
           RowBox[{"cff", "->", "2"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Thread", "[", 
           RowBox[{
            RowBox[{"Conjugate", "[", "cff", "]"}], "->", "3"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ham", ",", "jmp"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[2]:=",
 CellID->203969366,ExpressionUUID->"d252cd91-fc19-424a-b3c3-3777adb3d242"],

