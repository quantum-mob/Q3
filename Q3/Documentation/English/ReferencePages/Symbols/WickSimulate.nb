Cell["\<\
Now, we consider a projective model for the Hatano-Nelson Hamiltonian.\
\>", "ExampleText",
 CellID->1226856810,ExpressionUUID->"88aee97d-58d1-4b22-b2ae-d71707f76b33"],

Cell[TextData[{
 "Construct the dissipative model for the Hatano-Nelson model. Here, ",
 StyleBox["L",
  FontSlant->"Italic"],
 " is the number of fermion modes (lattice sites), \[Gamma] is the damping \
constant and ",
 StyleBox["J",
  FontSlant->"Italic"],
 " is the hopping amplitude:"
}], "ExampleText",
 CellID->1981503329,ExpressionUUID->"42846b41-eaa0-4975-9af8-7b09e565e09f"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$L", "=", "6"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$\[Gamma]", "=", "0.5"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$J", "=", 
    RowBox[{"1", "/", "$\[Gamma]"}]}], ";"}], " "}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[1]:=",
 CellID->2097536474,ExpressionUUID->"916bc2d8-ab22-4045-99c9-38254b0a9b0e"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "HatanoNelsonModel", "]"}], "\[IndentingNewLine]", 
 RowBox[{" ", 
  RowBox[{
   RowBox[{"HatanoNelsonModel", "[", 
    RowBox[{"L_", ",", "J_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ham", ",", "cff", ",", "jmp"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ham", "=", 
       RowBox[{"PlusTopple", "[", 
        RowBox[{
         RowBox[{"-", "J"}], "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"ham", "=", 
       RowBox[{"WickHermitian", "[", "ham", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"cff", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"One", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"One", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
            "]"}]}]}], ")"}], "/", 
        RowBox[{"Sqrt", "[", "2", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"jmp", "=", 
       RowBox[{"WickJump", "@", 
        RowBox[{"Join", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Thread", "[", 
           RowBox[{"cff", "->", "2"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Thread", "[", 
           RowBox[{
            RowBox[{"Conjugate", "[", "cff", "]"}], "->", "3"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ham", ",", "jmp"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[2]:=",
 CellID->203969366,ExpressionUUID->"2fb1b2bd-6689-4bf8-af49-c6e6138b26f6"],

