Cell[TextData[{
 "Construct the dissipative model for the Hatano-Nelson model. Here, ",
 StyleBox["L",
  FontSlant->"Italic"],
 " is the number of fermion modes (lattice sites), \[Gamma] is the damping \
constant and ",
 StyleBox["J",
  FontSlant->"Italic"],
 " is the hopping amplitude:"
}], "ExampleText",
 CellID->1761114702,ExpressionUUID->"0b77be4e-ddc7-4112-a2c0-4a8c9ebdca0a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$L", "=", "6"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$\[Gamma]", "=", "0.5"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$J", "=", 
    RowBox[{"1", "/", "$\[Gamma]"}]}], ";"}], " "}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[1]:=",
 CellID->171829249,ExpressionUUID->"72bfc7ab-4fb2-489f-9cf5-bd426bd3ef9a"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "HatanoNelsonModel", "]"}], "\[IndentingNewLine]", 
 RowBox[{" ", 
  RowBox[{
   RowBox[{"HatanoNelsonModel", "[", 
    RowBox[{"L_", ",", "J_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ham", ",", "cff", ",", "jmp"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ham", "=", 
       RowBox[{"PlusTopple", "[", 
        RowBox[{
         RowBox[{"-", "J"}], "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"ham", "=", 
       RowBox[{"WickHermitian", "[", "ham", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"cff", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"One", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"One", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
            "]"}]}]}], ")"}], "/", 
        RowBox[{"Sqrt", "[", "2", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"jmp", "=", 
       RowBox[{"WickJump", "@", 
        RowBox[{"Join", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Thread", "[", 
           RowBox[{"cff", "->", "0"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Thread", "[", 
           RowBox[{
            RowBox[{"Conjugate", "[", "cff", "]"}], "->", "1"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"ham", ",", "jmp"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellProlog:>Needs["QuantumMob`Q3`"],
 CellLabel->"In[2]:=",
 CellID->1177694704,ExpressionUUID->"4488c50b-e719-434a-a9fa-143821b6951c"],

