  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"i", ",", "j"}], 
      RowBox[{"2", "n"}]], 
     SubsuperscriptBox[
      UnderscriptBox["a", "_"], "i", "\[Dagger]"], 
     SubscriptBox["H", 
