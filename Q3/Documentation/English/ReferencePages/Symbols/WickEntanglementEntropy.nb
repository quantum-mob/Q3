Cell[TextData[{
 "The (normal) Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "7eb6791f-803e-40d7-a0a1-59cf50064517"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["G", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubsuperscriptBox["c", "j", "\[Dagger]"]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"58009490-6595-4874-8033-c9ffd51cf3c2"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],ExpressionUUID->
  "1e4f1e4f-c367-4084-bea7-c4f3fc4b90ea"],
 " is Hermitian, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["G", "\[Dagger]"], "=", "G"}], TraditionalForm]],
