Cell[TextData[{
 "The anomalous Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "ccb10fd2-2457-49a7-9dcf-41495804e0ab"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubscriptBox["c", "j"]}], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"d8114c13-160f-407b-8af3-17b88ec182a1"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]],ExpressionUUID->
  "04b305aa-30d5-49cf-9d7f-dab126237594"],
 " is skew-symmetric, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["F", "T"], "=", 
    RowBox[{"-", "F"}]}], TraditionalForm]],ExpressionUUID->
  "a89c3580-705f-4db2-ab5e-94d3191f0314"],
 "."
}], "Notes",
