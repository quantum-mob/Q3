 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n", "\[Times]", "2", "n"}], TraditionalForm]],ExpressionUUID->
  "2e7eb6d1-e5f6-418a-9caf-5f8ea1af3a9c"],
