  FontFamily->"Source Sans Pro",ExpressionUUID->
  "5143797f-9efd-48f7-935f-6c136d4cae90"],
 ". Then, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", " ", 
     SubscriptBox["c", "j"], 
     SuperscriptBox["u", "\[Dagger]"]}], ":=", 
    RowBox[{
     SubscriptBox["\[Sum]", "i"], 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubscriptBox["r", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"19cf89a0-d765-4dd1-bd95-485493e9a0f3"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", ",", 
    RowBox[{"j", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"2", "n"}]}], TraditionalForm]],ExpressionUUID->
  "14405e79-8a9d-4315-8e9c-1d83111fe9cc"],
 ",  where ",
 Cell[BoxData[
  RowBox[{
   StyleBox["r", "TI"], ":=", 
   RowBox[{
    TemplateBox[{
      Cell[
       TextData["MatrixExp"]], "paclet:ref/MatrixExp"},
     "RefLink",
