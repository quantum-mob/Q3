Cell[TextData[{
 "The (normal) Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "865edcb9-4c43-4ac2-a489-06584c032779"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["G", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubsuperscriptBox["c", "j", "\[Dagger]"]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"0195b6af-b4cc-45a1-ab0c-6b9bdabbd0d2"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],ExpressionUUID->
  "4bd5220e-fa38-4248-810a-643968b34d9a"],
 " is Hermitian, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["G", "\[Dagger]"], "=", "G"}], TraditionalForm]],
  ExpressionUUID->"fc6a0fc2-908a-4946-83c3-6e4c56ddc3a3"],
 ". The ",
 StyleBox["correlation matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SubsuperscriptBox["c", "i", "\[Dagger]"], 
       SubscriptBox["c", "j"]}], "\[RightAngleBracket]"}], "\[Congruent]", 
     SubscriptBox[
      RowBox[{"(", 
       RowBox[{"I", "-", 
        SuperscriptBox["G", "T"]}], ")"}], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}], TraditionalForm]],
  ExpressionUUID->"6c0cb2c8-bfd6-45e5-9131-2302773cc650"],
 " has equivalent information and is also frequently used."
}], "Notes",
 CellID->348061621,ExpressionUUID->"b0b05458-646f-4cd6-8caa-64588446c997"],

Cell[TextData[{
