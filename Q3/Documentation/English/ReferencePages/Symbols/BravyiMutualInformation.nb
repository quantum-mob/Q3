Cell[TextData[{
 "The anomalous Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "eea2401c-cc11-4ea1-8b2a-846fda1ddc90"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubscriptBox["c", "j"]}], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"d9bc91c1-847f-4de5-a217-58773f6240a5"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]],ExpressionUUID->
  "2fa00bdb-aec2-4944-9936-37b4b90f0419"],
 " is skew-symmetric, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["F", "T"], "=", 
    RowBox[{"-", "F"}]}], TraditionalForm]],ExpressionUUID->
  "2356ecca-ecc0-46fb-aee9-e24566db26c1"],
 "."
}], "Notes",
