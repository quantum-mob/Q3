  FormBox[
   RowBox[{
    SuperscriptBox["F", "T"], "=", 
    RowBox[{"-", "F"}]}], TraditionalForm]],ExpressionUUID->
  "b1a7acce-29cd-4325-8c5a-d74f20cc579f"],
