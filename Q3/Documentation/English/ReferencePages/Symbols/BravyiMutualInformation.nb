     SubscriptBox[
      RowBox[{"(", 
       RowBox[{"I", "-", 
        SuperscriptBox["G", "T"]}], ")"}], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}], TraditionalForm]],
