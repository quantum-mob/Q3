Cell[TextData[{
 "The cross entropy measures the difference between two probability \
distributions or quantum states; see also the classical and quantum ",
 StyleBox["relative entropies",
  FontSlant->"Italic"],
 "."
}], "Notes",
 CellID->1795278045,ExpressionUUID->"a51f20b8-161b-465b-969b-e9e69e76b347"],

Cell[TextData[{
 "More specifically, in classical information theory, it measures the average \
number of bits required to identify an event drawn from the set when the \
coding scheme used for the set is optimized for an estimated probability \
distribution ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["q", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["q", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"]}], "}"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "69a4720e-d546-45e0-8800-0c9e17d6bb4c"],
 ", rather than the true distribution ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["p", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["p", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"]}], "}"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "a9bc6e99-3fd9-444c-a193-b6d0aca0ba4d"],
 "."
}], "Notes",
