Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"de8f71c3-1689-414d-bc85-7fcd3c3c6eb9"],
 " be the full Pauli group on ",
 StyleBox["n",
  FontSlant->"Italic"],
 " qubits. The center of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"f29efca7-137f-410e-a77b-e3517346d12d"],
 " is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{"I", ",", " ", 
      RowBox[{"-", "I"}], ",", " ", 
      RowBox[{"\[ImaginaryI]", " ", "I"}], ",", " ", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "I"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"57f9d497-2bfe-4c90-8741-87ce26fa3943"],
 ". The Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"4e759e31-6023-4930-9cbd-ba37bb046789"],
 " is defined to be the quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], "/", 
     "\[ScriptCapitalZ]"}]}], TraditionalForm]],ExpressionUUID->
  "d6244718-1d79-4291-8e5d-23af1b8e3325"],
 ". The Pauli group is isomorphic to the Abelian group ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
    RowBox[{"2", "n"}]], TraditionalForm]],ExpressionUUID->
  "70970375-35a0-463f-a76a-341927aa8abd"],
 ", which can also be regarded as a vector space over ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[DoubleStruckCapitalZ]", "2"], TraditionalForm]],
  ExpressionUUID->"7246cc0d-7958-49f7-bf70-b345efe0136a"],
 "."
}], "Notes",
 CellID->447549871,ExpressionUUID->"4ba8a51c-987d-4f18-8b7e-daec0e61d167"],

Cell[TextData[{
 "A Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"64da16f2-7f25-4ebe-b7f4-0c7696027c7d"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "1"], ",", 
        SubscriptBox["z", "1"], ",", 
        SubscriptBox["x", "2"], ",", 
        SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["x", "n"], ",", 
        SubscriptBox["z", "n"]}], ")"}], "T"], "\[Element]", 
     SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
      RowBox[{"2", "n"}]]}]}], TraditionalForm]],ExpressionUUID->
  "e95fc3d0-06a2-4ab8-a466-7930f3cc1176"],
 "."
}], "Notes",
 CellID->1183077781,ExpressionUUID->"113989a1-477f-4b5a-994f-014d2397fc61"],

