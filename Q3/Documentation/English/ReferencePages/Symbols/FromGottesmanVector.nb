Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"50df7f28-8f93-4f78-bf12-54c6ce0f00a4"],
 " be the full Pauli group on ",
 StyleBox["n",
  FontSlant->"Italic"],
 " qubits. The center of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"6809a758-454a-4cc7-913e-5f7447ea0851"],
 " is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{"I", ",", " ", 
      RowBox[{"-", "I"}], ",", " ", 
      RowBox[{"\[ImaginaryI]", " ", "I"}], ",", " ", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "I"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"6ab6ba47-53fd-4e39-8bee-20b702f8679c"],
 ". The Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"1600c35a-9703-48f0-9942-c1fdff0abcc8"],
 " is defined to be the quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ScriptCapitalP]", "\[Prime]"], "(", "n", ")"}], "/", 
     "\[ScriptCapitalZ]"}]}], TraditionalForm]],ExpressionUUID->
  "223e7e75-76c4-4793-99f6-bbe5b7d820f3"],
 ". The Pauli group is isomorphic to the Abelian group ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
    RowBox[{"2", "n"}]], TraditionalForm]],ExpressionUUID->
  "f220e60a-fb92-4134-b37c-134b3aa218ab"],
 ", which can also be regarded as a vector space over ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[DoubleStruckCapitalZ]", "2"], TraditionalForm]],
  ExpressionUUID->"7272ebca-9e35-4ac3-a489-14f267e304a5"],
 "."
}], "Notes",
 CellID->447549871,ExpressionUUID->"9ff91254-fd61-4c8c-993a-184a6bb7f7b7"],

Cell[TextData[{
 "A Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"a0089850-2f77-4d93-88d3-01d05f757141"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "1"], ",", 
        SubscriptBox["z", "1"], ",", 
        SubscriptBox["x", "2"], ",", 
        SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["x", "n"], ",", 
        SubscriptBox["z", "n"]}], ")"}], "T"], "\[Element]", 
     SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
      RowBox[{"2", "n"}]]}]}], TraditionalForm]],ExpressionUUID->
  "fad0dcdf-602f-4b6b-9014-c83fa1b0f29a"],
 "."
}], "Notes",
 CellID->1183077781,ExpressionUUID->"007bf000-2913-4145-87e8-bf1a82001e88"],

