Cell[TextData[{
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["LindbladConvert"]], "paclet:QuantumMob/Q3/ref/LindbladConvert",
     "QuantumMob Package Symbol"},
   "PackageLink",
   BaseStyle->"InlineFormula"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "dbd8c3fa-dab2-4b2c-a11e-f34a429a1cf6"],
 " converts the Lindblad equation into the form\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SubscriptBox[
         OverscriptBox["\[Rho]", "."], 
         StyleBox["i", "TI"]], "\[LongEqual]", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         StyleBox["M", "TI"], 
         RowBox[{
           StyleBox["i", "TI"], 
           StyleBox["j", "TI"]}]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]], "+", 
        SubscriptBox[
         StyleBox["v", "TI"], 
         StyleBox["j", "TI"]], "  ", 
        RowBox[{"(", 
          RowBox[{
            StyleBox["i", "TI"], "\[LongEqual]", "1", ",", "\[Ellipsis]", ",", 
            SuperscriptBox[
             StyleBox["n", "TI"], "2"], "-", "1"}], ")"}]}], TraditionalForm],
     "errors" -> {}, "input" -> 
    "\\dot\\rho_i=\\sum_{j=1}^{n^2-1}M_{ij}\\rho_j+v_j \\quad \
(i=1,\\dots,n^2-1)", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "5028692c-e2cf-41b1-8718-bdd6830e0219"],
 ",\nwhere the density operator is represented in the Lie basis, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"{", 
         RowBox[{
           SubscriptBox[
            StyleBox["B", "TI"], 
            StyleBox["j", "TI"]], ":", 
           StyleBox["j", "TI"], "\[LongEqual]", "1", ",", "2", ",", 
           "\[Ellipsis]", ",", 
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], "}"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\{B_j:j=1,2,\\dots,n^2-1\\}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f52a369b-f0a3-4cdc-8474-82de527059e4"],
 ", as\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{"\[Rho]", "\[LongEqual]", 
        FractionBox["1", 
         StyleBox["n", "TI"]], 
        StyleBox["I", "TI"], "+", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         StyleBox["B", "TI"], 
         StyleBox["j", "TI"]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
    "\\rho=\\frac{1}{n}I+\\sum_{j=1}^{n^2-1}B_j\\rho_j", "state" -> "Boxes"|>,
   
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "19b2bd05-e96f-4ef4-91b1-6469656bc0d8"],
 " ."
}], "Notes",
 CellID->1292051031,ExpressionUUID->"82075801-ce1e-43b4-bece-4afba0971eb2"],

Cell[TextData[{
 "The solution of the Lindblad equation is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Rho]", 
          StyleBox["i", "TI"]], 
         RowBox[{"(", 
           StyleBox["t", "TI"], ")"}], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          StyleBox["j", "TI"], LimitsPositioning -> True], 
         SubscriptBox[
          RowBox[{"[", 
            SuperscriptBox[
             StyleBox["e", "TI"], 
             RowBox[{
               StyleBox["M", "TI"], 
               StyleBox["t", "TI"]}]], "]"}], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], 
         SubscriptBox["\[Rho]", 
          StyleBox["j", "TI"]], 
         RowBox[{"(", "0", ")"}], "+", 
         UnderscriptBox["\[Sum]", 
          StyleBox["j", "TI"], LimitsPositioning -> True], 
         SubscriptBox[
          RowBox[{"[", 
            RowBox[{
              SubsuperscriptBox["\[Integral]", "0", 
               StyleBox["t", "TI"]], 
              RowBox[{
                StyleBox["d", "TI"], 
                StyleBox["s", "TI"]}], " ", 
              SuperscriptBox[
               StyleBox["e", "TI"], 
               RowBox[{
                 StyleBox["M", "TI"], 
                 StyleBox["s", "TI"]}]]}], "]"}], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], 
         SubscriptBox[
          StyleBox["v", "TI"], 
          StyleBox["j", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\rho_i(t)=\\sum_j[e^{Mt}]_{ij}\\rho_j(0)+\\sum_j\\left[\\int_0^t{ds}\\,\
e^{Ms}\\right]_{ij}v_j", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d08f413b-864d-4327-9ea0-b9b363bd66e3"],
 "."
}], "Notes",
 CellID->421100671,ExpressionUUID->"22989858-6811-401a-8dd4-5fe3aa0355af"],

