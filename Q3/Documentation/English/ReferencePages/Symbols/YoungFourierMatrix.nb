Cell[TextData[{
 "The inverse transform reads as\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"g"},
     "Ket"], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "\[Lambda]"], 
     FractionBox[
      SqrtBox[
       SubscriptBox["d", "\[Lambda]"]], 
      SqrtBox[
       RowBox[{"|", "\[ScriptCapitalG]", "|"}]]], 
     UnderscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     TemplateBox[{
       RowBox[{"\[Lambda]", ",", 
         RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
      "Ket"], 
     RowBox[{
      SubsuperscriptBox["\[CapitalGamma]", 
       RowBox[{"j", "\[VeryThinSpace]", "i"}], "\[Lambda]"], "(", "g", 
      ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "5852b399-613b-485c-8919-435671940227"],
 " .\nHere, notice the reversed indices in ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[CapitalGamma]", 
     RowBox[{"j", "\[VeryThinSpace]", "i"}], "\[Lambda]"], "(", "g", ")"}], 
   TraditionalForm]],ExpressionUUID->"a86104fa-598a-49bb-8807-309d461553f6"],
 "."
}], "Notes",
 CellID->469159088,ExpressionUUID->"d7f96f96-ab2e-49da-b617-4b8b0ccfe3c3"],

Cell[TextData[{
 "Now, let ",
 Cell[BoxData[
  FormBox[
