Cell[TextData[{
 "Typically, quantum information scrambling is examined in terms of the \
out-of-time-order correlator (OTOC) \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["A", "\[Dagger]"], "(", "t", ")"}], 
      SuperscriptBox["B", "\[Dagger]"], 
      RowBox[{"A", "(", "t", ")"}], "B"}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"67b333c9-55ed-440b-b974-53d5b5541fe7"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "cb55a619-35f8-4e1f-ae65-349a9c4aca6d"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "2cb32c5f-90a5-4898-b5ba-5bf8d8ec6ff0"],
 " are local unitary operators, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "t", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "(", "t", ")"}], "A", " ", 
     RowBox[{"U", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "4d77a83c-5e24-45b9-946a-96f1b00d3605"],
 " is the Heisenberg picture of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "3e050e91-8d4d-400a-8fbe-1b5f12328afc"],
 " governed by the time-evolution operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "46baaa58-41c3-4763-b8e0-6ed320793000"],
 "(t)."
}], "Notes",
 CellID->491373739,ExpressionUUID->"e6223bc2-aabe-4f76-85ad-3bede20acb7c"],

Cell[TextData[{
 "The OTOC satisfies the following relation:\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{"C", "(", "t", ")"}], "]"}], "=", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"[", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], "\[Dagger]"], "[", 
        RowBox[{
         RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], 
       "\[RightAngleBracket]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "90907062-850a-4585-9e45-02c71f48714b"],
 " ."
}], "Notes",
 CellID->984433309,ExpressionUUID->"3f54e098-45f0-47cf-8d20-dcb2f4c66d3f"]
