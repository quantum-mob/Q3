Cell[TextData[{
 "A Clifford operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "443e2515-a586-4a02-8236-0f9a433f73c3"],
 " is completely specified (up to a global phase factor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", 
     RowBox[{"\[Pi]", " ", "/", "4"}]}]], TraditionalForm]],ExpressionUUID->
  "e01f2d0d-9940-4632-a3d7-6fd7c03891a4"],
 ") by the transformation of Pauli operators under conjugation by ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "d5818ac3-9cde-4af3-9ee4-61434ab2ba63"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["X", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["p", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "46f4e145-d46f-4340-845e-7c19b1864501"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["Z", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["q", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["c", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["d", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "3b8f8fc5-e5f1-466d-b8e8-f4301dce03b2"],
 "."
}], "Notes",
 CellID->1417856937,ExpressionUUID->"f041643d-b68e-4908-b38d-7741a4c04a1f"],

Cell[TextData[{
 "The Gottesman matrix of ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "85ff7d41-9a9d-41dd-9c55-97873703e7dd"],
 " is defined by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "U", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", "11"], 
        SubscriptBox["c", "11"], 
        SubscriptBox["a", "12"], 
        SubscriptBox["c", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "11"], 
        SubscriptBox["d", "11"], 
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["a", "21"], 
        SubscriptBox["c", "21"], 
        SubscriptBox["a", "22"], 
        SubscriptBox["c", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "21"], 
        SubscriptBox["b", "22"], 
        SubscriptBox["d", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "c6fbfe3f-b9c3-465c-9dc5-1ad71b7cdfd4"]
}], "Notes",
 CellID->2083430531,ExpressionUUID->"484dab83-04d0-4c30-971f-33bc1443a533"],

Cell[TextData[{
 "Recall that a Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"8cda24a7-bc5a-4b37-bcaa-40ef36f823c7"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], ",", 
       SubscriptBox["z", "1"], ",", 
       SubscriptBox["x", "2"], ",", 
       SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["x", "n"], ",", 
       SubscriptBox["z", "n"]}], ")"}], "T"]}], TraditionalForm]],
  ExpressionUUID->"b93f210f-4cf3-4a7e-a91a-490f915bd207"],
 "."
}], "Notes",
 CellID->1441760612,ExpressionUUID->"05d7db0a-2d12-4f31-9b33-16039a53d3bd"],

