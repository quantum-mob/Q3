Cell[TextData[{
 "A Clifford operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "217638ee-00e7-4fa3-8bb4-0c027a0b648a"],
 " is completely specified (up to a global phase factor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", 
     RowBox[{"\[Pi]", " ", "/", "4"}]}]], TraditionalForm]],ExpressionUUID->
  "3b1ecd07-2b0d-48c7-9d3d-cc6df8cef8af"],
 ") by the transformation of Pauli operators under conjugation by ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "5a6d94ae-7894-4bcf-b4a0-adb4ef28c642"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["X", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["p", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "14d2a3e3-b3be-4e57-b268-548a5a6f752f"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["Z", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["q", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["c", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["d", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "bd1b395b-6811-483b-83fd-5ff43649d0ec"],
 "."
}], "Notes",
 CellID->1417856937,ExpressionUUID->"ba8ba825-86c2-4e24-9fa8-a97cf564303a"],

Cell[TextData[{
 "The Gottesman matrix of ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "fcc0d2b0-0f34-4120-8c04-6c41bd802810"],
 " is defined by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "U", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", "11"], 
        SubscriptBox["c", "11"], 
        SubscriptBox["a", "12"], 
        SubscriptBox["c", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "11"], 
        SubscriptBox["d", "11"], 
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["a", "21"], 
        SubscriptBox["c", "21"], 
        SubscriptBox["a", "22"], 
        SubscriptBox["c", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "21"], 
        SubscriptBox["b", "22"], 
        SubscriptBox["d", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "1f323ec0-a824-474f-93fb-c43db7aaaecc"]
}], "Notes",
 CellID->2083430531,ExpressionUUID->"5e7ac0da-ba08-4a5a-bc84-30afccf6fccd"],

Cell[TextData[{
 "Recall that a Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"530723cc-8aae-4238-ad57-ab19cc9e6972"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], ",", 
       SubscriptBox["z", "1"], ",", 
       SubscriptBox["x", "2"], ",", 
       SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["x", "n"], ",", 
       SubscriptBox["z", "n"]}], ")"}], "T"]}], TraditionalForm]],
  ExpressionUUID->"34fb4491-b3e5-4fc7-b85b-efd2ab7befef"],
 "."
}], "Notes",
 CellID->1441760612,ExpressionUUID->"d488f610-5d6b-47d3-805e-dff32004fccf"],

