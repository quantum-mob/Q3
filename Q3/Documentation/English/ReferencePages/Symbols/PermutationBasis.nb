  ExpressionUUID->"51e5dd9e-68dc-4775-8f05-916498df2267"],
 ", \t\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ScriptCapitalM]", "\[Mu]"], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SuperscriptBox["\[ScriptCapitalS]", "\[Lambda]"], 
      SubscriptBox["K", 
       RowBox[{"\[Lambda]", "\[VeryThinSpace]", "\[Mu]"}]]}]}]}], 
   TraditionalForm]],ExpressionUUID->"4922c714-1220-4405-83f4-820d7dc8c23e"],
 " ,\nwhere the sum is over all ",
 StyleBox["partitions",
  FontSlant->"Italic"],
