Cell[TextData[{
 "A Clifford operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "281e5dd5-949c-4ed9-9cc3-95fd0496e916"],
 " is completely specified (up to a global phase factor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", 
     RowBox[{"\[Pi]", " ", "/", "4"}]}]], TraditionalForm]],ExpressionUUID->
  "8e2e709c-71f5-471d-84e9-6cc88c5ca2ff"],
 ") by the transformation of Pauli operators under conjugation by ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "3f8f1b51-3ce5-4ef6-b90c-b8eb592812ba"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["X", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["p", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "c703eb01-73b3-4f3a-b173-fa5cfb2904a8"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["Z", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["q", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["c", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["d", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "5d4d27f7-c5c9-43eb-9408-1b6ff1238e8f"],
 "."
}], "Notes",
 CellID->1417856937,ExpressionUUID->"09007311-70ac-43ae-be77-79d9e443054a"],

Cell[TextData[{
 "The Gottesman matrix of ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "79236810-66b8-4b2f-bb28-c779e9cd046c"],
 " is defined by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "U", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", "11"], 
        SubscriptBox["c", "11"], 
        SubscriptBox["a", "12"], 
        SubscriptBox["c", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "11"], 
        SubscriptBox["d", "11"], 
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["a", "21"], 
        SubscriptBox["c", "21"], 
        SubscriptBox["a", "22"], 
        SubscriptBox["c", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "21"], 
        SubscriptBox["b", "22"], 
        SubscriptBox["d", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "8b6b7ce6-0b7b-4dd7-a74c-1b9485383a64"]
}], "Notes",
 CellID->2083430531,ExpressionUUID->"2c5d1c14-15f3-4a8e-a942-234b04673ff5"],

Cell[TextData[{
 "Recall that a Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"79fc9c21-2eab-4989-9a1b-c7e707b00cfa"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], ",", 
       SubscriptBox["z", "1"], ",", 
       SubscriptBox["x", "2"], ",", 
       SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["x", "n"], ",", 
       SubscriptBox["z", "n"]}], ")"}], "T"]}], TraditionalForm]],
  ExpressionUUID->"2d670fcb-175f-4957-b2ff-28fa9772f512"],
 "."
}], "Notes",
 CellID->1441760612,ExpressionUUID->"eba28647-9f6b-441f-b349-5cd2f3d63bde"],

