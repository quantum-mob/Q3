Cell[TextData[{
 "A Clifford operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "7564be28-9b63-4c1d-b40d-f2d4ad6ea702"],
 " is completely specified (up to a global phase factor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", 
     RowBox[{"\[Pi]", " ", "/", "4"}]}]], TraditionalForm]],ExpressionUUID->
  "5c8921dd-2f2c-4a00-a3dd-02b8faf24eb2"],
 ") by the transformation of Pauli operators under conjugation by ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "fe98787f-e73e-404b-b2f1-bea60025e28f"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["X", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["p", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "454f23ee-d9c7-463b-b410-446042f898a3"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["Z", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["q", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["c", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["d", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "958f2233-8100-434f-bd4c-4f3dd4e1aaae"],
 "."
}], "Notes",
 CellID->1417856937,ExpressionUUID->"a2c5ad93-c69c-4bb2-a1ac-c29acd8047f3"],

Cell[TextData[{
 "The Gottesman matrix of ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "9469998a-d0f0-4922-9751-ac3cc3ea8284"],
 " is defined by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "U", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", "11"], 
        SubscriptBox["c", "11"], 
        SubscriptBox["a", "12"], 
        SubscriptBox["c", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "11"], 
        SubscriptBox["d", "11"], 
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["a", "21"], 
        SubscriptBox["c", "21"], 
        SubscriptBox["a", "22"], 
        SubscriptBox["c", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "21"], 
        SubscriptBox["b", "22"], 
        SubscriptBox["d", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "9e1e083d-c115-4b30-ab3b-6dc369da488f"]
}], "Notes",
 CellID->2083430531,ExpressionUUID->"87159d1d-604a-4958-ae19-ca508d53abb6"],

Cell[TextData[{
 "Recall that a Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"bde2f7e0-e30e-479e-b275-5d7f8f37b508"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], ",", 
       SubscriptBox["z", "1"], ",", 
       SubscriptBox["x", "2"], ",", 
       SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["x", "n"], ",", 
       SubscriptBox["z", "n"]}], ")"}], "T"]}], TraditionalForm]],
  ExpressionUUID->"9d487450-7add-4750-a395-3aaa734ed733"],
 "."
}], "Notes",
 CellID->1441760612,ExpressionUUID->"4123efd4-a9af-4c29-b6ad-43caf3ec36df"],

