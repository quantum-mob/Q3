     SubscriptBox[
      StyleBox["u", "TI"], 
      StyleBox["in", "TI"]], ",", 
     SubscriptBox[
      StyleBox["v", "TI"], 
      StyleBox[
       RowBox[{
        StyleBox["i", "TI"], 
        StyleBox["1", "TR"]}],
       ZeroWidthTimes->True]], ",", 
     StyleBox["\[Ellipsis]", "TR"], ",", 
