Cell[TextData[{
 "For  a mixed state\[LineSeparator]\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], "r"], 
     TemplateBox[{
       SubscriptBox["s", "k"]},
      "Ket"], 
     SubscriptBox["s", "k"], 
     TemplateBox[{
       SubscriptBox["s", "k"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "9e9cc2f2-3fc2-48c3-b977-597aa445ab69"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "97c9721c-d654-4647-930e-ee46cbf41301"],
 " is the rank of \[Rho] and quantum states ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     SubscriptBox["s", "k"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "3300ba39-d807-4dc1-879e-f102ab451832"],
 " are orthonormal, ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Purification"]], "paclet:QuantumMob/Q3/ref/Purification", 
     "QuantumMob Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   StyleBox["\[Rho]", "TR"], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "1a07e181-0b86-416f-843c-fc711cf8f9a8"],
 " returns the purification of the form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPsi]"},
     "Ket"], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "k"], 
     RowBox[{
      TemplateBox[{
        SubscriptBox["s", "k"]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"k"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "2e1e4627-cf37-40d5-b6fd-10d1e5964210"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{"k"},
        "Ket"], "|", "k"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
     "r"}], "}"}], TraditionalForm]],ExpressionUUID->
  "5915166e-d7c4-41d0-a198-335b937ad31e"],
 " is an arbitrary orthonormal set in another Hilbert space. "
}], "Notes",
 CellID->193298559,ExpressionUUID->"9cc18c51-2f5f-427d-bc39-eaa88155870a"],

Cell[TextData[{
 "For a given density operator \[Rho], there exists unitary freedom to choose \
a purification leading to the same density operator \[Rho]. It is called the ",
 StyleBox["Gisin",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox["-",
  FontSlant->"Italic"],
 StyleBox["Hughston",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox["-Jozsa-",
  FontSlant->"Italic"],
 StyleBox["Wooters",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox[" (GHJW) theorem",
  FontSlant->"Italic"],
 "."
}], "Notes",
 CellID->1715981318,ExpressionUUID->"a5c6fe08-d2b4-4dab-9031-96c64b48680e"],

Cell[TextData[{
 "For any unitary operators ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "4f4f7931-e3ba-4573-bdbe-837086570fe0"],
 " and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "3a827f71-afa4-41a9-adc6-e5dc54b99f0e"],
 ", the most general form of the purification of \[Rho] of dimension ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "d1a65977-02a4-4c71-a8c7-5b1243897e8f"],
 " is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPsi]"},
     "Ket"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SqrtBox["\[Rho]"], "U"}], ")"}], "\[CircleTimes]", "V"}], ")"}], 
     TemplateBox[{"\[CapitalPhi]"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "5f8f214e-56bc-45f0-8438-dbd665e7f2af"],
 ",\[LineSeparator]where\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPhi]"},
     "Ket"], ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["d"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], "d"], 
     RowBox[{
      TemplateBox[{"k"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"k"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "6d9f4f20-a5b3-464d-96d6-dcfaba1aaee6"],
 "."
}], "Notes",
 CellID->1791480633,ExpressionUUID->"95e04e5d-6ca9-4b18-967a-527f8220586e"]
