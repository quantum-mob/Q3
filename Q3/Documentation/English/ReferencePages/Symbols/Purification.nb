Cell[TextData[{
 "For  a mixed state\[LineSeparator]\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], "r"], 
     TemplateBox[{
       SubscriptBox["s", "k"]},
      "Ket"], 
     SubscriptBox["s", "k"], 
     TemplateBox[{
       SubscriptBox["s", "k"]},
      "Bra"]}]}], TraditionalForm]],ExpressionUUID->
  "ef7ff376-5d91-4503-afcb-f8315b31f52c"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],ExpressionUUID->
  "f0dd6d34-7251-4937-891b-d3b0c71dcc49"],
 " is the rank of \[Rho] and quantum states ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{
     SubscriptBox["s", "k"]},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "6a6d9e6d-bf65-49b0-bec6-dbc525a568b5"],
 " are orthonormal, ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Purification"]], "paclet:QuantumMob/Q3/ref/Purification", 
     "QuantumMob Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   StyleBox["\[Rho]", "TR"], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "4f23491c-2343-460b-b6cf-8edd2ec8f3ae"],
 " returns the purification of the form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPsi]"},
     "Ket"], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "k"], 
     RowBox[{
      TemplateBox[{
        SubscriptBox["s", "k"]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"k"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "84b014f5-7ccb-4888-9761-b15c03b47e59"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{"k"},
        "Ket"], "|", "k"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
     "r"}], "}"}], TraditionalForm]],ExpressionUUID->
  "9faf550f-9b82-4aa4-8eb3-d2d973374a46"],
 " is an arbitrary orthonormal set in another Hilbert space. "
}], "Notes",
 CellID->193298559,ExpressionUUID->"ad5cc7b2-5b36-4a3e-901e-286aa76f7436"],

Cell[TextData[{
 "For a given density operator \[Rho], there exists unitary freedom to choose \
a purification leading to the same density operator \[Rho]. It is called the ",
 StyleBox["Gisin",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox["-",
  FontSlant->"Italic"],
 StyleBox["Hughston",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox["-Jozsa-",
  FontSlant->"Italic"],
 StyleBox["Wooters",
  IgnoreSpellCheck->True,
  FontSlant->"Italic"],
 StyleBox[" (GHJW) theorem",
  FontSlant->"Italic"],
 "."
}], "Notes",
 CellID->1715981318,ExpressionUUID->"fc1a9fde-ac09-421b-b4ec-debbfc5eafa2"],

Cell[TextData[{
 "For any unitary operators ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "747d3027-f632-4e0c-9f44-faf487669c2f"],
 " and ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "be290026-c4d8-4add-a089-ae2cbb5cfdf3"],
 ", the most general form of the purification of \[Rho] of dimension ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "e955d7bd-1f0f-4424-a345-9b9c3a999f96"],
 " is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPsi]"},
     "Ket"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SqrtBox["\[Rho]"], "U"}], ")"}], "\[CircleTimes]", "V"}], ")"}], 
     TemplateBox[{"\[CapitalPhi]"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "2f047763-6646-4880-8582-1eed79760799"],
 ",\[LineSeparator]where\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPhi]"},
     "Ket"], ":=", 
    RowBox[{
     FractionBox["1", 
      SqrtBox["d"]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], "d"], 
     RowBox[{
      TemplateBox[{"k"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"k"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "c69aa0bc-d417-489b-a7f5-0de46b8e776c"],
 "."
}], "Notes",
 CellID->1791480633,ExpressionUUID->"5c2065d5-442f-4a5a-b261-b9bdeb9b179f"]
