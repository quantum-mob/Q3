  FontFamily->"Source Sans Pro",ExpressionUUID->
  "33dcbc52-7ae2-416c-ac0d-79f4e9d87d7c"],
 "."
}], "ExampleText",
 CellID->2002068658,ExpressionUUID->"faf5c6bd-6308-4098-8637-057ffd35b44e"],

