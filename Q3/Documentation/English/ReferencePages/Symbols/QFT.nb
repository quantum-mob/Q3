  FormBox[Cell[TextData[Cell[BoxData[
    FormBox[
     RowBox[{
      OverscriptBox["x", "~"], ":=", 
      SubscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", 
          RowBox[{"k", "-", "1"}]], "\[Ellipsis]", "\[VeryThinSpace]", 
         SubscriptBox["x", "2"], 
         SubscriptBox["x", "1"]}], ")"}], "2"]}], TraditionalForm]],
    ExpressionUUID->"66c62485-9424-411b-9bbc-a748176a205f"]],ExpressionUUID->
