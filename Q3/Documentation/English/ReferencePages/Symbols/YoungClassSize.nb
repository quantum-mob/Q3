Cell[TextData[{
 "The cardinality (size) of conjugacy class ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalC]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"a753a26d-2a25-4f07-afec-84988a3a72cc"],
 " of cycle decomposition type ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "\[RightTee]", "n"}], TraditionalForm]],
  ExpressionUUID->"369aeaa8-10a4-41a4-ab9f-78d770bc1b47"],
 " is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     SubscriptBox["\[ScriptCapitalC]", "\[Lambda]"], 
     "\[RightBracketingBar]"}], ":=", 
    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox["1", 
          SubscriptBox["m", "1"]], 
         SuperscriptBox["2", 
          SubscriptBox["m", "2"]]}], "..."}], 
       SuperscriptBox["n", 
        RowBox[{
         SubscriptBox["m", "n"], " "}]], 
       RowBox[{
        SubscriptBox["m", "1"], "!"}], 
       RowBox[{
        SubscriptBox["m", "2"], "!"}], 
       RowBox[{
        SubscriptBox["\[Ellipsis]m", "n"], "!"}]}]], "=", 
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{"\[LeftBracketingBar]", 
       SubscriptBox["\[ScriptCapitalZ]", "\[Lambda]"], 
       "\[RightBracketingBar]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "609b2ed1-a792-4550-89af-410beffa92da"],
 " ."
}], "Notes",
