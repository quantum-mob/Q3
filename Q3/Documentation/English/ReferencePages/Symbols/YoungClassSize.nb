Cell[TextData[{
 "The cardinality (size) of conjugacy class ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalC]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"3b90c68b-ca13-4bca-b54d-ba667ccb61d8"],
 " of cycle decomposition type ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "\[RightTee]", "n"}], TraditionalForm]],
  ExpressionUUID->"b9624620-e67c-4963-a876-7d65d71fb387"],
 " is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     SubscriptBox["\[ScriptCapitalC]", "\[Lambda]"], 
     "\[RightBracketingBar]"}], ":=", 
    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox["1", 
          SubscriptBox["m", "1"]], 
         SuperscriptBox["2", 
          SubscriptBox["m", "2"]]}], "..."}], 
       SuperscriptBox["n", 
        RowBox[{
         SubscriptBox["m", "n"], " "}]], 
       RowBox[{
        SubscriptBox["m", "1"], "!"}], 
       RowBox[{
        SubscriptBox["m", "2"], "!"}], 
       RowBox[{
        SubscriptBox["\[Ellipsis]m", "n"], "!"}]}]], "=", 
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{"\[LeftBracketingBar]", 
       SubscriptBox["\[ScriptCapitalZ]", "\[Lambda]"], 
       "\[RightBracketingBar]"}]]}]}], TraditionalForm]],ExpressionUUID->
  "3bad7662-6aef-4815-afce-30830a647487"],
 " ."
}], "Notes",
