 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     RowBox[{"exp", "[", 
      RowBox[{"z", "\[VeryThinSpace]", 
       SuperscriptBox["a", "\[Dagger]"]}], "]"}], 
     TemplateBox[{"0"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     FractionBox[
      SuperscriptBox["z", "n"], 
      SqrtBox[
       RowBox[{"n", "!"}]]], 
     TemplateBox[{"n"},
      "Ket"]}]}], TraditionalForm]],
  InitializationCell->True,ExpressionUUID->
  "5ac03447-9aeb-4141-916a-3ce9b95d50db"],
