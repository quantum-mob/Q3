 ", ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["CoherentState"]], "paclet:Graphy/ref/CoherentState", 
     "Graphy Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   RowBox[{"<|", 
    RowBox[{"\[Ellipsis]", ",", " ", 
     RowBox[{
      StyleBox["a", "TI"], "\[Rule]", 
      StyleBox["z", "TI"]}], ",", " ", "\[Ellipsis]"}], "|>"}], "]"}]], 
  "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "5c62fe33-036f-4b37-9eb9-568b7b9d4d96"],
 " is expanded as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     RowBox[{"exp", "[", 
      RowBox[{"z", "\[VeryThinSpace]", 
       SuperscriptBox["a", "\[Dagger]"]}], "]"}], 
     TemplateBox[{"0"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     FractionBox[
      SuperscriptBox["z", "n"], 
      SqrtBox[
       RowBox[{"n", "!"}]]], 
     TemplateBox[{"n"},
      "Ket"]}]}], TraditionalForm]],
  InitializationCell->True,ExpressionUUID->
  "e4df11a9-1d7d-4a27-ac2d-703ce94dfbc2"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox[
       RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], "2"]}],
      "/", "2"}], ")"}], TraditionalForm]],ExpressionUUID->
  "5dab45af-22de-407b-91f5-849e83c44ccd"],
