Cell[TextData[{
 "More restricted case requires that all dressed ",
 StyleBox[ButtonBox["Dirac fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Fermion"],
  FontSlant->"Italic"],
 " modes be orthogonal to each other satisfying the canonical \
anti-commutation relations: Suppose that matrix ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],ExpressionUUID->
  "881f95d0-84dc-46f0-ac9b-2a70cf7411e5"],
 ", which is in the block form ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", ":=", 
    RowBox[{"(", GridBox[{
       {"u", "v"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "1d7285f9-4d7b-4ef5-aec9-ddc28b18fb17"],
 ", describes a sequence of ",
 StyleBox["mutually orthogonal",
  FontSlant->"Italic"],
 " dressed Dirac fermion modes\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "i"], ":=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"j", "=", "1"}], "n"], 
      SubscriptBox["u", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SubscriptBox["a", "j"]}], "+", 
     RowBox[{
      SubscriptBox["v", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SubsuperscriptBox["a", "j", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"b6beb50e-13c4-471f-9de3-1847fcc04e39"],
 ". \[LineSeparator]Note that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "k"], TraditionalForm]],ExpressionUUID->
  "2a8b15e4-66e2-485c-a5a2-8d3c9abb7f30"],
 " form a orthogonal set of ",
 StyleBox[ButtonBox["Dirac fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Fermion"],
  FontSlant->"Italic"],
 " modes only when \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"u", "\[VeryThinSpace]", 
      SuperscriptBox["u", "\[Dagger]"]}], "+", 
     RowBox[{"v", "\[VeryThinSpace]", 
      SuperscriptBox["v", "\[Dagger]"]}]}], "=", "I"}], TraditionalForm]],
  ExpressionUUID->"46e1e1ff-510a-473d-a554-3b11c87a2837"],
 "   and   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"u", "\[VeryThinSpace]", 
      SuperscriptBox["v", "T"]}], "+", 
     RowBox[{"v", "\[VeryThinSpace]", 
      SuperscriptBox["u", "T"]}]}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"63e34212-90a4-4792-905a-5b14b5463b1d"],
 ". \[LineSeparator]If the ",
 StyleBox[ButtonBox["Dirac fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Fermion"],
  FontSlant->"Italic"],
 " modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "k"], TraditionalForm]],ExpressionUUID->
  "b21a0ce9-709a-4ff8-a786-51aa08e7b3c2"],
 " are expressed in terms of ",
 StyleBox[ButtonBox["Majorana fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/MajoranaFermions"],
  FontSlant->"Italic"],
 " modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", 
    RowBox[{
     RowBox[{"2", "k"}], "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "0aa1bb98-1871-4036-b19b-c7edfd730c0f"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", 
    RowBox[{"2", "k"}]], TraditionalForm]],ExpressionUUID->
  "b57c2911-a0b4-41df-a83c-a5ebf05dbe54"],
 ", then the dressed modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "k"], TraditionalForm]],ExpressionUUID->
  "6ab2da53-bd31-43a9-8f11-04d2bd99e0a3"],
 " read as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "i"], ":=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"j", "=", "1"}], 
      RowBox[{"2", "n"}]], "[", 
     RowBox[{
      RowBox[{
       SubscriptBox[
        RowBox[{"(", 
         RowBox[{"Re", "\[VeryThinSpace]", "m"}], ")"}], 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
       SubscriptBox["c", "j"]}], "+", 
      RowBox[{
       SubscriptBox[
        RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "(", 
         RowBox[{"Im", "\[VeryThinSpace]", "m"}], ")"}], 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
       SubscriptBox["c", "j"]}]}], "]"}]}], TraditionalForm]],ExpressionUUID->
  "a5d97784-2d18-4644-ac66-5a3feb48ca23"],
 " .\[LineSeparator]This defines dressed ",
 StyleBox[ButtonBox["Majorana fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/tutorial/MajoranaFermions"],
  FontSlant->"Italic"],
 " modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", 
    RowBox[{
     RowBox[{"2", "i"}], "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "14e05f76-7500-47df-aab9-ded3076241be"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", 
    RowBox[{"2", "i"}]], TraditionalForm]],ExpressionUUID->
  "dc19a2bf-47fe-469d-a368-1022bd03d1bf"],
 " by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", 
     RowBox[{
      RowBox[{"2", "i"}], "-", "1"}]], ":=", 
    RowBox[{
     SubscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"j", "=", "1"}], 
        RowBox[{"2", "n"}]], "(", 
       RowBox[{"Re", "\[VeryThinSpace]", "m"}], ")"}], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     SubscriptBox["c", "j"]}]}], TraditionalForm]],ExpressionUUID->
  "57abffb0-a400-4379-940c-c4c7abba5ef0"],
 " ,    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", 
     RowBox[{"2", "i"}]], ":=", 
    RowBox[{
     SubscriptBox[
      RowBox[{
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"j", "=", "1"}], 
        RowBox[{"2", "n"}]], "(", 
       RowBox[{"Im", "\[VeryThinSpace]", "m"}], ")"}], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     SubscriptBox["c", "j"]}]}], TraditionalForm]],ExpressionUUID->
  "1b5afe9d-05a4-418b-a17e-9216e69d6f46"],
 " .\[LineSeparator]Furthermore, the previous conditions are equivalent to \
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"w", "\[VeryThinSpace]", 
     SuperscriptBox["w", "T"]}], "=", "I"}], TraditionalForm]],ExpressionUUID->
  "e5af0332-b5fc-48c2-b158-47f6e7a64805"],
 "    with    ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", ":=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{"Re", "\[VeryThinSpace]", "m"}]},
       {
        RowBox[{"Im", "\[VeryThinSpace]", "m"}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "73308104-afad-4fed-ac10-8d168cbab3a4"],
 " ."
}], "Notes",
 CellID->1847349677,ExpressionUUID->"8b208612-1354-485f-8dad-6b2e778da2d3"],

Cell[TextData[{
 "The dressed ",
 StyleBox[ButtonBox["Dirac fermion",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Fermion"],
  FontSlant->"Italic"],
 " modes associated with ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["WickMeasurement"]], 
     "paclet:QuantumMob/Q3/ref/WickMeasurement", "QuantumMob Package Symbol"},
    
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   StyleBox["m", "TI"], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "ccfbf9bd-a1f5-4c14-bfe4-068ed52641a2"],
 " for a matrix ",
 Cell[BoxData[
  StyleBox["m", "TI"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "5d89939b-b936-4f91-a411-3db0da87c88a"],
 " may be ",
 StyleBox["unnormalized",
  FontSlant->"Italic"],
 ". That is, the dressed Dirac fermion modes ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox[
    StyleBox["b", "TI"], 
    StyleBox["i", "TI"]], ":=", 
   RowBox[{
    SubscriptBox[
     StyleBox["\[CapitalSigma]", "TR"], 
     StyleBox["j", "TI"]], "\[VeryThinSpace]", 
    SubscriptBox[
     StyleBox["m", "TI"], 
     StyleBox["ij", "TI"]], "\[VeryThinSpace]", 
    SubscriptBox[
     StyleBox["c", "TI"], 
     StyleBox["j", "TI"]]}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "d1748058-b9fe-4bbc-a9c3-b126a7b657c6"],
 " may have additional factor ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["f", "TI"], 
   StyleBox["i", "TI"]]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "f46d95a6-290d-4ede-95fb-b832220e948b"],
 " in the anti-commutation relations, ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["b", "TI"], 
      StyleBox["i", "TI"]], ",", 
     SubsuperscriptBox[
      StyleBox["b", "TI"], 
      StyleBox["j", "TI"], "\[Dagger]"]}], "}"}], "=", 
   RowBox[{
    SubscriptBox[
     StyleBox["f", "TI"], 
     StyleBox["i", "TI"]], "\[VeryThinSpace]", 
    SubscriptBox[
     StyleBox["\[Delta]", "TR"], 
     StyleBox["ij", "TI"]]}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "a950c6b0-2ffe-421b-a8ad-f10aa34e1b13"],
 ". In order for ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox[
    StyleBox["f", "TI"], 
    StyleBox["i", "TI"]], "=", "1"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "e0756195-30ac-4098-90bb-544027e1a920"],
 ", each row ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox[
    StyleBox["m", "TI"], 
    StyleBox["i", "TI"]], ":=", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["m", "TI"], 
      StyleBox[
       RowBox[{
        StyleBox["i", "TI"], 
        StyleBox["1", "TR"]}],
       ZeroWidthTimes->True]], ",", 
     SubscriptBox[
      StyleBox["m", "TI"], 
      StyleBox[
       RowBox[{
        StyleBox["i", "TI"], 
        StyleBox["2", "TR"]}],
       ZeroWidthTimes->True]], ",", 
     StyleBox["\[Ellipsis]", "TR"], ",", 
     SubscriptBox[
      StyleBox["m", "TI"], 
      StyleBox[
       RowBox[{"i", ",", 
        RowBox[{"2", "n"}]}], "TI"]]}], "}"}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "f2e5cf8c-dc39-4f32-9c52-f5b2a476b9ad"],
 " must be normalized such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"\[LeftDoubleBracketingBar]", 
      SubscriptBox["m", "i"], "\[RightDoubleBracketingBar]"}], "2"], "=", 
    RowBox[{"1", "/", "2"}]}], TraditionalForm]],ExpressionUUID->
  "c4642494-8882-42b6-886e-3a2f09b40234"],
 " (",
 StyleBox["not",
  FontSlant->"Italic"],
 " 1)."
}], "Notes",
 CellID->1083981116,ExpressionUUID->"a694d1d5-a009-4b24-9860-a875eafe4846"],

Cell[TextData[{
 "The normalization of ",
 Cell[BoxData[
  RowBox[{
   StyleBox["msr", "TI"], ":=", 
   RowBox[{"WickMeasurement", "[", 
    StyleBox["m", "TI"], "]"}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "0cb33bc5-5059-4319-ab34-3dcf19de0ab5"],
