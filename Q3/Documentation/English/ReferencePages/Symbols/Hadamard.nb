Cell[TextData[{
 "When applied to each qubit in state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"0"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "e9eec4e8-74af-483b-9ad3-090642a6462a"],
 ", the Hadamard gate generates a linear superposition of all states in the \
computational basis,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]], 
     SuperscriptBox[
      TemplateBox[{"0"},
       "Ket"], 
      RowBox[{"\[CircleTimes]", "n"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", "\[CenterEllipsis]", "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      TemplateBox[{"x"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "daa6d6f4-45b6-49f3-acfb-8a638a6f3a72"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["x", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "2"]},
      "Ket"], "\[CircleTimes]", "\[CenterEllipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "de4ef974-2635-4234-9b74-7177953f060d"],
 " for an integer ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "2e5d3922-7b3e-4afd-83b7-7274947dca4b"],
 " represented by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], 
       SubscriptBox["x", "2"], 
       SubscriptBox["\[Ellipsis]x", "n"]}], ")"}], "2"]}], TraditionalForm]],
  ExpressionUUID->"5914b716-bbc4-49f3-8f4f-ae568d9dbd40"],
 " in the binary digits."
}], "Notes",
 CellID->1964087756,ExpressionUUID->"0544fea2-30f0-46ae-9e86-89cf035fd156"],

