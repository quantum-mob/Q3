Cell[TextData[{
 "When applied to each qubit in state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"0"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "3b979d31-dde1-487a-bed9-0849f34e7ded"],
 ", the Hadamard gate generates a linear superposition of all states in the \
computational basis,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]], 
     SuperscriptBox[
      TemplateBox[{"0"},
       "Ket"], 
      RowBox[{"\[CircleTimes]", "n"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", "\[CenterEllipsis]", "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      TemplateBox[{"x"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "80f5928b-3329-4358-84bf-b7adb2c576f2"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["x", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "2"]},
      "Ket"], "\[CircleTimes]", "\[CenterEllipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "c16f50b4-2eae-4f56-a918-e87c6b9359e5"],
 " for an integer ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "00dc7aac-0f0b-4a1c-ac22-03419660cc3c"],
 " represented by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], 
       SubscriptBox["x", "2"], 
       SubscriptBox["\[Ellipsis]x", "n"]}], ")"}], "2"]}], TraditionalForm]],
  ExpressionUUID->"3d24295d-c72b-4e8f-aae8-9de21addd24e"],
 " in the binary digits."
}], "Notes",
 CellID->1964087756,ExpressionUUID->"d16dc0c6-3e4f-403d-b637-43ca6a7760a5"],

