  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      TemplateBox[{
        Cell[
         TextData["ArrayFlatten"]], "paclet:ref/ArrayFlatten"},
       "RefLink",
       BaseStyle->{"InlineFormula"}], "[", 
      RowBox[{
       RowBox[{"{", 
