Cell[TextData[{
 "In the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle Green\[CloseCurlyQuote]s function is represented \
by the ",
 Cell[BoxData[
  RowBox[{"2", 
   StyleBox["n", "TI"], "\[Times]", "2", 
   StyleBox["n", "TI"]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "51048786-1fd5-4c5e-b788-bb8be35451eb"],
 " matrix\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["G", "_"], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       UnderscriptBox["a", "_"], 
       SuperscriptBox[
        UnderscriptBox["a", "_"], "\[Dagger]"]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"G", "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], 
          RowBox[{"I", "-", 
           SuperscriptBox["G", "T"]}]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"I", "-", 
           SuperscriptBox["C", "T"]}], "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], "C"}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "14b956b1-343c-4024-bfd1-0031bd36876b"],
 ",\[LineSeparator]where  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["a", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["a", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["a", "n", "\[Dagger]"], ",", 
      SubscriptBox["a", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["a", "n"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "4bfb0a5f-a23a-4333-a45c-c3eb4a375300"],
 " is the Nambu spinor of the fermion modes."
}], "Notes",
 CellID->795234925,ExpressionUUID->"c0f5b145-9687-468f-a221-ce362ed9a7e3"],

Cell[TextData[{
 "Similarly, in the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle correlation matrix has the form\[LineSeparator]\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["C", "_"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"I", "0"},
         {"0", "0"}
        }], ")"}], "-", 
      UnderscriptBox["G", "_"]}], "=", 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["C", "T"], 
         RowBox[{"-", "F"}]},
        {
         RowBox[{"-", 
          SuperscriptBox["F", "\[Dagger]"]}], 
         RowBox[{"-", "C"}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "c30dba53-6ccc-47f2-a05a-3b82ea16638d"],
 " ."
}], "Notes",
 CellID->775823808,ExpressionUUID->"e008c2b1-30e5-4eab-b091-73c2fa9280b3"],

