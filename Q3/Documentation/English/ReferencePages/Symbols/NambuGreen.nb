Cell[TextData[{
 "In the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle Green\[CloseCurlyQuote]s function is represented \
by the ",
 Cell[BoxData[
  RowBox[{"2", 
   StyleBox["n", "TI"], "\[Times]", "2", 
   StyleBox["n", "TI"]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "2bde2b6b-5bea-45cd-9b4d-f95c45695953"],
 " matrix\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["G", "_"], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       UnderscriptBox["a", "_"], 
       SuperscriptBox[
        UnderscriptBox["a", "_"], "\[Dagger]"]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"G", "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], 
          RowBox[{"I", "-", 
           SuperscriptBox["G", "T"]}]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"I", "-", 
           SuperscriptBox["C", "T"]}], "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], "C"}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "30dfb2c4-f18c-41b4-ae7b-fbf6f97e4eae"],
 ",\[LineSeparator]where  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["a", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["a", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["a", "n", "\[Dagger]"], ",", 
      SubscriptBox["a", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["a", "n"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "2a00b2a1-330b-48a0-9b7a-469b5497bd07"],
 " is the Nambu spinor of the fermion modes."
}], "Notes",
 CellID->795234925,ExpressionUUID->"e89bd9f1-394a-41fd-a164-99c81d21b12c"],

Cell[TextData[{
 "Similarly, in the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle correlation matrix has the form\[LineSeparator]\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["C", "_"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"I", "0"},
         {"0", "0"}
        }], ")"}], "-", 
      UnderscriptBox["G", "_"]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"G", "-", "I"}], "F"},
          {
           SuperscriptBox["F", "\[Dagger]"], 
           RowBox[{"I", "-", 
            SuperscriptBox["G", "T"]}]}
         }], ")"}]}], "=", 
      RowBox[{"-", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"-", 
            SuperscriptBox["C", "T"]}], "F"},
          {
           SuperscriptBox["F", "\[Dagger]"], "C"}
         }], ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "fac2c4ff-d869-4955-8232-1264b79bef7f"],
 " ."
}], "Notes",
 CellID->775823808,ExpressionUUID->"22515bc9-089d-49b1-b538-1b5ed75d0b14"],

