Cell[TextData[{
 "In the Nambu notation, the full single-particle Green\[CloseCurlyQuote]s \
function is defined to be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["G", "_"], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       UnderscriptBox["c", "_"], 
       SuperscriptBox[
        UnderscriptBox["c", "_"], "\[Dagger]"]}], "\[RightAngleBracket]"}], 
     "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"G", "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], 
          RowBox[{"I", "-", 
           SuperscriptBox["G", "T"]}]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"I", "-", 
           SuperscriptBox["C", "T"]}], "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], "C"}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "81092f1b-92d6-4b5d-bbcb-c5a2b608a117"],
 ", where  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["c", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["c", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["c", "n", "\[Dagger]"], ",", 
      SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["c", "n"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "c45160ad-31a5-4e32-87e2-f26d3d1bba21"],
 " is the Nambu spinor of the fermion modes."
}], "Notes",
 CellID->795234925,ExpressionUUID->"c0f5b145-9687-468f-a221-ce362ed9a7e3"],

Cell[TextData[{
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Normal"]], "paclet:ref/Normal"},
    "RefLink",
    BaseStyle->{"InlineFormula"}], "[", 
