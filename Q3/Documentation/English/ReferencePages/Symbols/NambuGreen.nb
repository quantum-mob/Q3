Cell[TextData[{
 "In the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle Green\[CloseCurlyQuote]s function is represented \
by the ",
 Cell[BoxData[
  RowBox[{"2", 
   StyleBox["n", "TI"], "\[Times]", "2", 
   StyleBox["n", "TI"]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "21df4086-18b6-4c95-aca4-5810001edea8"],
 " matrix\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["G", "_"], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       UnderscriptBox["a", "_"], 
       SuperscriptBox[
        UnderscriptBox["a", "_"], "\[Dagger]"]}], "\[RightAngleBracket]"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"G", "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], 
          RowBox[{"I", "-", 
           SuperscriptBox["G", "T"]}]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"I", "-", 
           SuperscriptBox["C", "T"]}], "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], "C"}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "e2da03b1-24fb-4995-bee4-60abec9e0dd4"],
 ",\[LineSeparator]where  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["a", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["a", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["a", "n", "\[Dagger]"], ",", 
      SubscriptBox["a", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["a", "n"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "b928493f-05e3-4523-a416-46e1f4a56d90"],
 " is the Nambu spinor of the fermion modes."
}], "Notes",
 CellID->795234925,ExpressionUUID->"0c29d089-9662-4d10-88fa-599a52c54631"],

Cell[TextData[{
 "Similarly, in the ",
 StyleBox["Nambu space",
  FontSlant->"Italic"],
 ", the full single-particle correlation matrix has the form\[LineSeparator]\t\
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["C", "_"], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"I", "0"},
         {"0", "0"}
        }], ")"}], "-", 
      UnderscriptBox["G", "_"]}], "=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"G", "-", "I"}], "F"},
          {
           SuperscriptBox["F", "\[Dagger]"], 
           RowBox[{"I", "-", 
            SuperscriptBox["G", "T"]}]}
         }], ")"}]}], "=", 
      RowBox[{"-", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"-", 
            SuperscriptBox["C", "T"]}], "F"},
          {
           SuperscriptBox["F", "\[Dagger]"], "C"}
         }], ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "e44ded47-5b72-4aa2-8424-1698629a2377"],
 " ."
}], "Notes",
 CellID->775823808,ExpressionUUID->"846f970a-7302-4d1d-9bee-00c46d24daac"],

