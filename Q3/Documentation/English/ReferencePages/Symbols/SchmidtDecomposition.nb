  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["w", "j"]},
        "Ket"], ":", "j"}], "=", "1"}], ",", "\[Ellipsis]", ",", "n"}], "}"}],
