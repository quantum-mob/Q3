Cell[TextData[{
 "The ",
 StyleBox[ButtonBox["Schmidt decomposition",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/SchmidtDecomposition"],
  FontSlant->"Italic"],
 " of a state vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[CapitalPsi]"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "18d66731-3eee-4756-bc8c-89ec3accb861"],
 " in a tensor-product space ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalV]", "\[CircleTimes]", "\[ScriptCapitalW]"}], 
   TraditionalForm]],ExpressionUUID->"3a615b19-adfa-4b67-b61b-cb32d5356060"],
 " is to rewrite ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"\[CapitalPsi]"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "a03e8767-2e42-46d3-9af1-f05049ec6997"],
 " in the least number of terms of the form\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"\[CapitalPsi]"},
     "Ket"], "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], "R"], 
     RowBox[{
      TemplateBox[{
        SubscriptBox["v", "k"]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{
        SubscriptBox["w", "k"]},
       "Ket"]}], 
     SubscriptBox["s", "k"]}]}], TraditionalForm]],ExpressionUUID->
  "c8f91aae-caa7-44dc-9b22-ebd0d81a6c0c"],
 " ,\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["v", "i"]},
        "Ket"], ":", "i"}], "=", "1"}], ",", "\[Ellipsis]", ",", "m"}], "}"}],
    TraditionalForm]],ExpressionUUID->
  "d5392cd9-b134-4562-ae05-f9c65c8c96b6"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["w", "j"]},
        "Ket"], ":", "j"}], "=", "1"}], ",", "\[Ellipsis]", ",", "n"}], "}"}],
    TraditionalForm]],ExpressionUUID->
  "3d653612-e75c-4468-9237-692d526ac0b9"],
