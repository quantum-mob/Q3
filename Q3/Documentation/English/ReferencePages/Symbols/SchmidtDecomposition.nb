  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         SubscriptBox["v", "i"]},
        "Ket"], ":", "i"}], "=", "1"}], ",", "\[Ellipsis]", ",", "m"}], "}"}],
    TraditionalForm]],ExpressionUUID->
  "ecbba70b-8802-4a41-93eb-bb50098fd50b"],
 " and ",
