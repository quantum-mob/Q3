Cell[TextData[{
 "The ",
 StyleBox["covariance matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "d5f096a0-fec9-492a-8fe5-6e51da0f9581"],
