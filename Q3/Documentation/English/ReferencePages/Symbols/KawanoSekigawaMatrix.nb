Cell[TextData[{
 "In the Young-Fourier basis \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         RowBox[{"\[Lambda]", ",", 
           RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
        "Ket"], ":", 
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["\[ScriptCapitalS]", "n"], ")"}]}]}], ";", " ", "i"}], ",", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"f1ba16df-20fd-4b46-b606-e30e81e36576"],
 ", \nthe right regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "cad28c30-e124-47de-8466-53a77394592a"],
 " is block-diagonalized\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Lambda]"]], "\[CircleTimes]", 
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"2cc6537a-fda8-46b5-84eb-83304b6e5ec0"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"1711064c-950f-4d1a-a84e-900fb2099b8c"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"9e3446a2-b6da-438c-9243-a6d677b3e12e"],
 " are the irreducible representations of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "c5c94eaa-5a96-4a84-98a8-54cadebc4316"],
 "."
}], "Notes",
 CellID->1621858379,ExpressionUUID->"026bdcc2-69a4-491a-837f-2e619dead608"],

Cell[TextData[{
 "In the Kawano-Sekigawa basis\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Mu]", ",", 
          RowBox[{"k", ";", "\[Lambda]"}], ",", "\[VeryThinSpace]", "j"}]},
       "Ket"], ":", 
      RowBox[{"\[Mu]", "\[Element]", 
       RowBox[{"\[ScriptCapitalR]", "(", 
        SubscriptBox["\[ScriptCapitalS]", 
         RowBox[{"n", "-", "1"}]], ")"}]}]}], ";", 
     RowBox[{"k", "\[Element]", "\[Mu]"}], ";", 
     RowBox[{"\[Lambda]", "\[Element]", 
      SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}], ";", " ", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"7c6b796c-91a6-4afc-bb36-654735de1390"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalC]", "\[Mu]"], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["S", "n"], ")"}]}], ":", "\[Mu]"}], "->", "\[Lambda]"}],
      "}"}]}], TraditionalForm]],ExpressionUUID->
  "5630570b-4386-4fd1-8148-73a977ec6748"],
 " is the set of children of \[Mu] in the Young-Bratteli diagram, the right \
regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "fd353c79-2617-4615-b192-64315ad7f3aa"],
 " is still block-diagonalized but in a slightly different form:\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Mu]"]], "\[CircleTimes]", 
      RowBox[{
       SubscriptBox["\[CirclePlus]", 
        RowBox[{"\[Lambda]", "\[Element]", 
         SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}]], 
       RowBox[{
        SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", 
        ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "d9c9e57a-9cbe-49ae-acb0-dbd7b7323c63"],
 "."
}], "Notes",
 CellID->999972632,ExpressionUUID->"b5de8dc7-ae45-4a55-a974-c0dc77ee5734"],

