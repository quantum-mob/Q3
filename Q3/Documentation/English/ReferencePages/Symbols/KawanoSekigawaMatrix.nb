Cell[TextData[{
 "In the Young-Fourier basis \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         RowBox[{"\[Lambda]", ",", 
           RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
        "Ket"], ":", 
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["\[ScriptCapitalS]", "n"], ")"}]}]}], ";", " ", "i"}], ",", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"cc945f52-c6e1-4d02-bf83-2eb8389da885"],
 ", \nthe right regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "4dc4aad8-a578-4484-870f-ed60e38be8cd"],
 " is block-diagonalized\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Lambda]"]], "\[CircleTimes]", 
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"3c765dbe-1875-4618-9337-2d70fec00d4b"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"9aa1e060-c831-4217-a65c-e3e8594e7919"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"c070d9e7-5045-434f-ac4c-d03a0ae45c47"],
 " are the irreducible representations of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "4b0ca1c4-4a7a-47b8-89c4-0fa0002264ed"],
 "."
}], "Notes",
 CellID->1621858379,ExpressionUUID->"cec0f89e-0e75-4232-9803-9481ab781d85"],

Cell[TextData[{
 "In the Kawano-Sekigawa basis\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Mu]", ",", 
          RowBox[{"k", ";", "\[Lambda]"}], ",", "\[VeryThinSpace]", "j"}]},
       "Ket"], ":", 
      RowBox[{"\[Mu]", "\[Element]", 
       RowBox[{"\[ScriptCapitalR]", "(", 
        SubscriptBox["\[ScriptCapitalS]", 
         RowBox[{"n", "-", "1"}]], ")"}]}]}], ";", 
     RowBox[{"k", "\[Element]", "\[Mu]"}], ";", 
     RowBox[{"\[Lambda]", "\[Element]", 
      SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}], ";", " ", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"52ca989a-f757-43ea-a11c-2cc5c8780e54"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalC]", "\[Mu]"], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["S", "n"], ")"}]}], ":", "\[Mu]"}], "->", "\[Lambda]"}],
      "}"}]}], TraditionalForm]],ExpressionUUID->
  "dc5b0546-8608-4c5e-b1be-5538398087d8"],
 " is the set of children of \[Mu] in the Young-Bratteli diagram, the right \
regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "d06573fe-1080-4fe7-94e2-99c9f618f7d2"],
 " is still block-diagonalized but in a slightly different form:\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Mu]"]], "\[CircleTimes]", 
      RowBox[{
       SubscriptBox["\[CirclePlus]", 
        RowBox[{"\[Lambda]", "\[Element]", 
         SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}]], 
       RowBox[{
        SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", 
        ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "c52cefb8-d762-4151-ab82-9bf37f2c377c"],
 "."
}], "Notes",
 CellID->999972632,ExpressionUUID->"f2651fa6-c966-4f58-97d9-cd4009107fda"],

