  FormBox[
   RowBox[{
    TemplateBox[{"a"},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["a", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["a", "2"]},
      "Ket"], "\[CircleTimes]", "\[Ellipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["a", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "702ed19c-f45d-408e-877c-66ad0dd64df8"],
 " and ",
