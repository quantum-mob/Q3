Cell[TextData[{
 "Any fermionic Gaussian state of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "2b7d09f1-780c-4ef3-8d74-f77be3d9a60d"],
 " fermion modes can be written as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     FractionBox["\[ScriptCapitalN]", 
      SuperscriptBox["2", "n"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{"k", "=", "1"}], "n"]}]}], TraditionalForm]],ExpressionUUID->
  "ad458117-d1bf-4d86-acb8-ee82cefb5619"],
 "(1+\[ImaginaryI]\[VeryThinSpace]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Lambda]", "k"], 
     SubscriptBox["c", 
      RowBox[{
       RowBox[{"2", "k"}], "-", "1"}]], 
     SubscriptBox["c", 
      RowBox[{"2", "k"}]]}], ")"}], TraditionalForm]],ExpressionUUID->
  "93d08c85-92ae-4dfc-a60b-e7a4db034fb5"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],ExpressionUUID->
  "1080c4bf-8794-4448-9edf-87ef2a8f7a07"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"2", "n"}]}], TraditionalForm]],ExpressionUUID->
  "c66c7f40-7077-4d69-bc7b-437af0032cf7"],
 ") are Majorana fermion operators corresponding to the ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "b65a1e71-07cd-451d-adfc-ac08f2aec393"],
 " Dirac fermion modes, ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalN]", TraditionalForm]],ExpressionUUID->
  "96af33e5-f444-479b-9462-1a9e19fe4ab8"],
 " is the norm square of the state (i.e., note that some quantum channels do \
not preserve the trace of the state and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "\[VeryThinSpace]", "\[Rho]"}], "=", "\[ScriptCapitalN]"}], 
   TraditionalForm]],ExpressionUUID->"2b9e5117-2452-4b5c-845b-8bc318dee409"],
 "), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
    SubscriptBox["\[Lambda]", "k"]}], TraditionalForm]],ExpressionUUID->
  "b2cbbf32-9dd3-4658-85c4-0c546b3cd9a7"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", "k"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{
      RowBox[{"-", "1"}], ",", "1"}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"f74ac65a-8538-4bcf-bce2-6397075d370e"],
 ") are eigenvalues of the ",
 StyleBox["covariance matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "b79a85b8-2daa-44bf-8356-a63a96719a9f"],
 ", which is a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n", "\[Times]", "2", "n"}], TraditionalForm]],ExpressionUUID->
  "d32ea347-e058-4b52-8338-50ed3673ad37"],
 " real anti-symmetric matrix (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", "T"], "=", 
    RowBox[{"-", "V"}]}], TraditionalForm]],ExpressionUUID->
  "db813b6f-7c8c-43d0-bc19-40e48344c062"],
 ") in the Majorana spinor space [",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData[
      StyleBox[{
        StyleBox["Bravyi", IgnoreSpellCheck -> True], " (2005)"}]]], 
    "https://dl.acm.org/doi/abs/10.5555/2011637.2011640"},
   "WebLink",
   BaseStyle->{"Notes"}]],ExpressionUUID->
  "9cb494e5-ddd2-4a76-b590-bf906cd0c25c"],
 "]."
}], "Notes",
 CellID->52239125,ExpressionUUID->"df232eb3-c9be-41c7-9b30-bc9921393313"],

Cell[TextData[{
 "Introducing ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n"}], TraditionalForm]],ExpressionUUID->
  "5eca8c15-c5a2-4fa9-b654-b28accfb278a"],
 " ",
 ButtonBox["Grassmann variables",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Grassmann"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]],ExpressionUUID->
  "b431ba2d-a814-47b7-aa9a-5eea37913bb1"],
 " respectively corresponding to the Majorana modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],ExpressionUUID->
  "5a7a91e7-06ee-4b7b-80bb-03741169ba20"],
 ", the fermionic Gaussian state reads as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "\[DotEqual]", 
    RowBox[{
     FractionBox["\[ScriptCapitalN]", 
      SuperscriptBox["2", "n"]], 
     RowBox[{"exp", "(", 
      RowBox[{
       FractionBox["\[ImaginaryI]", "2"], 
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"i", ",", 
         RowBox[{"j", "=", "1"}]}], 
        RowBox[{"2", "n"}]], 
       SubscriptBox["x", "i"], 
       SubscriptBox["V", 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
       SubscriptBox["x", "j"]}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "f7e86c98-3c28-4d8f-8862-87ccbc52d308"],
 " ,\[LineSeparator]expressed directly in terms of the covariance matrix ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "6d45739f-10ae-4b4b-bd17-da337d1ffe28"],
 " [",
 StyleBox[ButtonBox["Bravyi",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://dl.acm.org/doi/abs/10.5555/2011637.2011640"], None}],
  IgnoreSpellCheck->True],
 ButtonBox[" (2005)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://dl.acm.org/doi/abs/10.5555/2011637.2011640"], None}],
 "]."
}], "Notes",
 CellID->1331370085,ExpressionUUID->"38a801d4-57a4-4349-89b4-a13da0c94d99"],

Cell[TextData[{
 "The ",
