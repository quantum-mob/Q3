Cell[TextData[{
 "Any fermionic Gaussian state of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "acad143d-b768-4e4a-a08e-2a1692a7b575"],
 " fermion modes can be written as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "=", 
    RowBox[{
     FractionBox["\[ScriptCapitalN]", 
      SuperscriptBox["2", "n"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{"k", "=", "1"}], "n"]}]}], TraditionalForm]],ExpressionUUID->
  "7f9a3540-7010-4b14-ba31-9e8b241fb1d6"],
 "(1+\[ImaginaryI]\[VeryThinSpace]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Lambda]", "k"], 
     SubscriptBox["c", 
      RowBox[{
       RowBox[{"2", "k"}], "-", "1"}]], 
     SubscriptBox["c", 
      RowBox[{"2", "k"}]]}], ")"}], TraditionalForm]],ExpressionUUID->
  "a2bbc544-5d2f-4632-834f-bc69c97ae5b9"],
 ",\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],ExpressionUUID->
  "d9397e70-6377-410a-acaa-1220affb5319"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"2", "n"}]}], TraditionalForm]],ExpressionUUID->
  "fbf881e4-c997-4dc7-8f27-3ee1239f8205"],
 ") are Majorana fermion operators corresponding to the ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "1c0c3730-817c-4116-94c2-b10c442ecbe8"],
 " Dirac fermion modes, ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalN]", TraditionalForm]],ExpressionUUID->
  "712e9d5c-ea83-4a25-8525-c85c637e6c13"],
 " is the norm square of the state (i.e., note that some quantum channels do \
not preserve the trace of the state and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "\[VeryThinSpace]", "\[Rho]"}], "=", "\[ScriptCapitalN]"}], 
   TraditionalForm]],ExpressionUUID->"fd184165-c767-4751-ba71-c9416d3fd704"],
 "), and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
    SubscriptBox["\[Lambda]", "k"]}], TraditionalForm]],ExpressionUUID->
  "b6784038-256f-4cca-b963-ffb7c67115ab"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", "k"], "\[Element]", 
    RowBox[{"[", 
     RowBox[{
      RowBox[{"-", "1"}], ",", "1"}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"1cf9709b-5654-449e-9179-a59c579ca19b"],
 ") are eigenvalues of the ",
 StyleBox["covariance matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "fa18828a-8869-4143-8fe0-aa33b30acc6e"],
 ", which is a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n", "\[Times]", "2", "n"}], TraditionalForm]],ExpressionUUID->
  "c9dbcf12-5c13-4028-b532-9a0a6ac4cd7e"],
 " real anti-symmetric matrix (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", "T"], "=", 
    RowBox[{"-", "V"}]}], TraditionalForm]],ExpressionUUID->
  "a177a13e-5906-4db7-9f3a-570c857cd395"],
 ") in the Majorana spinor space [",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData[
      StyleBox[{
        StyleBox["Bravyi", IgnoreSpellCheck -> True], " (2005)"}]]], 
    "https://dl.acm.org/doi/abs/10.5555/2011637.2011640"},
   "WebLink",
   BaseStyle->{"Notes"}]],ExpressionUUID->
  "07b55e2c-5d68-4418-9377-b16f25651d01"],
 "]."
}], "Notes",
 CellID->52239125,ExpressionUUID->"227440ee-1010-4a81-affe-84808655af89"],

Cell[TextData[{
 "Introducing ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n"}], TraditionalForm]],ExpressionUUID->
  "3b4a0882-4be5-4e98-bbde-8e65d053416c"],
 " ",
 ButtonBox["Grassmann variables",
  BaseStyle->"Link",
  ButtonData->"paclet:QuantumMob/Q3/ref/Grassmann"],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]],ExpressionUUID->
  "3ead87f9-58e8-42fd-90c2-b3c156087f60"],
 " respectively corresponding to the Majorana modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],ExpressionUUID->
  "27d4f63f-1d91-4d1d-9807-0627a3401350"],
 ", the fermionic Gaussian state reads as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "\[DotEqual]", 
    RowBox[{
     FractionBox["\[ScriptCapitalN]", 
      SuperscriptBox["2", "n"]], 
     RowBox[{"exp", "(", 
      RowBox[{
       FractionBox["\[ImaginaryI]", "2"], 
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"i", ",", 
         RowBox[{"j", "=", "1"}]}], 
        RowBox[{"2", "n"}]], 
       SubscriptBox["x", "i"], 
       SubscriptBox["V", 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
       SubscriptBox["x", "j"]}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "a1320216-76f3-41ab-b527-e55183c7f7d8"],
 " ,\[LineSeparator]expressed directly in terms of the covariance matrix ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "b6f2c059-ec0f-4052-b8a2-6d363cdd05fb"],
 " [",
 StyleBox[ButtonBox["Bravyi",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://dl.acm.org/doi/abs/10.5555/2011637.2011640"], None}],
  IgnoreSpellCheck->True],
 ButtonBox[" (2005)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://dl.acm.org/doi/abs/10.5555/2011637.2011640"], None}],
 "]."
}], "Notes",
 CellID->1331370085,ExpressionUUID->"789110bd-0aa6-4bd9-9e1a-c62426102256"],

Cell[TextData[{
 "The ",
 StyleBox["covariance matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox["V", TraditionalForm]],ExpressionUUID->
  "0f16357f-ae3a-49b5-a02f-83a268a91c3c"],
