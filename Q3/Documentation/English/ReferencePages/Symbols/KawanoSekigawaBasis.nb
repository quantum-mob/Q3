Cell[TextData[{
 "In the Young-Fourier basis \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         RowBox[{"\[Lambda]", ",", 
           RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
        "Ket"], ":", 
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["\[ScriptCapitalS]", "n"], ")"}]}]}], ";", " ", "i"}], ",", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"42ae7089-6d5d-46df-b154-9b1c8e6bebbc"],
 ", \nthe right regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "dca82f37-d936-4b89-bd1f-684f14832efe"],
 " is block-diagonalized\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Lambda]"]], "\[CircleTimes]", 
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"eb32413a-cd98-4a38-bdbf-d8cc67a3606e"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"bc744cd1-46fe-4a00-bf37-d42aa4774cd2"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"d5fbe2b1-72f2-41f1-a2a1-b2669c8a95ad"],
 " are the irreducible representations of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "8ce27fea-6d6d-48e6-a917-da4c2e2ca73e"],
 "."
}], "Notes",
 CellID->1621858379,ExpressionUUID->"1d0547fc-8a20-44e8-b0b3-4988d2e878ee"],

Cell[TextData[{
 "In the Kawano-Sekigawa basis\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Mu]", ",", 
          RowBox[{"k", ";", "\[Lambda]"}], ",", "\[VeryThinSpace]", "j"}]},
       "Ket"], ":", 
      RowBox[{"\[Mu]", "\[Element]", 
       RowBox[{"\[ScriptCapitalR]", "(", 
        SubscriptBox["\[ScriptCapitalS]", 
         RowBox[{"n", "-", "1"}]], ")"}]}]}], ";", 
     RowBox[{"k", "\[Element]", "\[Mu]"}], ";", 
     RowBox[{"\[Lambda]", "\[Element]", 
      SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}], ";", " ", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"f210801f-df4e-42c2-8b20-4d6f43a3e59d"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalC]", "\[Mu]"], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["S", "n"], ")"}]}], ":", "\[Mu]"}], "->", "\[Lambda]"}],
      "}"}]}], TraditionalForm]],ExpressionUUID->
  "ef980e89-3061-4986-ae35-379738186e9d"],
 " is the set of children of \[Mu] in the Young-Bratteli diagram, the right \
regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "edacb1d7-e62f-49ca-a43d-5adca44c0584"],
 " is still block-diagonalized but in a slightly different form:\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Mu]"]], "\[CircleTimes]", 
      RowBox[{
       SubscriptBox["\[CirclePlus]", 
        RowBox[{"\[Lambda]", "\[Element]", 
         SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}]], 
       RowBox[{
        SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", 
        ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "84eed760-f3db-4771-bf41-fca4e736584b"],
 "."
}], "Notes",
 CellID->999972632,ExpressionUUID->"ea0fcc64-b6c2-4edd-aa92-761b37992e81"],

