Cell[TextData[{
 "In the Young-Fourier basis \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         RowBox[{"\[Lambda]", ",", 
           RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
        "Ket"], ":", 
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["\[ScriptCapitalS]", "n"], ")"}]}]}], ";", " ", "i"}], ",", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"8fdd836d-d4f1-4930-bc97-3ef141da24e1"],
 ", \nthe right regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "16c17561-317a-48af-b8af-eded13271ade"],
 " is block-diagonalized\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Lambda]"]], "\[CircleTimes]", 
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"6b1fe681-daa2-4ecc-9438-b0544d76a3cb"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"b6a33f9f-5776-497d-8a3b-a488442a047f"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"4ab255df-63af-40ca-a451-a28bcfc0ad1d"],
 " are the irreducible representations of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "fa7f5688-1d58-478f-8c42-2b187a2dee04"],
 "."
}], "Notes",
 CellID->1621858379,ExpressionUUID->"cfd07e92-193c-4748-bb50-9ce17bd08bd5"],

Cell[TextData[{
 "In the Kawano-Sekigawa basis\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Mu]", ",", 
          RowBox[{"k", ";", "\[Lambda]"}], ",", "\[VeryThinSpace]", "j"}]},
       "Ket"], ":", 
      RowBox[{"\[Mu]", "\[Element]", 
       RowBox[{"\[ScriptCapitalR]", "(", 
        SubscriptBox["\[ScriptCapitalS]", 
         RowBox[{"n", "-", "1"}]], ")"}]}]}], ";", 
     RowBox[{"k", "\[Element]", "\[Mu]"}], ";", 
     RowBox[{"\[Lambda]", "\[Element]", 
      SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}], ";", " ", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"eed51d28-b621-44c5-a5a6-45e33a7e91da"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalC]", "\[Mu]"], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["S", "n"], ")"}]}], ":", "\[Mu]"}], "->", "\[Lambda]"}],
      "}"}]}], TraditionalForm]],ExpressionUUID->
  "e7eef36d-9daf-4018-ba60-f0edc2ead71f"],
 " is the set of children of \[Mu] in the Young-Bratteli diagram, the right \
regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "391bc72e-b2e6-406d-b360-a30c39eb0805"],
 " is still block-diagonalized but in a slightly different form:\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Mu]"]], "\[CircleTimes]", 
      RowBox[{
       SubscriptBox["\[CirclePlus]", 
        RowBox[{"\[Lambda]", "\[Element]", 
         SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}]], 
       RowBox[{
        SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", 
        ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "7e4caf69-baf9-4c4a-9402-a92ec9582019"],
 "."
}], "Notes",
 CellID->999972632,ExpressionUUID->"5117ab49-4991-401a-adcb-c6e5a8695ffe"],

