Cell[TextData[{
 "In the Young-Fourier basis \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       TemplateBox[{
         RowBox[{"\[Lambda]", ",", 
           RowBox[{"i", "\[VeryThinSpace]", "j"}]}]},
        "Ket"], ":", 
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["\[ScriptCapitalS]", "n"], ")"}]}]}], ";", " ", "i"}], ",", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"91410abe-fc44-4b29-9f2c-c21c6d9407d8"],
 ", \nthe right regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "23b70225-d7aa-456d-be22-482ec0190d12"],
 " is block-diagonalized\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Lambda]"]], "\[CircleTimes]", 
      RowBox[{
       SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"b7d200fd-c67f-4ddf-ae40-9400fb68205a"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "\[Element]", 
    SubscriptBox["\[ScriptCapitalS]", "n"]}], TraditionalForm]],
  ExpressionUUID->"710c9026-8987-4ec4-b6fa-024a9ba23679"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], TraditionalForm]],
  ExpressionUUID->"a35cfe8a-e814-45a2-8c8a-725951f06828"],
 " are the irreducible representations of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "237c3781-ae3d-4e8b-84d2-4e87f8f9c659"],
 "."
}], "Notes",
 CellID->1621858379,ExpressionUUID->"17e13660-e007-4dce-8722-df082c00bed1"],

Cell[TextData[{
 "In the Kawano-Sekigawa basis\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Mu]", ",", 
          RowBox[{"k", ";", "\[Lambda]"}], ",", "\[VeryThinSpace]", "j"}]},
       "Ket"], ":", 
      RowBox[{"\[Mu]", "\[Element]", 
       RowBox[{"\[ScriptCapitalR]", "(", 
        SubscriptBox["\[ScriptCapitalS]", 
         RowBox[{"n", "-", "1"}]], ")"}]}]}], ";", 
     RowBox[{"k", "\[Element]", "\[Mu]"}], ";", 
     RowBox[{"\[Lambda]", "\[Element]", 
      SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}], ";", " ", 
     RowBox[{"j", "\[Element]", "\[Lambda]"}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"9e4535c3-0d69-44cc-ad52-b209811574cb"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[ScriptCapitalC]", "\[Mu]"], ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Lambda]", "\[Element]", 
        RowBox[{"\[ScriptCapitalR]", "(", 
         SubscriptBox["S", "n"], ")"}]}], ":", "\[Mu]"}], "->", "\[Lambda]"}],
      "}"}]}], TraditionalForm]],ExpressionUUID->
  "df3d7e9a-0f55-43ae-9450-2b18b3bff77c"],
 " is the set of children of \[Mu] in the Young-Bratteli diagram, the right \
regular representation of the symmetric group ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalS]", "n"], TraditionalForm]],ExpressionUUID->
  "34f4cce7-9abd-4ae5-a449-40fde287594e"],
 " is still block-diagonalized but in a slightly different form:\
\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"R", "(", "g", ")"}], "=", 
    RowBox[{
     SubscriptBox["\[CirclePlus]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["I", 
       SubscriptBox["d", "\[Mu]"]], "\[CircleTimes]", 
      RowBox[{
       SubscriptBox["\[CirclePlus]", 
        RowBox[{"\[Lambda]", "\[Element]", 
         SubscriptBox["\[ScriptCapitalC]", "\[Mu]"]}]], 
       RowBox[{
        SuperscriptBox["\[CapitalGamma]", "\[Lambda]"], "(", "g", 
        ")"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "cade199f-52d7-4360-9a2d-12bfae2f7403"],
 "."
}], "Notes",
 CellID->999972632,ExpressionUUID->"9c43857b-10af-4e27-bbc6-5bc151ef2abf"],

