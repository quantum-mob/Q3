Cell[TextData[{
 "The (normal) Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "eee12963-59f1-434f-aa52-62b69d71096f"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["G", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubsuperscriptBox["c", "j", "\[Dagger]"]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"b8c855df-265c-4aca-98c2-1ed51e8fae06"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],ExpressionUUID->
  "ffa71442-c7f0-44f1-822e-ce3782313e56"],
 " is Hermitian, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["G", "\[Dagger]"], "=", "G"}], TraditionalForm]],
  ExpressionUUID->"2eea3bc0-607c-4dce-ae5f-14d34ed23ff9"],
 ". The ",
 StyleBox["correlation matrix",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["C", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SubsuperscriptBox["c", "i", "\[Dagger]"], 
       SubscriptBox["c", "j"]}], "\[RightAngleBracket]"}], "\[Congruent]", 
     SubscriptBox[
      RowBox[{"(", 
       RowBox[{"I", "-", 
        SuperscriptBox["G", "T"]}], ")"}], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}], TraditionalForm]],
  ExpressionUUID->"b33ff76f-ae33-4fa1-8177-a4a31bfa36d6"],
 " has equivalent information and is also frequently used."
}], "Notes",
 CellID->348061621,ExpressionUUID->"24e04b4f-b489-4b92-975e-e87f8c931c8a"],

Cell[TextData[{
