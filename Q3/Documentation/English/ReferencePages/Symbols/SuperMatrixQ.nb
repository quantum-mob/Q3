Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalF]", ":", 
    RowBox[{"\[Rho]", "\[RightTeeArrow]", "\[Sigma]"}]}], TraditionalForm]],
  ExpressionUUID->"0e6623e9-3c54-4e50-ad49-f2a70d3f064f"],
