  FormBox[
   RowBox[{
    RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
     SubscriptBox["c", 
      RowBox[{"2", "k"}]]}], ":=", 
    RowBox[{
     SubscriptBox["a", "k"], "-", 
