  FormBox[
   SubscriptBox["c", 
    RowBox[{
     RowBox[{"2", "k"}], "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "f4303b44-f3ed-4fc0-a62f-86768d7303a5"],
