Cell[TextData[{
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["NambuHermitian"]], "paclet:QuantumMob/Q3/ref/NambuHermitian", 
     "QuantumMob Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   RowBox[{"{", 
    RowBox[{
     StyleBox["H", "TI"], ",", 
     StyleBox["\[CapitalDelta]", "TI"]}], "}"}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "2e477b31-2847-4ee3-8762-bcf90092a179"],
 " represents the quadratic many-body Hamiltonian\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["H", "^"], ":=", 
    RowBox[{
     FractionBox["1", "2"], 
     SubscriptBox["\[CapitalSigma]", 
      RowBox[{"i", ",", "j"}]], 
     SubsuperscriptBox[
      UnderoverscriptBox["a", "_", "^"], "i", "\[Dagger]"], 
     SubscriptBox[
      UnderscriptBox["H", "_"], 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
     SubscriptBox[
      UnderoverscriptBox["a", "_", "^"], "j"]}]}], TraditionalForm]],
  ExpressionUUID->"e87cbb8f-5e2b-4c45-9c1b-24f0d3b67107"],
 " (notice the factor of 1/2)   with   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["H", "_"], ":=", 
    RowBox[{"(", GridBox[{
       {"H", "\[CapitalDelta]"},
       {
        SuperscriptBox["\[CapitalDelta]", "\[Dagger]"], 
        RowBox[{"-", 
         SuperscriptBox["H", "T"]}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "d2b8e7b9-2f77-4dcf-a53f-bdffd3d1af83"],
 "    and    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["a", "_", "^"], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox[
         OverscriptBox["a", "^"], "i"]},
       {
        SubsuperscriptBox[
         OverscriptBox["a", "^"], "i", "\[Dagger]"]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "2b648c44-df4a-478f-ae03-ce164bf56699"],
 "\[LineSeparator] for Dirac fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["a", "^"], "i"], TraditionalForm]],ExpressionUUID->
  "609f87e8-2a56-4c7b-b155-24c073eba4bc"],
 "."
}], "Notes",
 CellID->1744165731,ExpressionUUID->"515ee5c2-6eba-4857-8c64-bc5ea2b0dc3b"],

