Cell[TextData[{
 "Typically, quantum information scrambling is examined in terms of the \
out-of-time-order correlator (OTOC) \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["A", "\[Dagger]"], "(", "t", ")"}], 
      SuperscriptBox["B", "\[Dagger]"], 
      RowBox[{"A", "(", "t", ")"}], "B"}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"cbdcf251-ebef-40a3-96e1-0ecd8435ba27"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "fd18b51a-dc91-40d0-9623-38cfe022ce73"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "a370ced1-24fa-4842-96ce-d7d0ad64e4aa"],
 " are local unitary operators, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "t", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "(", "t", ")"}], "A", " ", 
     RowBox[{"U", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "aff2e6dc-11fb-4aa0-89be-6f6db9c79fa2"],
 " is the Heisenberg picture of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "ee98a6d5-9c1c-4fee-8488-6e549aa3c900"],
 " governed by the time-evolution operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "3b8e8c07-9b40-443c-b626-0f3395392330"],
 "(t)."
}], "Notes",
 CellID->491373739,ExpressionUUID->"29d27a9b-94b0-47b4-9925-90fa9d5ff559"],

Cell[TextData[{
 "The OTOC satisfies the following relation:\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{"C", "(", "t", ")"}], "]"}], "=", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"[", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], "\[Dagger]"], "[", 
        RowBox[{
         RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], 
       "\[RightAngleBracket]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "89edd35f-1968-439f-8113-50f2cf0955ae"],
 " ."
}], "Notes",
 CellID->984433309,ExpressionUUID->"7f815e56-2b02-4dab-a3f8-f419cb64c192"]
}, Dynamic[CurrentValue[
 EvaluationNotebook[], {TaggingRules, "Openers", "NotesSection"}, Closed]]]],

