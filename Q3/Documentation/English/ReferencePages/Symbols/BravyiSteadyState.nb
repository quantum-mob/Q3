Cell[BoxData[{
 RowBox[{"Clear", "[", "HatanoNelsonModel", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HatanoNelsonModel", "[", 
   RowBox[{"J_", ",", "\[Gamma]_", ",", "L_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ham", ",", "cff", ",", "jmp"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ham", "=", 
      RowBox[{"PlusTopple", "[", 
       RowBox[{
        RowBox[{"-", "J"}], "*", 
        RowBox[{"One", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ham", "=", 
      RowBox[{"BravyiHermitian", "[", 
       RowBox[{"NambuHermitian", "@", 
        RowBox[{"{", 
         RowBox[{"ham", ",", "0"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cff", "=", 
      RowBox[{
       RowBox[{"One", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], "+", 
       RowBox[{"I", "*", 
        RowBox[{"One", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
         "]"}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"cff", "=", 
      RowBox[{"ArrayFlatten", "@", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"cff", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "cff"}], "}"}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
