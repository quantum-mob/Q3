Cell[TextData[{
 "According to the Murnaghan-Nakayama rule, the character ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Chi]", "\[Lambda]"], "(", "\[Mu]", ")"}], 
   TraditionalForm]],ExpressionUUID->"28b8b58e-a7bd-4cd6-bb09-ebbb981d87cd"],
 " is given by the sum of the signatures of the leg lengths of rim hooks \
along the paths, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", 
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", "1", "\[CenterDot]", "1", "\[CenterDot]",
       "1"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", "1", "\[CenterDot]", "1", "\[CenterDot]",
       "1", "\[CenterDot]", "1"}], "+", "0"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"e46e4f78-7315-4721-877f-3973e60c5ca8"],
 ":"
}], "ExampleText",
