Cell[TextData[{
 "According to the Murnaghan-Nakayama rule, the character ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Chi]", "\[Lambda]"], "(", "\[Mu]", ")"}], 
   TraditionalForm]],ExpressionUUID->"24776cc1-d7dd-4f32-add8-850c3e832c3b"],
 " is given by the sum of the signatures of the leg lengths of rim hooks \
along the paths, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", 
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", "1", "\[CenterDot]", "1", "\[CenterDot]",
       "1"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "\[CenterDot]", "1", "\[CenterDot]", "1", "\[CenterDot]",
       "1", "\[CenterDot]", "1"}], "+", "0"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"e6d8b50a-2bc2-453a-8891-2f08f38abbf7"],
 ":"
}], "ExampleText",
