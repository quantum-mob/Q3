Cell[TextData[{
 "Typically, quantum information scrambling is examined in terms of the \
out-of-time-order correlator (OTOC) \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["A", "\[Dagger]"], "(", "t", ")"}], 
      SuperscriptBox["B", "\[Dagger]"], 
      RowBox[{"A", "(", "t", ")"}], "B"}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"7cf6690f-f382-4618-a34c-055b80fe43c7"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "565d9d71-ea11-4be5-ae93-fe5e75fa08b3"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "4beaef9d-b159-4407-a9d3-b3869a4a2039"],
 " are local unitary operators, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "t", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "(", "t", ")"}], "A", " ", 
     RowBox[{"U", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "7bdc8c36-d54a-42db-aa78-a4cd1626c6d9"],
 " is the Heisenberg picture of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "9c2ffabe-7548-4202-a942-aeb2bd6854d6"],
 " governed by the time-evolution operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "7461569a-b179-49a7-9d34-6ca8b582f662"],
 "(t)."
}], "Notes",
 CellID->491373739,ExpressionUUID->"bacc9602-22bd-4847-9ca7-fa3542bef77a"],

Cell[TextData[{
 "The OTOC satisfies the following relation:\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{"C", "(", "t", ")"}], "]"}], "=", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"[", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], "\[Dagger]"], "[", 
        RowBox[{
         RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], 
       "\[RightAngleBracket]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "1f96bab3-5765-4bfa-92b0-75691337eb1a"],
 " ."
}], "Notes",
 CellID->984433309,ExpressionUUID->"48dbf434-6160-4b07-99d3-aef1da9cbffe"],

