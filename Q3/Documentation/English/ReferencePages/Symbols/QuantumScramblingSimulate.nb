Cell[TextData[{
 "Typically, quantum information scrambling is examined in terms of the \
out-of-time-order correlator (OTOC) \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["A", "\[Dagger]"], "(", "t", ")"}], 
      SuperscriptBox["B", "\[Dagger]"], 
      RowBox[{"A", "(", "t", ")"}], "B"}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"f981ccb2-c4b4-4e3e-94c6-7d83abab2f98"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "24ed81ae-dc75-44e8-8ff4-4835300d39a5"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "7a5c25e0-8806-4ca4-b672-0cac8e8c6a10"],
 " are local unitary operators, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "t", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "(", "t", ")"}], "A", " ", 
     RowBox[{"U", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "1e642502-88eb-418d-865c-284e42246076"],
 " is the Heisenberg picture of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "a3efd75a-d8df-424a-b77a-952e1f342ce3"],
 " governed by the time-evolution operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "4ac508c1-17eb-4e6b-8ebc-3fd66a10f957"],
 "(t)."
}], "Notes",
 CellID->491373739,ExpressionUUID->"db9ea31e-dfd7-43d4-94c8-b58a329a75bb"],

Cell[TextData[{
 "The OTOC satisfies the following relation:\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{"C", "(", "t", ")"}], "]"}], "=", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"[", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], "\[Dagger]"], "[", 
        RowBox[{
         RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], 
       "\[RightAngleBracket]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a60c4016-6db1-4a77-91ca-4fb583cc3c9d"],
 " ."
}], "Notes",
 CellID->984433309,ExpressionUUID->"ff928eea-94a3-4cde-8bd7-edcccd587924"],

