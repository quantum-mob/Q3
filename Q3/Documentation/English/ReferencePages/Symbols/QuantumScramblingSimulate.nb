Cell[TextData[{
 "Typically, quantum information scrambling is examined in terms of the \
out-of-time-order correlator (OTOC) \[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"C", "(", "t", ")"}], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{
       SuperscriptBox["A", "\[Dagger]"], "(", "t", ")"}], 
      SuperscriptBox["B", "\[Dagger]"], 
      RowBox[{"A", "(", "t", ")"}], "B"}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"ba4ce98f-367d-4a87-aa62-46515fc81989"],
 ", \[LineSeparator]where ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "c5a5517d-ed93-45c5-aa7b-15ebb8378a43"],
 " and ",
 Cell[BoxData[
  FormBox["B", TraditionalForm]],ExpressionUUID->
  "1171c1ef-7966-4617-8cb2-4856efb1731c"],
 " are local unitary operators, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "t", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["U", "\[Dagger]"], "(", "t", ")"}], "A", " ", 
     RowBox[{"U", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "a3b9a882-a354-4372-ab86-822ac4c8cbc3"],
 " is the Heisenberg picture of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "1f27232b-4bd2-47c2-8d7c-9644aebdcac0"],
 " governed by the time-evolution operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "0b57ef83-6f5e-4d6f-882a-3824c1b3e43a"],
 "(t)."
}], "Notes",
 CellID->491373739,ExpressionUUID->"2a5716ae-c040-4941-a255-c69a57973927"],

Cell[TextData[{
 "The OTOC satisfies the following relation:\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{"C", "(", "t", ")"}], "]"}], "=", 
    RowBox[{"1", " ", "-", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"[", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], "\[Dagger]"], "[", 
        RowBox[{
         RowBox[{"A", "(", "t", ")"}], ",", "B"}], "]"}], 
       "\[RightAngleBracket]"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "cff0311e-216f-4438-a2de-d6b569081784"],
 " ."
}], "Notes",
 CellID->984433309,ExpressionUUID->"2df6b6ad-af1a-4cbf-903a-ab3ba42e6cd1"],

