Cell[TextData[{
 "Consider a many-body Hamiltonian\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["H", "^"], ":=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      SubscriptBox["\[CapitalSigma]", 
       RowBox[{"i", ",", "j"}]], 
      SuperscriptBox[
       UnderscriptBox["a", "_"], "\[Dagger]"], 
      SubscriptBox["H", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SubscriptBox[
       OverscriptBox["a", "^"], "j"]}], "=", 
     RowBox[{
      FractionBox["\[ImaginaryI]", "4"], 
      SubscriptBox["\[CapitalSigma]", 
       RowBox[{"i", ",", "j"}]], 
      SubscriptBox[
       OverscriptBox["c", "^"], "i"], 
      SubscriptBox["A", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SubscriptBox[
       OverscriptBox["c", "^"], "j"]}]}]}], TraditionalForm]],ExpressionUUID->
  "b9e89436-f18f-47a1-a103-ecd2304c3ffd"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "f1f4407e-6245-4305-b58e-2d31d909634d"],
 " is a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n", "\[Times]", "2", "n", " "}], TraditionalForm]],
  ExpressionUUID->"c8e97de8-532d-4aca-81c6-85e476b1b85d"],
 StyleBox["Hermitian",
  FontSlant->"Italic"],
 " matrix and ",
 Cell[BoxData[
