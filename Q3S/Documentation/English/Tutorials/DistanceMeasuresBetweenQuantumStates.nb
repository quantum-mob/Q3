Cell[TextData[{
 "\tWe define a function to construct the Kraus elements from the unitary \
representation of the quantum operation. Here ",
 StyleBox["U", "Input"],
 " is the unitary operator on the total system (system plus environment) and ",
 StyleBox["init", "Input"],
 " is the initial (pure) state of the environment. The system and environment \
are assumed to be initially in a product state."
}], "TechNoteText",
 CellID->1053796602,ExpressionUUID->"fb67c1dc-fb78-46f3-a3a3-60897d6509a9"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$d", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Clear", "[", "kraus", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"kraus", "[", 
   RowBox[{
    RowBox[{"U_", "?", "MatrixQ"}], ",", 
    RowBox[{"init_", "?", "VectorQ"}]}], "]"}], ":=", 
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"kraus", "[", 
      RowBox[{"U", ",", "init", ",", "#"}], "]"}], "&"}], ",", 
    RowBox[{"Range", "[", "$d", "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"kraus", "[", 
    RowBox[{
     RowBox[{"U_", "?", "MatrixQ"}], ",", 
     RowBox[{"init_", "?", "VectorQ"}], ",", "k_Integer"}], "]"}], ":=", 
   RowBox[{"PartialTrace", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"U", ".", 
      RowBox[{"CircleTimes", "[", 
       RowBox[{
        RowBox[{"One", "[", "$d", "]"}], ",", 
        RowBox[{"Dyad", "[", 
         RowBox[{"init", ",", 
          RowBox[{"UnitVector", "[", 
           RowBox[{"$d", ",", "k"}], "]"}]}], "]"}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"$d", ",", "$d"}], "}"}], ",", 
     RowBox[{"{", "2", "}"}]}], "]"}]}], ";"}]}], "Input",
 CellLabel->"In[18]:=",
 CellID->988050642,ExpressionUUID->"6d9c0a19-e2dd-4f60-87c1-3d11ecc94819"],

