     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{
       SubscriptBox["B", "\[UpTee]"], "[", 
       RowBox[{
        RowBox[{
         RowBox[{"cos", "(", 
          RowBox[{"\[Omega]", "\[VeryThinSpace]", "t"}], ")"}], 
         SuperscriptBox["S", "x"]}], "+", 
        RowBox[{
         RowBox[{"sin", "(", 
          RowBox[{"\[Omega]", "\[VeryThinSpace]", "t"}], ")"}], 
         SuperscriptBox["S", "y"]}]}], "]"}]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SubscriptBox["B", "0"], 
      SuperscriptBox["S", "z"]}]}]}], TraditionalForm]],ExpressionUUID->
  "6ce66d87-0b52-4203-b20b-b1dc9fdaabac"],
