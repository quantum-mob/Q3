Cell[BoxData[
 RowBox[{
  RowBox[{"prob", "[", "t_", "]"}], "=", 
  RowBox[{
   RowBox[{"Abs", "[", 
    RowBox[{"ExpToTrig", "@", 
     RowBox[{"Normal", "@", 
      RowBox[{"Matrix", "@", 
       RowBox[{"vec", "[", "t", "]"}]}]}]}], "]"}], "^", "2"}]}]], "Input",
 CellLabel->"In[19]:=",
 CellID->540399814,ExpressionUUID->"3a477ce2-8a1a-46b5-8a67-4f5c97b256a1"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   SuperscriptBox[
    InterpretationBox[
     RowBox[{"\[LeftBracketingBar]", 
      RowBox[{
       RowBox[{"Cos", "[", 
        FractionBox["t", "2"], "]"}], "-", 
       FractionBox[
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"Sin", "[", 
          FractionBox["t", "2"], "]"}]}], 
        SqrtBox["2"]]}], "\[RightBracketingBar]"}],
     Abs[Cos[
       Rational[1, 2] $CellContext`t] + (Complex[0, -1] 2^Rational[-1, 2]) 
       Sin[Rational[1, 2] $CellContext`t]]], "2"], ",", 
   RowBox[{
    FractionBox["1", "2"], " ", 
    SuperscriptBox[
     InterpretationBox[
      RowBox[{"\[LeftBracketingBar]", 
       RowBox[{"Sin", "[", 
        FractionBox["t", "2"], "]"}], "\[RightBracketingBar]"}],
      Abs[
       Sin[Rational[1, 2] $CellContext`t]]], "2"]}]}], "}"}]], "Output",
 CellLabel->"Out[19]=",
