Cell["\<\
We take an ancillary register consisting of four qubits, which we call the \
\[OpenCurlyDoubleQuote]probe\[CloseCurlyDoubleQuote] register.\
\>", "TechNoteText",
 CellID->868733443,ExpressionUUID->"b17fda10-f9b4-43d6-b3f8-66b435e97ecd"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"$m", "=", "4"}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "the", " ", "number", " ", "of", " ", "qubits", " ", "in", " ", "the", 
    " ", "\"\<probe\>\"", " ", "register"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"kk", "=", 
    RowBox[{"Range", "[", "$m", "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"the", " ", "\"\<probe\>\"", " ", "register"}], " ", 
   "*)"}]}]}], "Input",
