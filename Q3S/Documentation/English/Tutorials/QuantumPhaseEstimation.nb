Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"m", "/", "2"}]]], 
     RowBox[{
      SubscriptBox["\[Sum]", "x"], 
      RowBox[{
       TemplateBox[{"x"},
        "Ket"], "\[CircleTimes]", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["U", "x"], 
         TemplateBox[{"\[Phi]"},
          "Ket"]}], ")"}]}]}]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"m", "/", "2"}]]], 
      RowBox[{
       SubscriptBox["\[Sum]", "x"], 
       RowBox[{
        TemplateBox[{"x"},
         "Ket"], "\[CircleTimes]", 
        RowBox[{"(", 
         RowBox[{
          TemplateBox[{"\[Phi]"},
           "Ket"], 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]2\[Pi]"}], " ", "\[Phi]"}]]}], 
         ")"}]}]}]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox["1", 
         SuperscriptBox["2", 
          RowBox[{"m", "/", "2"}]]], 
        RowBox[{
         SubsuperscriptBox["\[Sum]", "x", 
          RowBox[{
           SuperscriptBox["2", "m"], "-", "1"}]], 
         RowBox[{
          TemplateBox[{"x"},
           "Ket"], 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "\[ImaginaryI]2\[Pi]"}], " ", "\[Phi]"}]]}]}]}], 
       ")"}], "\[CircleTimes]", 
      TemplateBox[{"\[Phi]"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "7cb74028-808e-4976-854d-9b71599f5f0f"],
 " ."
}], "TechNoteText",
 CellID->945268725,ExpressionUUID->"56ed9747-7af9-4b73-8575-7c0b33ff8d4a"],

