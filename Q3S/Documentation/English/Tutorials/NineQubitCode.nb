Cell["\<\
Let us define functions to encode the bit-flip and phase-flip codes.\
\>", "TechNoteMathCaption",
 CellID->103039677,ExpressionUUID->"86832b48-96b1-43a0-b2d0-56ec217f6991"],

Cell[BoxData[
 RowBox[{
  RowBox[{"BitFlipEncoding", "[", 
   RowBox[{"ss", ":", 
    RowBox[{"{", 
     RowBox[{"__", "?", "QubitQ"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Apply", "[", 
   RowBox[{"QuantumCircuit", ",", 
    RowBox[{"CNOT", "@@@", 
     RowBox[{"Thread", "[", 
      RowBox[{"List", "[", 
       RowBox[{
        RowBox[{"First", "@", "ss"}], ",", 
        RowBox[{"Rest", "@", "ss"}]}], "]"}], "]"}]}]}], "]"}]}]], "Input",
 CellLabel->"In[270]:=",
 CellID->771514597,ExpressionUUID->"26e86233-0397-4725-8804-4a040a44c99d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PhaseFlipEncoding", "[", 
   RowBox[{"ss", ":", 
    RowBox[{"{", 
     RowBox[{"__", "?", "QubitQ"}], "}"}]}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"QuantumCircuit", "[", 
   RowBox[{
