Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalL]", "(", "\[Rho]", ")"}], ":=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"\[ImaginaryI]", "[", 
       RowBox[{"H", ",", "\[Rho]"}], "]"}]}], "-", 
     RowBox[{"{", 
      RowBox[{"G", ",", "\[Rho]"}], "}"}], "+", 
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "\[Mu]"], 
