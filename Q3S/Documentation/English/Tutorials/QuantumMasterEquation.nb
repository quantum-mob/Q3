  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "'"}], ")"}], "=", 
    RowBox[{
     UnderscriptBox["\[CapitalSigma]", "\[Mu]"], 
     RowBox[{
      SubscriptBox["F", "\[Mu]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
     RowBox[{"\[Rho]", "(", "t", ")"}], " ", 
     RowBox[{
      SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"73e98775-988b-4b6b-9723-3b705fb0c9c7"],
 " ,"
}], "TechNoteText",
