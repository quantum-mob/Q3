Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["d\[Rho]", 
     RowBox[{"d", "\[VeryThinSpace]", "t"}]], "=", 
    RowBox[{"\[ScriptCapitalL]", "(", "\[Rho]", ")"}]}], TraditionalForm]],
  ExpressionUUID->"91732f6d-3693-4062-a315-52881c15c29a"],
 ","
}], "TechNoteText",
 CellFrameLabels->{{None, 
