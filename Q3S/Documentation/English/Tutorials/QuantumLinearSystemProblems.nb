Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Ket"], ":=", 
    RowBox[{
     SuperscriptBox["A", 
      RowBox[{"-", "1"}]], 
     RowBox[{
      TemplateBox[{"b"},
       "Ket"], " ", "/", " ", 
      SubscriptBox[
       RowBox[{"\[LeftDoubleBracketingBar]", 
        RowBox[{
         SuperscriptBox["A", 
          RowBox[{"-", "1"}]], 
         TemplateBox[{"b"},
          "Ket"]}], "\[RightDoubleBracketingBar]"}], "2"]}]}]}], 
   TraditionalForm]],ExpressionUUID->"cdbcc863-4299-4b69-ade4-bf29c1831c4c"]
}], "TechNoteText",
 CellID->1913001532,ExpressionUUID->"0c9694e3-d50e-4034-a453-3a76d923988f"],

