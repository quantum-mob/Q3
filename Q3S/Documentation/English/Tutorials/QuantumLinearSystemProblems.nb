Cell[TextData[{
 "Given the access to an invertible matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "b00becc0-dc61-438a-93cf-046b2c078cc6"],
 " and to a normalized quantum state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"b"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "8384a349-28be-4da8-88ad-61a0fb1c38c5"],
