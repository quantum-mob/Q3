Cell[TextData[{
 "Expanding a real polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "target"], "(", "x", ")"}], TraditionalForm]],
  ExpressionUUID->"a8cb8deb-c3a4-42c4-a58d-0620d1de7e4e"],
