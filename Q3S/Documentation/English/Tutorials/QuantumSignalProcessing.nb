Cell[TextData[{
 "The quantum signal processing (QSP) of degree ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "ad6f22ec-0e6b-4996-8180-e16f3d44b067"],
 " with phases ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "0"], ",", 
      SubscriptBox["\[Phi]", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "d"]}], "}"}], "=", 
    RowBox[{":", 
     OverscriptBox["\[Phi]", "\[RightVector]"]}]}], TraditionalForm]],
  ExpressionUUID->"6ff50fa2-e46f-48e7-b12b-5faac4518015"],
 " is a parameterized representation of a polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "0d07045f-f847-45c7-9341-67a4da0503da"],
 " of degree ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "fdbf80cf-ab3a-4ec4-89da-c7dd491b316e"],
 " and parity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "-", 
    RowBox[{"2", 
     RowBox[{"(", 
      RowBox[{"d", " ", "mod", " ", "2"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"681d366b-020b-4373-bb45-ee400a90d2fa"],
 ", through the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Times]", "2"}], TraditionalForm]],ExpressionUUID->
  "10b4ccb4-f2c5-4055-984d-5cbc58eb9ce1"],
 " unitary matrix"
}], "TechNoteText",
 CellID->1149534814,ExpressionUUID->"3de6a6c8-caa0-435c-9238-4679a5bb0fe8"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"]], "(", "x", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
        SubscriptBox["\[Phi]", "0"], "Z"}]], 
      RowBox[{
       UnderoverscriptBox["\[CapitalPi]", 
        RowBox[{"k", "=", "1"}], "d"], "[", 
       RowBox[{
        RowBox[{"W", "(", "x", ")"}], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          SubscriptBox["\[Phi]", "k"], "Z"}]]}], "]"}]}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"P", "(", "x", ")"}], 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"Q", "(", "x", ")"}], 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["x", "2"]}]]}]},
        {
         RowBox[{"\[VeryThinSpace]", 
          RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
           RowBox[{
            SuperscriptBox["Q", "*"], "(", "x", ")"}], 
           SqrtBox[
            RowBox[{"1", "-", 
             SuperscriptBox["x", "2"]}]]}]}], 
         RowBox[{
          SuperscriptBox["P", "*"], "(", "x", ")"}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "7d9fe0a0-3001-44af-8a36-6cd68dd75bfc"],
 " ,"
}], "TechNoteText",
 CellID->1855416719,ExpressionUUID->"868bff09-7251-476c-bb50-f84f7decdd01"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "f7240dc1-86e3-4faa-998d-d52171275fa6"],
 " is the Pauli-",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "9645f58c-310e-4bc4-844b-1e4f2c0a1e17"],
 " matrix,"
}], "TechNoteText",
