Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"W", "(", "x", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {"x", 
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"]}]]}]},
       {
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
