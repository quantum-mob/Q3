Cell["Complete Decoherence", "TechNoteSection",
 CellID->1763954588,ExpressionUUID->"da8d0f98-eb75-4327-9030-43b7337116ff"],

Cell[BoxData[{
 RowBox[{"<<", "Q3`"}], "\[IndentingNewLine]", 
