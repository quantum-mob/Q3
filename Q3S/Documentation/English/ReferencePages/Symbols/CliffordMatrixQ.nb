Cell[TextData[{
 "A Clifford operator ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "e935aef1-c3c7-4a2d-9153-62b4691be21a"],
 " is completely specified (up to a global phase factor ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", 
     RowBox[{"\[Pi]", " ", "/", "4"}]}]], TraditionalForm]],ExpressionUUID->
  "9e34d417-fa6c-4de8-9459-56b56fb9867f"],
 ") by the transformation of Pauli operators under conjugation by ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "98d000aa-9606-4e73-945a-ad2de187086b"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["X", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["p", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["a", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["b", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "4205affe-8d4c-42a5-bcf1-cb6eba5109f6"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SubscriptBox["Z", "j"], " ", 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      SubscriptBox["q", "j"]], 
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "n"}]], 
     RowBox[{
      SubsuperscriptBox["X", "i", 
       SubscriptBox["c", 
        StyleBox["ij",
         FontSlant->"Italic"]]], 
      SubsuperscriptBox["Z", "i", 
       SubscriptBox["d", 
        StyleBox["ij",
         FontSlant->"Italic"]]]}]}]}], TraditionalForm]],ExpressionUUID->
  "0aaecaa7-580f-4ad7-a4d0-f8d5af6c53b1"],
 "."
}], "Notes",
 CellID->1417856937,ExpressionUUID->"f9a0fd19-cfd7-4d3c-81c5-9802701a05a7"],

Cell[TextData[{
 "The Gottesman matrix of ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "4a0810ee-7da9-4e2f-abdb-bb945b5e3452"],
 " is defined by\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"S", "(", "U", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", "11"], 
        SubscriptBox["c", "11"], 
        SubscriptBox["a", "12"], 
        SubscriptBox["c", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "11"], 
        SubscriptBox["d", "11"], 
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "12"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["a", "21"], 
        SubscriptBox["c", "21"], 
        SubscriptBox["a", "22"], 
        SubscriptBox["c", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {
        SubscriptBox["b", "12"], 
        SubscriptBox["d", "21"], 
        SubscriptBox["b", "22"], 
        SubscriptBox["d", "22"], "\[Ellipsis]", "\[Ellipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
        "\[DescendingEllipsis]"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "d0dfbf64-277f-49e3-91d8-8717c0572026"]
}], "Notes",
 CellID->2083430531,ExpressionUUID->"d7b78722-cd53-48f6-95b1-90bf455241cb"],

Cell[TextData[{
 "Recall that a Pauli operator\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[CapitalPi]", 
       RowBox[{" ", 
        RowBox[{"j", "=", "1"}]}], 
       RowBox[{" ", "n"}]], 
      RowBox[{
       SubsuperscriptBox["X", "j", 
        SubscriptBox["x", "j"]], 
       SubsuperscriptBox["Z", "j", 
        SubscriptBox["z", "j"]]}]}], "\[Element]", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"c66d8c39-c6ce-47ab-8c97-4aff3702fc0d"],
 "\[LineSeparator]is completely specified by the corresponding Gottesman \
vector,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "(", "P", ")"}], ":=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], ",", 
       SubscriptBox["z", "1"], ",", 
       SubscriptBox["x", "2"], ",", 
       SubscriptBox["z", "2"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["x", "n"], ",", 
       SubscriptBox["z", "n"]}], ")"}], "T"]}], TraditionalForm]],
  ExpressionUUID->"26307f87-4dfc-44d4-80d1-56b8c1bc6839"],
 "."
}], "Notes",
 CellID->1441760612,ExpressionUUID->"a97e9e0e-8cc9-4978-8e96-ddd760c75774"],

