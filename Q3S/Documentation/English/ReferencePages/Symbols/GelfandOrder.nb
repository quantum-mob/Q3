  FormBox[
   RowBox[{
    RowBox[{"shape", "(", "S", ")"}], "<", 
    RowBox[{"shape", "(", "T", ")"}]}], TraditionalForm]],ExpressionUUID->
  "06676be6-835f-468e-ab2f-84168450872e"],
 "; (ii) ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "31b64c15-a72d-4737-929d-955f1b939723"],
 " and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "5c501be6-dcd2-4115-a153-1dce2dca2cdf"],
