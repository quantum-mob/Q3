Cell[TextData[{
 StyleBox["Lexicographic ordering of Young tableaux",
  FontSlant->"Italic"],
 ": Let ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "633db327-c4f3-467b-8a2b-ef1da61342df"],
 " and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "1e9ab633-8a8b-4ad8-912b-27216cefbca3"],
