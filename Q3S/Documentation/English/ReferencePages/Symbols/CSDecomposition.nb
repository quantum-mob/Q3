       {
        SubscriptBox["U", "21"], 
        SubscriptBox["U", "22"]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["V", "1"], "0"},
        {"0", 
         SubscriptBox["V", "2"]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {"C", "S", "0"},
        {
         RowBox[{"-", "S"}], "C", "0"},
        {"0", "0", "I"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubsuperscriptBox["W", "1", "\[Dagger]"], "0"},
        {"0", 
         SubsuperscriptBox["W", "2", "\[Dagger]"]}
