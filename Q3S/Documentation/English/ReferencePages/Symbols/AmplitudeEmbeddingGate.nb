 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["x", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["x", 
       SuperscriptBox["2", "n"]]}], "}"}], "\[RightTeeArrow]", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"k", "=", "1"}], 
      SuperscriptBox["2", "n"]], 
