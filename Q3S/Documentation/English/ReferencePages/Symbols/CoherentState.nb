  "981f8110-85bc-47d0-9241-c317bbb0dbb7"],
 ", ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["CoherentState"]], "paclet:Graphy/ref/CoherentState", 
     "Graphy Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   RowBox[{"<|", 
    RowBox[{"\[Ellipsis]", ",", " ", 
     RowBox[{
      StyleBox["a", "TI"], "\[Rule]", 
      StyleBox["z", "TI"]}], ",", " ", "\[Ellipsis]"}], "|>"}], "]"}]], 
  "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "b035defa-86a1-4562-861d-4db50053a76d"],
 " is expanded as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     RowBox[{"exp", "[", 
      RowBox[{"z", "\[VeryThinSpace]", 
       SuperscriptBox["a", "\[Dagger]"]}], "]"}], 
     TemplateBox[{"0"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], 
         "2"]}], "/", "2"}], ")"}], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     FractionBox[
      SuperscriptBox["z", "n"], 
      SqrtBox[
       RowBox[{"n", "!"}]]], 
     TemplateBox[{"n"},
      "Ket"]}]}], TraditionalForm]],
  InitializationCell->True,ExpressionUUID->
  "5ac03447-9aeb-4141-916a-3ce9b95d50db"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox[
       RowBox[{"\[LeftBracketingBar]", "z", "\[RightBracketingBar]"}], "2"]}],
      "/", "2"}], ")"}], TraditionalForm]],ExpressionUUID->
  "7a118e48-65e8-4fca-add8-c7d5f8d42497"],
