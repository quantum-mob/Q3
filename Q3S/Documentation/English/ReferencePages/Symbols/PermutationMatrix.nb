Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "52a3c32a-05df-4535-a4dd-d9f053c68a84"],
 " be a group. Suppose that ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "19707ced-9bf7-434a-8bea-50aaefd51dab"],
 " acts on a set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalS]", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["s", "1"], ",", 
      RowBox[{
       SubscriptBox["s", "2"], "\[Ellipsis]"}], ",", 
      SubscriptBox["s", "n"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "baa8d273-3725-4f89-847d-c53efe20fc29"],
 ". The permutation representation of ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalG]", TraditionalForm]],ExpressionUUID->
  "260f09c0-6669-404f-a4a1-a3e833c390fa"],
 " associated with ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalS]", TraditionalForm]],ExpressionUUID->
  "5c4a9b36-e73f-437c-8e0e-44fe5932fd82"],
 " is spanned by the standard basis ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["s", "1"]},
       "Ket"], ",", 
      TemplateBox[{
        SubscriptBox["s", "2"]},
       "Ket"], ",", "\[Ellipsis]", ",", 
      TemplateBox[{
        SubscriptBox["s", "n"]},
       "Ket"]}], "}"}], ","}], TraditionalForm]],ExpressionUUID->
  "9037d0f0-9092-4ce9-9fc3-4d515948db3e"],
 " and the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "\[Element]", "\[ScriptCapitalG]"}], TraditionalForm]],
  ExpressionUUID->"a260dc84-7487-4025-ab3b-888d63c02948"],
 " is defined by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["G", "^"], 
     TemplateBox[{
       SubscriptBox["s", "j"]},
      "Ket"]}], ":=", 
    RowBox[{
     TemplateBox[{
       RowBox[{"G", "\[VeryThinSpace]", 
         SubscriptBox["s", "j"]}]},
      "Ket"], "=", 
     RowBox[{
      UnderscriptBox["\[CapitalSigma]", "i"], 
      TemplateBox[{
        SubscriptBox["s", "i"]},
       "Ket"], 
      RowBox[{
       SubscriptBox["\[CapitalGamma]", 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], "(", "g", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"6f1d027a-2441-4e01-83ff-00ee9842d47a"],
 "\[LineSeparator]for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["s", "j"], "\[Element]", "\[ScriptCapitalS]"}], 
   TraditionalForm]],ExpressionUUID->"60e5a83c-d741-4889-a453-9054d7a9d36d"],
 "."
}], "Notes",
 CellID->869301310,ExpressionUUID->"777e6ce7-3aa1-41b9-8060-c20196970aec"],

Cell[TextData[{
