     RowBox[{
      RowBox[{
       RowBox[{"S", "(", "A", ")"}], "-", 
       RowBox[{"S", "(", 
        RowBox[{"A", "|", "B"}], ")"}]}], "=", 
      RowBox[{
       RowBox[{"S", "(", "B", ")"}], "-", 
       RowBox[{"S", "(", 
        RowBox[{"B", "|", "A"}], ")"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"e9a4a2a7-108a-4427-85bf-dfb1bd5fb529"],
 " ,\nwhere ",
